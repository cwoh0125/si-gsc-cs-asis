		if(ds_rsv_hist_out.rowcount < 1){
			cf_Alert("BNSE0036","구매이력이");			
			return;
	}
	
		
	var sPerTCd ; 
	var sPerTDt = "ICCS_CSC 구매 이력 다운로드";  // 수행업무 상세내용
	var sCalFun = "Btn_Excel_onclick()";  // 호출함수명 	
	var sInqCon = gfn_getStringFromDataset(ds_srch_cond); //조회 조건은 고객번호 
