function Btn_Excel_onclick(obj:Button,  e:ClickEventInfo)
{
	
	var objDsCust = this.parent.parent.all["ds_cust_info"];
	var objFlgDs	= this.parent.parent.all["ds_srch_flg"];	
	


		
		
	if(getTrim(objDsCust.getColumn(0,"IO_CUST_NO")) == ""){
