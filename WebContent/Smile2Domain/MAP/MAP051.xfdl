/************************************************************************************************************************
******************************************* 콜백
************************************************************************************************************************/
function fn_callBack(svcid){

	
	if(svcid=="M1"){
	
		if(ds_result.rowcount=="0"){
			alert("월평가 조회 결과가 없습니다.");
			return;
		}else{
			for(i=0;i<ds_result.rowcount;i++){				
					ds_result.setColumn(i,"DDATA", ds_result.getColumn(i,"DDATA").substr(0,2)*60+ds_result.getColumn(i,"DDATA").substr(3,2)*1);
					ds_result.setColumn(i,"GDATA", ds_result.getColumn(i,"GDATA").substr(0,2)*60+ds_result.getColumn(i,"GDATA").substr(3,2)*1);				
			}
		
		}
			srch_moth_base();
	}
	
	if(svcid=="MB"){
	
	Grid_MAP060.setCellProperty('head','15','text',ds_result_MB.getColumn(0,"MBASE")+"일");	
    
	setResult();	
	setULP();
	
	}
	
	if(svcid=="R3"){	
		if(ds_result_F.rowcount <1) { 
			alert("월평가 조회 결과가 없습니다."); return;
		}
		srch_moth_base();
	}
	if(svcid=="CF"){
	Btn_sel_onclick();
	alert("월평가 관리자에 의해 수정 되었습니다.");
	
	}
	
	if(svcid=="D1"){
	Btn_sel_onclick();
	alert("평가삭제 되었습니다.");
	
	}
}



