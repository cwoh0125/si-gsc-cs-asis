function fn_callBack_sta(svcid){
	
	if(svcid=="SRCH_STA"){
	Stt_Cnt_sta.text=Cal_start.text;
	}
	else if(svcid=="STA"){
		
			if(ds_sta_srch.getColumn(0,"CDATA")=="0"){	
				if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?
								
					ds_sta_saves.clearData();				
					
						for (i=0;i<ds_result_F.rowcount;i++){
						
						ds_sta_saves.addRow();
						ds_sta_saves.setColumn(i,"SEQ_NO",ds_result_F.getColumn(i,"USR_ID")+ds_result_F.getColumn(i,"TIME_KEY"));
						ds_sta_saves.setColumn(i,"TIME_KEY",ds_result_F.getColumn(i,"TIME_KEY"));
						ds_sta_saves.setColumn(i,"USR_ID",ds_result_F.getColumn(i,"USR_ID"));
						ds_sta_saves.setColumn(i,"USR_NM",ds_result_F.getColumn(i,"USR_NM"));
						ds_sta_saves.setColumn(i,"GRP_CD",Cmb_GRP_CD.value);
						ds_sta_saves.setColumn(i,"PART_CD",Cmb_PART_CD.value);
						ds_sta_saves.setColumn(i,"MAP_DATA",ds_result_F.getColumn(i,"FINAL_SUM"));
						ds_sta_saves.setColumn(i,"MAP_GRADE",ds_result_F.getColumn(i,"AGRADE"));
					
				//	alert(i+" : "+ds_result_F.getColumn(i,"USR_ID")+" : "+ds_result_F.getColumn(i,"USR_NM"));
						//alert(i+" : " +ds_sta_save.getColumn(i,"SEQ_NO"));
					
						}	
								
							
								
					var	strSvcID = "MAP_INSERT";
					var	strURL = SVC_USR_URL;
					var	strInDatasets = "INSERT_0_IDATA=ds_sta_saves:a "; 
					var	strOutDatasets = "";
					var	strArgument = "INSERT_0_ID=MAP050.Set_MAP050_STA_INSERT";
					var strCallbackFunc = "fn_callBack_sta";
					var strProcessBar = true;

					cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
				}else{
				
				ds_sta_saves.clearData();		
				
					for (i=0;i<ds_result_F.rowcount;i++){
						
						ds_sta_saves.addRow();
						ds_sta_saves.setColumn(i,"SEQ_NO",ds_result_F.getColumn(i,"USR_ID")+ds_result_F.getColumn(i,"TIME_KEY"));
						ds_sta_saves.setColumn(i,"TIME_KEY",ds_result_F.getColumn(i,"TIME_KEY"));
						ds_sta_saves.setColumn(i,"USR_ID",ds_result_F.getColumn(i,"USR_ID"));						
						ds_sta_saves.setColumn(i,"MAP_DATA",ds_result_F.getColumn(i,"FINAL_SUM"));
						ds_sta_saves.setColumn(i,"MAP_GRADE",ds_result_F.getColumn(i,"AGRADE"));
					
				//	alert(i+" : "+ds_result_F.getColumn(i,"USR_ID")+" : "+ds_result_F.getColumn(i,"USR_NM"));
						//alert(i+" : " +ds_sta_save.getColumn(i,"SEQ_NO"));
					
						}	
				
					var	strSvcID = "MAP_UPDATE";
					var	strURL = SVC_USR_URL;
					var	strInDatasets = "UPDATE_0_IDATA=ds_sta_saves:a "; 
					var	strOutDatasets = "";
					var	strArgument = "UPDATE_0_ID=MAP050.Set_MAP050_STA_UPDATE";
					var strCallbackFunc = "fn_callBack_sta";
					var strProcessBar = true;

					cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
				}
	}
	

}
