// 셋팅 점수 기초

function setResult(){

var sumdata;
	
	 for(i=0;i<ds_result_F.rowcount;i++){
	 
	 //평가 수정 201702 TM_CNT추가 생산성 계산에만 포함 하면 된다.
     //sumdata=parseInt(ds_result_F.getColumn(i,"INBND_HOUSE"))+parseInt(ds_result_F.getColumn(i,"EMAIL"));
	 //sumdata=parseInt(ds_result_F.getColumn(i,"INBND_HOUSE"))+parseInt(ds_result_F.getColumn(i,"EMAIL"))+parseInt(ds_result_F.getColumn(i,"TM_CNT"));    
		
		sumdata=parseInt(ds_result_F.getColumn(i,"INBND_HOUSE"))+parseInt(ds_result_F.getColumn(i,"EMAIL"))+parseInt(ds_result_F.getColumn(i,"TM_CNT"))+parseInt(ds_result_F.getColumn(i,"CALLBACK"));
		//합계에 콜백건수 추가 (콜백처리후, 콜백배정화면에서 처리완료 + 통화결과가 통화성공  + 콜백테이블에 상담번호가 있어야 => 평가에 반영된다. 2017.06.22
		
		ds_result_F.setColumn(i,"BASE_SUM", Math.round(sumdata / ds_result_MB.getColumn(0,"MBASE"),1));
		ds_result_F.setColumn(i,"BASE_RESULT",Math.round(ds_result_F.getColumn(i,"BASE_SUM") * 0.3,1));
		
		if(ds_result_F.getColumn(i,"BASE_RESULT")>30){
			ds_result_F.setColumn(i,"BASE_RESULT",'30');
		}
		ds_result_F.setColumn(i,"QA_JUMSUT",Math.round(ds_result_F.getColumn(i,"QA_TOT")*0.3,1));
	 	 	 	 
			ds_result_F.setColumn(i,"DDDATA", ds_result_F.getColumn(i,"DDDATA").substr(0,2)*60+ds_result_F.getColumn(i,"DDDATA").substr(3,2)*1);
			ds_result_F.setColumn(i,"GGDATA", ds_result_F.getColumn(i,"GGDATA").substr(0,2)*60+ds_result_F.getColumn(i,"GGDATA").substr(3,2)*1);				
		
			//기준시간 변경 2분 -> 1분 20180111 허선화P 요청
			//ds_result_F.setColumn(i,"DDDATA_B", ds_result_F.getColumn(i,"INBND_HOUSE")*2);
			ds_result_F.setColumn(i,"DDDATA_B", ds_result_F.getColumn(i,"INBND_HOUSE")*1);
			ds_result_F.setColumn(i,"GGDATA_B", ds_result_F.getColumn(i,"DDAY")*40);
			
			ds_result_F.setColumn(i,"ERROR_DATA", Math.round((100-ds_result_F.getColumn(i,"ERROR_DATA"))*0.1,1));
			//ds_result_F.setColumn(i,"STAND_DATA", Math.round((50-ds_result_F.getColumn(i,"STAND_DATA"))*0.1,1)); //2015.02.09 허선화파트장이 상향은 +10 하향은 -10
			
			
			////평가 계산 분기 허선화P 2017-03-09		
			if(ds_srch3.getColumn(0,"TIME_KEY") < "201703"){			
				ds_result_F.setColumn(i,"STAND_DATA", Math.round((50+parseInt(ds_result_F.getColumn(i,"STAND_DATA")))*0.1,1)); //2015.02.09 허선화파트장이 상향은 +10 하향은 -10
				
			}
			else{   
				ds_result_F.setColumn(i,"STAND_DATA", Math.round((100+parseInt(ds_result_F.getColumn(i,"STAND_DATA")))*0.1,1)); //2017.03.01 허선화P 기본 점수를 10점으로.
				
			}		
			
					
			ds_result_F.setColumn(i,"BEGIN_SUM",Math.round(parseFloat(ds_result_F.getColumn(i,"BASE_RESULT"))+parseFloat(ds_result_F.getColumn(i,"QA_JUMSUT"))+parseFloat(ds_result_F.getColumn(i,"ERROR_DATA"))+parseFloat(ds_result_F.getColumn(i,"STAND_DATA"))+parseFloat(ds_result_F.getColumn(i,"QUIZ"))+parseFloat(ds_result_F.getColumn(i,"ULP")),1) );
			
			ds_result_F.setColumn(i,"FINAL_SUM", Math.round(ds_result_F.getColumn(i,"BEGIN_SUM")-ds_result_F.getColumn(i,"AMINUS"),1));
				 
	 }
}

// 셋팅 근태

function setULP(){

