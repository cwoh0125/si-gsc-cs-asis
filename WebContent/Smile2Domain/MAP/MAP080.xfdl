function fn_callBack_srch(){
	if(ds_result.rowcount > 0){
		Stt_Cnt.text = ds_working.rowcount;
	}else if(ds_result.rowcount=="0"){
		cf_Alert("COMI0006");//조회된건이 없습니다.
		ds_result.clearData();
		
	}
	
