function Btn_sel_onclick(obj:Button,  e:ClickEventInfo)
{

	ds_srch_out.clearData();
	ds_result.clearData();
	
	var in_data = Cal_start.text;
			
	fun_ds_srch(ds_srch_out, "TIME_KEY", in_data.replace("-","")+"%");
