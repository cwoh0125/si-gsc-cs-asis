    Edt_TITE.value = Cmb_PRDCT_CD.text;

    ds_prdct_dtls_cd.filter("HIG_CODE =='" + prdct_cd + "'");
    Cmb_PRDCT_DTLS_CD.index = 0;
    Cmb_PRDCT_DTLS_CD_onitemchanged();
}

