/*============================================================================*/
function Btn_R1_onclick(obj:Button,  e:ClickEventInfo) {    

    //필수체크
    if(!cf_ValidDate(Cal_start.value, Cal_end.value)) {
        Cal_start.setFocus();
        return false;
    }


    ds_blbd.clearData();
    ds_blbd_empty.clearData();
    ds_file.clearData();
    
    Edt_TITE.value = "";
    Txa_CTT.value = "";
    Edt_TITE.enable = false;
    Txa_CTT.enable = false;
    Cmb_PRDCT_CD.enable = false;
    Cmb_PRDCT_DTLS_CD.enable = false;
    
    Btn_save.enable = false;
    Btn_delete.enable = false;
    Btn_write.enable = true;
        
    str_inx = str_inx;
    end_inx = str_inx + cnt ;
    
    fun_ds_srch(ds_srch, "REG_DT_FROM", Cal_start.value);    
    fun_ds_srch(ds_srch, "REG_DT_TO"  , Cal_end.value);
    
    fun_ds_srch(ds_srch, "PRDCT_CD", Cmb_SRCH_PRDCT_CD.value);    
    fun_ds_srch(ds_srch, "PRDCT_DTLS_CD", Cmb_SRCH_PRDCT_DTLS_CD.value);    

    fun_ds_srch(ds_srch, "STR_INX", str_inx);    
    fun_ds_srch(ds_srch, "END_INX", end_inx);
    
    fun_ds_srch(ds_srch, "SEARCH_KW", Cmb_SEARCH_KW.value);
    fun_ds_srch(ds_srch, "SEARCH_FIELD", Edt_SEARCH_FIELD.value);
    
    //거래시작    조회
    var strSvcID = "R1";
    var strURL = SVC_USR_URL;
    var strInDatasets = "SELECT_0_IDATA=ds_srch:a SELECT_1_IDATA=ds_srch:a"; 
    var strOutDatasets = "ds_blbd=SELECT_0_ODATA ds_cnt=SELECT_1_ODATA";
    var strArgument = "SELECT_0_ID=TSS210.GetBlbdList_SELECT SELECT_1_ID=TSS210.GetBlbdListTotCnt_SELECT";    
    var strCallbackFunc = "fn_callBack";
    var strProcessBar = true;
    
    cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);    
}

