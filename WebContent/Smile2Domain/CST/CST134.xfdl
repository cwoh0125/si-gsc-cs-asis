/********************************************************************************** 
 * 조회버튼 클릭 시(CSC에서는 가맹점은 조회조건으로 사용하지 않는다)
***********************************************************************************/
function Btn_Srch_onclick(obj:Button,  e:ClickEventInfo)
{
	if(Cmb_SrchGbn.value == "01") {
		if(getTrim(Edt_SrchCustNo.value) == "") {
			cf_Alert("COME0007", "고객번호");		// 고객번호 을(를) 입력하셔야합니다.
			Edt_SrchCustNo.setFocus();
			return;
		} else if(getTrim(Edt_SrchCustNo.value).length != 10) {
			cf_Alert("COME0028","고객번호","10");	// 고객번호는 10자리가 입력되어야 합니다.		
			Edt_SrchCustNo.setFocus();
			return;			
		}		
	} else if(Cmb_SrchGbn.value == "02") {
		if(getTrim(Msk_SrchCrdNo.value) == "") {
			cf_Alert("COME0007", "카드번호");		// 카드번호 을(를) 입력하셔야합니다.
			Msk_SrchCrdNo.setFocus();
			return;			
		} else if(getTrim(Msk_SrchCrdNo.value).length != 16) {
			cf_Alert("COME0028","카드번호","16");	// 카드번호는 10자리가 입력되어야 합니다.		
			Msk_SrchCrdNo.setFocus();
			return;
		}
	}
	
	if(getTrim(Cal_SrchRegFrom.value) == "") {
		cf_Alert("COME0007", "등록일자");	// 등록일자 을(를) 입력하셔야합니다.
		Cal_SrchRegFrom.setFocus();
		return;
	}
	
	if(getTrim(Cal_SrchRegTo.value) == "") {
		cf_Alert("COME0007", "등록일자");	// 등록일자 을(를) 입력하셔야합니다.
		Cal_SrchRegTo.setFocus();
		return;
	}
	
	if(getTrim(Cal_SrchRegFrom.value) > getTrim(Cal_SrchRegTo.value)){
		cf_Alert("COME0012");		// 입력한 기간을 확인하여 주시기 바랍니다.		
		Cal_SrchRegFrom.setFocus();
		return;
	}
	
	// 최대 1년으로 조회기간 설정
	var strCompDt = AddMonth(Cal_SrchRegFrom.value,12);
	if(strCompDt < Cal_SrchRegTo.value){
		cf_Alert("COME0019","1년");
		Cal_SrchRegFrom.setFocus();
		return;
	}	
	
	// 고객가입 전자서명 조회	
	fn_selDSign_if();
}

/********************************************************************************** 
 * Interface 고객가입 전자서명 조회(dsDsignSelC01)
***********************************************************************************/
function fn_selDSign_if()
{
	var strGbnVal = "";
	
	if(Cmb_SrchGbn.value == "01") {
		strGbnVal = getTrim(Edt_SrchCustNo.value);
	} else if(Cmb_SrchGbn.value == "02") {
		strGbnVal = getTrim(Msk_SrchCrdNo.value);
	}	

	// 건수 초기화
	Stt_tot.text = "";
	
	// ImageView DS 초기화
	Div_Img.ds_img_view.clearData();	

	// IF DS 초기화
	ds_if_sel_dsign_env.clearData();
	ds_if_sel_dsign_common.clearData();
	ds_if_sel_dsign_column.clearData();
	ds_if_sel_dsign_header.clearData();
	ds_if_sel_dsign_data.clearData();
	ds_if_sel_dsign_record.clearData();
	
	// IF ENV DS 세팅
	fn_setIfEnvDs(ds_if_sel_dsign_env,"WAS","dsDsignSelC01.do","","STP_DS_DSIGN_SEL_C01_Rsp","","OT_RESULT");

	// IF DATA DS 세팅		
	fun_ds_srch(ds_if_sel_dsign_data, "req_chnl_code","610050");			
	fun_ds_srch(ds_if_sel_dsign_data, "input_user_id",getTrim(GetUserInfo("gUSR_ID")));
	fun_ds_srch(ds_if_sel_dsign_data, "input_user_nm",getTrim(GetUserInfo("gUSR_NM")));
	fun_ds_srch(ds_if_sel_dsign_data, "input_user_ip",getTrim(GetUserInfo("gLGIN_IP")));

	fun_ds_srch(ds_if_sel_dsign_data, "in_gubn",Cmb_SrchGbn.value);							// 조회구분
	fun_ds_srch(ds_if_sel_dsign_data, "in_cust_info",strGbnVal);							// 고객번호/카드번호
	fun_ds_srch(ds_if_sel_dsign_data, "in_joinchnl_cd",getTrim(Cmb_SrchJoinChnl.value));	// 가입채널
	fun_ds_srch(ds_if_sel_dsign_data, "in_strt_dt",getTrim(Cal_SrchRegFrom.value));			// 조회시작일	
	fun_ds_srch(ds_if_sel_dsign_data, "in_end_dt",getTrim(Cal_SrchRegTo.value));			// 조회종료일
	fun_ds_srch(ds_if_sel_dsign_data, "in_frch_cd","");										// 가맹점코드
	fun_ds_srch(ds_if_sel_dsign_data, "in_inpt_div",getTrim(Cmb_SrchInptDiv.value));		// 등록구분
		
	var	strSvcID = "IF_DSIGN_CSC_001";
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_sel_dsign_env:a INTERFACE_DATA=ds_if_sel_dsign_data:a";
	var	strOutDatasets = "ds_if_sel_dsign_common=DS_COMMON ds_if_sel_dsign_header=DS_HEADER ds_if_sel_dsign_column=DS_COLUMN ds_if_sel_dsign_record=DS_RECORD";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_if";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}

/********************************************************************************** 
 * Interface 고객서명 이미지조회(dsDsignImgSelC01)
***********************************************************************************/
function fn_selDSignImg_if()
{
	var strCrdNo 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"CRD_NO")).replace("-","");
	var strJoinCd 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"JOINCHNL_CD"));
	var strRegDt 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"REG_DT")).replace("-","");

	if(strCrdNo == ""){
		cf_Alert("CAME0005","카드번호");		// 카드번호 가(이) 없습니다.
		return;
	}
	
	if(strJoinCd == ""){
		cf_Alert("CAME0005","가입채널");		// 가입채널 가(이) 없습니다.
		return;
	}
	
	if(strRegDt == ""){
		cf_Alert("CAME0005","등록일자");		// 등록일자 가(이) 없습니다.	
		return;
	}

	// IF DS 초기화
	ds_if_sel_dsignImg_env.clearData();
	ds_if_sel_dsignImg_common.clearData();
	ds_if_sel_dsignImg_column.clearData();
	ds_if_sel_dsignImg_header.clearData();
	ds_if_sel_dsignImg_data.clearData();
	ds_if_sel_dsignImg_record.clearData();
	
	// IF ENV DS 세팅
	fn_setIfEnvDs(ds_if_sel_dsignImg_env,"WAS","dsDsignImgSelC01.do","","STP_DS_DSIGN_IMG_SEL_C01_Rsp","","");

	// IF DATA DS 세팅		
	fun_ds_srch(ds_if_sel_dsignImg_data, "req_chnl_code","610050");
	fun_ds_srch(ds_if_sel_dsignImg_data, "input_user_id",getTrim(GetUserInfo("gUSR_ID")));
	fun_ds_srch(ds_if_sel_dsignImg_data, "input_user_nm",getTrim(GetUserInfo("gUSR_NM")));
	fun_ds_srch(ds_if_sel_dsignImg_data, "input_user_ip",getTrim(GetUserInfo("gLGIN_IP")));
	
	fun_ds_srch(ds_if_sel_dsignImg_data, "in_crd_no",strCrdNo);			// 카드번호
	fun_ds_srch(ds_if_sel_dsignImg_data, "in_joinchnl_cd",strJoinCd);	// 가입채널코드
	fun_ds_srch(ds_if_sel_dsignImg_data, "in_reg_dt",strRegDt);			// 등록일자
		
	var	strSvcID = "IF_DSIGN_CSC_002";
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_sel_dsignImg_env:a INTERFACE_DATA=ds_if_sel_dsignImg_data:a";
	var	strOutDatasets = "ds_if_sel_dsignImg_common=DS_COMMON ds_if_sel_dsignImg_header=DS_HEADER ds_if_sel_dsignImg_column=DS_COLUMN ds_if_sel_dsignImg_record=DS_RECORD";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_if";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}

/********************************************************************************** 
 * 삭제버튼 클릭 시
***********************************************************************************/
function Btn_Del_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_if_sel_dsign_record.rowcount == 0){
		cf_Alert("BNSE0036","전자서명 정보");		// 조회된 전자서명 정보가(이) 없습니다.
		return;
	}
	
	if (!cf_Confirm("COMI0012","삭제"))	return;		// 삭제 처리하시겠습니까?
	
	// 전자서명 삭제
	fn_delDSign_if();	
}

/********************************************************************************** 
 * Interface 고객가입 전자서명 삭제(dsDsignDelC01)
***********************************************************************************/
function fn_delDSign_if()
{
	var strCrdNo 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"CRD_NO")).replace("-","");
	var strJoinCd 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"JOINCHNL_CD"));
	var strRegDt 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"REG_DT")).replace("-","");

	if(strCrdNo == ""){
		cf_Alert("CAME0005","카드번호");		// 카드번호 가(이) 없습니다.
		return;
	}
	
	if(strJoinCd == ""){
