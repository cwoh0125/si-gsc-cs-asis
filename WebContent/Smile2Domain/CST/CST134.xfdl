/********************************************************************************** 
 * IF 콜백처리
***********************************************************************************/
function fn_callBack_if(svcid,strErrCode,strErrMsg)
{
	var strCrdNo	= "";
	var strRegDt	= "";
	var strJoinCd 	= "";

	var strHttpCd 	= "";
	var strHttpMsg 	= "";
	var strRtnCd	= "";
	var strRtnMsg	= "";
		
	// 전자서명 조회
	if(svcid == "IF_DSIGN_CSC_001"){			
		strHttpCd 	= ds_if_sel_dsign_common.getColumn(ds_if_sel_dsign_common.rowposition,"HTTP_RET_CODE");
		strHttpMsg 	= ds_if_sel_dsign_common.getColumn(ds_if_sel_dsign_common.rowposition,"HTTP_RET_MSG");
		
		if(strHttpCd == "0"){					
			strRtnCd	= ds_if_sel_dsign_header.getColumn(ds_if_sel_dsign_header.rowposition,"OT_RESPON_CODE");
			strRtnMsg	= ds_if_sel_dsign_header.getColumn(ds_if_sel_dsign_header.rowposition,"OT_RES_MSG");
			
			if(strRtnCd=="00000"){
				//건수표시
				if(ds_if_sel_dsign_record.rowcount == 0){
					cf_Alert("BNSE0036","결과");	// 조회된 결과가 없습니다.					
				} else{
					Stt_tot.text = ds_if_sel_dsign_record.rowcount;
				}
				
				// 거래이력등록				
				strJoinCd = getTrim(Cmb_SrchJoinChnl.value);
				if(Cmb_SrchGbn.value == "01"){
					fn_insScanTranHist("DSIG001", Cmb_SrchGbn.value, getTrim(Edt_SrchCustNo.value), "", getTrim(Cal_SrchRegFrom.value), getTrim(Cal_SrchRegTo.value), strJoinCd, "");
				} else if(Cmb_SrchGbn.value == "02"){
					fn_insScanTranHist("DSIG001", Cmb_SrchGbn.value, "", getTrim(Msk_SrchCrdNo.value), getTrim(Cal_SrchRegFrom.value), getTrim(Cal_SrchRegTo.value), strJoinCd, "");
				}				
				
			} else{
				cf_Alert("COME0035",strRtnMsg);
				Grd_DSignList.nodatatext = "조회된 결과가 없습니다.";
			}
									
		} else{
			cf_Alert("COME0035",strHttpMsg);
			Grd_DSignList.nodatatext = "조회된 결과가 없습니다.";
		}
	}
			
	// 고객서명 이미지 조회
	if(svcid == "IF_DSIGN_CSC_002"){			
		strHttpCd 	= ds_if_sel_dsignImg_common.getColumn(ds_if_sel_dsignImg_common.rowposition,"HTTP_RET_CODE");
		strHttpMsg 	= ds_if_sel_dsignImg_common.getColumn(ds_if_sel_dsignImg_common.rowposition,"HTTP_RET_MSG");
		
		if(strHttpCd == "0"){					
			strRtnCd	= ds_if_sel_dsignImg_header.getColumn(ds_if_sel_dsignImg_header.rowposition,"OT_RESPON_CODE");
			strRtnMsg	= ds_if_sel_dsignImg_header.getColumn(ds_if_sel_dsignImg_header.rowposition,"OT_RES_MSG");
			
			if(strRtnCd=="00000"){
				// Image SET
				fn_setImgViewer();
				
				// 거래이력등록
				strCrdNo 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"CRD_NO")).replace("-","");
				strJoinCd	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"JOINCHNL_CD"));
				strRegDt 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"REG_DT")).replace("-","");
				fn_insScanTranHist("DSIG002", "", "", strCrdNo, "", "", strJoinCd, strRegDt);
			} else{
				cf_Alert("COME0035",strRtnMsg);
			}									
		} else{
			cf_Alert("COME0035",strHttpMsg);
		}
	}
				
	// 전자서명 삭제
	if(svcid == "IF_DSIGN_CSC_003"){			
		strHttpCd 	= ds_if_del_dsign_common.getColumn(ds_if_del_dsign_common.rowposition,"HTTP_RET_CODE");
		strHttpMsg 	= ds_if_del_dsign_common.getColumn(ds_if_del_dsign_common.rowposition,"HTTP_RET_MSG");
		
		if(strHttpCd == "0"){					
			strRtnCd	= ds_if_del_dsign_header.getColumn(ds_if_del_dsign_header.rowposition,"OT_RESPON_CODE");
			strRtnMsg	= ds_if_del_dsign_header.getColumn(ds_if_del_dsign_header.rowposition,"OT_RES_MSG");			
			if(strRtnCd=="00000"){
				cf_Alert("TSSI0007","삭제");		// 삭제처리되었습니다.
				// 거래이력등록
				strCrdNo 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"CRD_NO")).replace("-","");
				strJoinCd 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"JOINCHNL_CD"));
				strRegDt 	= getTrim(ds_if_sel_dsign_record.getColumn(ds_if_sel_dsign_record.rowposition,"REG_DT")).replace("-","");
				fn_insScanTranHist("DSIG003", "", "", strCrdNo, "", "", strJoinCd, strRegDt);				
				// 삭제 후 재조회
				Btn_Srch_onclick();				
			} else{
				cf_Alert("COME0035",strRtnMsg);
			}									
		} else{
			cf_Alert("COME0035",strHttpMsg);
		}
	}
}


/********************************************************************************** 
 * 1. BASE64 인코딩된 이미지데이터(string)를  BASE64 디코딩한다. 
 * 2. 디코딩된 Buffer type DATA를 Div_Img.ds_img_view에 SET(Image Viewer에 Image SET)
 * 3. Image Viewer는 디코딩된 Buffer type을 Image로 보여줌.
***********************************************************************************/
function fn_setImgViewer()
{
	var strEcdImg = ds_if_sel_dsignImg_header.getColumn(0,"OT_DSIGN_IMG");
	var ExtObj = new ExtCommon();
	var objDcdBuf = new Buffer();
	
	// 이미지뷰어 초기화
	fn_InitImgViewer();
	Div_Img.ds_img_view.addRow();
			
	objDcdBuf = ExtObj.decodeBase64(strEcdImg,"BIN");
	Div_Img.ds_img_view.setColumn(0,"BLOB_IMG",objDcdBuf);
	
	// 이미지크기에 따라 동적으로 뷰어크기를 늘린다.(스크롤처리 되도록)
	var intVieH	= Div_Img.Img_DSignImg.position.height;		// 이미지뷰어 Height
	var intVieW = Div_Img.Img_DSignImg.position.width;		// 이미지뷰어 Width
	var intImgH = Div_Img.Img_DSignImg.imageheight;			// 이미지 Height
	var intImgW = Div_Img.Img_DSignImg.imagewidth;			// 이미지 Width
	
	if(intVieH < intImgH){
		Div_Img.Img_DSignImg.position.height = intImgH;
		Div_Img.resetScroll();
	}
	
	if(intVieW < intImgW){
		Div_Img.Img_DSignImg.position.width = intImgW;
		Div_Img.resetScroll();
	}	
}

/********************************************************************************** 
 * 조회구분콤보 onkeyup 이벤트
***********************************************************************************/
function Cmb_SrchGbn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var strCmbCd = Cmb_SrchGbn.value;
	
	switch(strCmbCd)
	{
		case "01":		// 고객번호로 조회
			Stt_SrchCustNo.visible	= true;
			Edt_SrchCustNo.visible	= true;
			Btn_SrchCustNo.visible	= true;			
			Stt_SrchCrdNo.visible	= false;
			Msk_SrchCrdNo.visible	= false;						
			Edt_SrchCustNo.setFocus();
			break;
		case "02":		// 카드번호호 조회
			Stt_SrchCustNo.visible	= false;
			Edt_SrchCustNo.visible	= false;
			Btn_SrchCustNo.visible	= false;
			Stt_SrchCrdNo.visible	= true;
			Msk_SrchCrdNo.visible	= true;
			Msk_SrchCrdNo.setFocus();
			break;
		default :
			Stt_SrchCustNo.visible	= true;
			Edt_SrchCustNo.visible	= true;
			Btn_SrchCustNo.visible	= true;
			Stt_SrchCrdNo.visible	= false;
			Msk_SrchCrdNo.visible	= false;
			Edt_SrchCustNo.setFocus();
			break;
	}
	
	// 조회조건 초기화
	fn_initSrchCond();	
}

