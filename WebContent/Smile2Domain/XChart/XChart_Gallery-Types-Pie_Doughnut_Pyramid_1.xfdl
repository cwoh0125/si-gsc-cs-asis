    <Script type="xscript4.0"><![CDATA[
function Button00_onclick(obj:Button,  e:ClickEventInfo)
{
	XChart1.Gallery = "Gallery_Pie";
	XChart1.Chart3D = true;
	XChart1.OpenData("COD_Values", 1, 3);
	XChart1.set_Value(0, 0, 20);
	XChart1.set_Value(0, 1, 45);
	XChart1.set_Value(0, 2, 30);
	XChart1.CloseData("COD_Values");
	XChart1.PointLabels = true;
	var pi2 = XChart1.Point.get_Item(-1,2);
	pi2.SeparateSlice  = 35;
}

function chartfx_onload(obj:Form, e:LoadEventInfo)
{
	Button00_onclick();
	Button01_onclick();
	Button02_onclick();
	Button03_onclick();
}

function Button01_onclick(obj:Button,  e:ClickEventInfo)
{
	XChart2.Gallery = "Gallery_Pie";
	XChart2.Chart3D = true;
}

function Button02_onclick(obj:Button,  e:ClickEventInfo)
{
	XChart3.Gallery = "Gallery_Pie";
	XChart3.Chart3D = false;
	XChart3.OpenData("COD_Values", 1, 3);
	XChart3.set_Value(0, 0, 20);
	XChart3.set_Value(0, 1, 45);
	XChart3.set_Value(0, 2, 30);
	XChart3.CloseData("COD_Values");
	XChart3.PointLabels = true;
	
	
	XChart3.GalleryObj.Shadows = true;
}

function Button03_onclick(obj:Button,  e:ClickEventInfo)
{
	XChart4.OpenData("COD_Values", 1, 5);
	XChart4.set_Value(0, 0, 41);
	XChart4.set_Value(0, 1, 44);
	XChart4.set_Value(0, 2, 34);
	XChart4.set_Value(0, 3, 24);
	XChart4.set_Value(0, 4, 17);
	XChart4.CloseData("COD_Values");
	
	XChart4.Gallery = "Gallery_Doughnut";
	XChart4.Chart3D = true;
	XChart4.PointLabels = true;
}
]]></Script>
