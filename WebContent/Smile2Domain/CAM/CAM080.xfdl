{
	//체크박스 초기화
	for(var j = 0; j<ds_list.rowcount; j++) {
		ds_list.setColumn(j, "CHK" , "0");
	}
	
	//선택된 갯수만큼 체크
	if(cmb_num.text > 0) {
		if(ds_list.rowcount > 0) {
			for(var i = 0; i<cmb_num.text; i++){
				ds_list.setColumn(i, "CHK" , "1");
			}
		}
	}
