function setDefaultValue()
{
    var arrCombo           = ["Cmb_ChnlGrp", "cmb_num"];    									
    var arrSearchKey       = ["NW153", "NW154"];               							
    var arrListMode        = ["TY", "TY"];                    					
    var arrOptionRowValue  = [",전체", ",전체"];                  					

	setComboList(arrCombo, arrSearchKey, arrListMode, arrOptionRowValue);     					//공통콤보가져오기
	
	//현재일시 셋팅
    Cal_Sdt.value = AddMonth(getToday(),-1);
    Cal_Edt.value = getToday();	
