function Grd_M_Tgt_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if( e.cell == 0){
		cf_AllChk(obj,0, "CHK");
		return;
	}else{
		cf_gridSort(obj, e);
	}
}



//------------------------------------------------------------------------------------------
// 첨부파일
//------------------------------------------------------------------------------------------
function Grd_M_Tgt_oncellclick(obj:Grid, e:GridClickEventInfo)
{
    if(e.col != 5)    
      return;
	
	
    var strFileUrl = ds_list.getColumn(ds_list.rowposition,"APND_FILE_ADDR");  
	
	//다중 첨부파일 처리 20200811
	var objArr = "";
	objArr = strFileUrl.split("|");
	
	trace(objArr);
	
	trace(objArr.length);    
  
   if(ds_list.getColumn(ds_list.rowposition,"APND_FILE_ADDR") != "" && objArr.length == 1)
   {
     system.execBrowser(ds_list.getColumn(ds_list.rowposition,"APND_FILE_ADDR"));
   }else if(ds_list.getColumn(ds_list.rowposition,"APND_FILE_ADDR") != "" && objArr.length > 1){
		ds_file_list.clearData();
		var strUrl="" ;		
		
		for(i=0;i < objArr.length ; i++){		
			ds_file_list.addRow();	
			ds_file_list.setColumn(i,"APND_FILE_ADDR",objArr[i]);		
		}	
   }
   trace(ds_file_list.saveXML());
}

