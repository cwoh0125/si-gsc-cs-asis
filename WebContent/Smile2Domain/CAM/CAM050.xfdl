}

function Grd_AsgnHist_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col == 1){
		if(e.oldrow != e.row){
			ds_asgnhistinfo.setColumn(e.oldrow,"CHK",0);
			ds_asgnhistinfo.setColumn(e.row,"CHK",1);
			fn_clearGrid();
			fn_initialData();
		}
	}
}

function Cmb_FltList_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
