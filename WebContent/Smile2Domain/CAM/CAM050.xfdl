			}else{			
				strQryWhr += "'" + getTrim(ds_grdcond_list.getColumn(i,"Column5")) + "' ";		// 조건값
			}
			
			if (getTrim(ds_grdcond_list.getColumn(i,"Column4")) == "BETWEEN"){
