function Cmb_TgtAsgnStdd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(Cmb_TgtAsgnStdd.value == "1") {
		Msk_AsgnCnt.enable 		= true;
		Msk_AsgnCnt.value 		= 0;
	} else if(Cmb_TgtAsgnStdd.value == "2") {
