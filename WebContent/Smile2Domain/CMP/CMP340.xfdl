// 부문/ 본부 변경시 처리팀/지사 선택.
function Cmb_SrchDept_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(getTrim(Cmb_SrchDept.value) != "") {
		// 상담사조회
		ds_srch_team.clearData();
		ds_handler_team.clearData();	
		fun_ds_srch(ds_srch_team, "HGRK_DEPT_CD", Cmb_SrchDept.value);
		
		//거래시작	
		var	strSvcID = "R1";
		var	strURL = SVC_USR_URL;
		var	strInDatasets = "SELECT_0_IDATA=ds_srch_team:a"; 
		var	strOutDatasets = "ds_handler_team=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_HndlTeamList_SELECT";
		var strCallbackFunc = "fn_callBack";
		var strProcessBar = false;
		
		cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
	} else if (getTrim(Cmb_SrchDept.value) == "") {
		ds_handler_team.clearData();
		ds_handler_team.insertRow(0);
		ds_handler_team.setColumn(0,"CODE_NM","전체");
		Cmb_SrchHandlerTeam.index = 0;		
	}
}

