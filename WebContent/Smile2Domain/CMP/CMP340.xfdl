// 기간구분 변경시
function Cmb_TermGbn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(getTrim(Cmb_TermGbn.value))
	{
		case "1" :
			cal_day_1.visible 		= true;
			cal_day_2.visible 		= false;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			break;
		
		case "2" :
			cal_day_1.visible 		= true;
			cal_day_2.visible 		= false;
			Cmb_HalfYearly.visible	= true;
			Cmb_Quarter.visible		= false;
					
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();					
			Cmb_HalfYearly.index	= 0;
			break;
		
		case "3" :
			cal_day_1.visible 		= true;
			cal_day_2.visible 		= false;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= true;
			
			cal_day_1.dateformat	= "yyyy";			
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			Cmb_Quarter.index		= 0;
			break;
		
		case "4" :
			cal_day_1.visible 		= true;
			cal_day_2.visible 		= false;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy-MM";
			cal_day_1.editformat	= "yyyy-MM";
			cal_day_1.value			= getToday();			
			break;
		
		case "5" :
			cal_day_1.visible 		= true;
			cal_day_2.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
					
			cal_day_1.dateformat	= "yyyy-MM-dd";
			cal_day_1.editformat	= "yyyy-MM-dd";
			cal_day_1.value			= getToday();	
			cal_day_2.dateformat	= "yyyy-MM-dd";
			cal_day_2.editformat	= "yyyy-MM-dd";
			cal_day_2.value			= getToday();					
			break;
		
		default :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			break;
	}
}

