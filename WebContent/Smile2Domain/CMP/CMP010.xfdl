//처리자 조회 버튼 이벤트 
//------------------------------------------------------------------------------------------
var g_alarmYn = false;
function Btn_UserSearch_onclick(obj:Button,  e:ClickEventInfo)
{
	// 처리결과, 처리일시에 입력항목이 있으면 처리자는 변경될 수 없다.
	if(getTrim(Txa_Result.value) != "" || getTrim(Cal_HandleDate.value) != "" ||
	   getTrim(Msk_HandleTime.value) != ""){
		cf_Alert("CMPE0002");	// 처리결과를 입력한 이후에는 처리자를 변경할 수 없습니다.
		Txa_Result.setFocus();
		return;
	}

	ds_get_param.clearData();
	ds_set_param.clearData();

	fun_ds_srch(ds_get_param, "gubun", "1");

	// 불만발생업체 조회 후, 담당 영업 사원 ID를 넘겨받아 자동 조회(상담사 제외)
	var p_USR_GRD_CD = GetUserInfo("gUSR_GRD_CD");
	if (p_USR_GRD_CD != "E" && p_USR_GRD_CD != "F") // 일반상담사, 선임상담사의 경우, GSC 직원 조회 불가
	{
		fun_ds_srch(ds_get_param, "usrgubun", "2"); //  콤보선택 가능, 양자선택(관리자나 불만 담당자가 주로 사용)
		fun_ds_srch(ds_get_param, "CHRG_BUSS_EMP_ID", ds_cmp_in.getColumn(0, "CHRG_BUSS_EMP_ID")); // 담당 영업 사원 ID
	}
//trace(ds_get_param.saveXML());

	var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};
	PopupDialog("COM140", "COM::COM140.xfdl", -1, -1, 770, 300, false, -1, arrParam, false);
//trace(ds_set_param.saveXML());

	if(ds_set_param.rowcount < 1){
		return;
	}else{
		// 처리자구분 체크
		var p_PART_CD = ds_set_param.getColumn(0,"PART_CD");
		var p_USR_GRD_CD = ds_set_param.getColumn(0,"USR_GRD_CD");

		// PART_CD : BNS1 상담1파트, BNS2 상담2파트, BNS3 상담3파트, BNS4 LPP파트, GSC1 GSC직원, ORD1 주문파트, PLN1 QA파트, PLN2 불만파트, SYS1 관리파트, TEC1 테크서비스
		// USR_GRD_CD : A 관리자, B 센터장, C 그룹장, D 파트장, E 선임 상담사, F 일반 상담사, G TEC관리자, H TEC결재권자

		// 불만파트 -> 불만담당
		if (p_PART_CD == "PLN2"){
			Chk_Handler1.value = "N";
			Chk_Handler2.value = "Y";
			Chk_Handler3.value = "N";
			g_alarmYn = true;
		}else{
			// 관리자, 센터장, 그룹장, 파트장 -> 파트장
			if (p_USR_GRD_CD == "A" || p_USR_GRD_CD == "B" || p_USR_GRD_CD == "C" || p_USR_GRD_CD == "D"){
				Chk_Handler1.value = "N";
				Chk_Handler2.value = "N";
				Chk_Handler3.value = "Y";
				g_alarmYn = true;
			}
			// 상담사 -> 상담사
			else if (p_USR_GRD_CD == "E" || p_USR_GRD_CD == "F"){
				Chk_Handler1.value = "Y";
				Chk_Handler2.value = "N";
				Chk_Handler3.value = "N";
				g_alarmYn = false;
			}
			// 파트장, 상담사 제외 -> 불만담당(알람제외)
			else{
				Chk_Handler1.value = "N";
				Chk_Handler2.value = "Y";
				Chk_Handler3.value = "N";
				g_alarmYn = false;
			}
		}

		if(ds_set_param.getColumn(0,"USR_DV_CD") == "1"){ // 1:상담사(TBL_USR 조회)
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_ID", ds_set_param.getColumn(0,"USR_ID")); // 처리자ID
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_ID_NM", ds_set_param.getColumn(0,"USR_NM")); // 처리자명
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_CHRG_DEPT", "고객센터"); // 처리부서 - 통합CRM센터
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CD", ds_set_param.getColumn(0,"USR_GRD_CD")); // 상담원 등급 코드
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CDNM", ds_set_param.getColumn(0,"USR_GRD_CD_NM")); // 상담원 등급 코드명
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_TEL_NO", ds_set_param.getColumn(0,"EXT_NO")); // 내선번호
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_MBPH_NO", maskPhone(ds_set_param.getColumn(0,"MBPH_NO"))); // 휴대전화
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_EML_ADDR", ds_set_param.getColumn(0,"EML_ADDR")); // 이메일

			// 불만접수처리자이력 테이블 입력데이터 설정(OB이관 후 재지정을 위해 fun_ds_srch로 대체 + OB에 입력되었던 항목 삭제
			fun_ds_srch(ds_dspsr_hist_in, "DSPSR_ID", ds_set_param.getColumn(0,"USR_ID")); // 처리자ID
			fun_ds_srch(ds_dspsr_hist_in, "DSPSR_CHRG_DEPT", "고객센터"); // 처리부서 - 통합CRM센터
		}
		else if(ds_set_param.getColumn(0,"USR_DV_CD") == "2"){ // 2:담당MC(TBL_HR_EMP_CD 조회)
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_ID", ds_set_param.getColumn(0,"EMP_NO")); // 처리자ID
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_ID_NM", ds_set_param.getColumn(0,"EMP_NM")); // 처리자명
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_CHRG_DEPT", ds_set_param.getColumn(0,"BLNG_DEPT_CDNM")); // 처리부서
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CD", ds_set_param.getColumn(0,"POSN_CD")); // 처리자 직급 코드
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CDNM", ds_set_param.getColumn(0,"POSN_NM")); // 처리자 직급 코드명
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_TEL_NO", maskPhone(ds_set_param.getColumn(0,"WKPL_TEL_NO"))); // 처리자 전화 번호
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_MBPH_NO", maskPhone(ds_set_param.getColumn(0,"MBPH_NO"))); // 휴대전화
			ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_EML_ADDR", ds_set_param.getColumn(0,"EML_ADDR")); // 이메일

			// 불만접수처리자이력 테이블 입력데이터 설정(OB이관 후 재지정을 위해 fun_ds_srch로 대체 + OB에 입력되었던 항목 삭제
			fun_ds_srch(ds_dspsr_hist_in, "DSPSR_ID", ds_set_param.getColumn(0,"EMP_NO")); // 처리자ID
			fun_ds_srch(ds_dspsr_hist_in, "DSPSR_CHRG_DEPT", ds_set_param.getColumn(0,"BLNG_DEPT_CDNM")); // 처리부서
		}
//trace(ds_cmp_in.saveXML());

		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_YN_OB_CNTR", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CNTR_DV_CD", "");
//  	Cmb_Center.value = "";

	}
}

