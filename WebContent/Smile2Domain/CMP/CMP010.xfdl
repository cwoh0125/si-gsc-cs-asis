//------------------------------------------------------------------------------------------
//화면 Load에, Login한 상담사 체크하여, 처리자 항목에 기본값 세팅
//------------------------------------------------------------------------------------------
function chk_handler(){

	// 처리자구분 체크
	var p_PART_CD		= GetUserInfo("gPART_CD");
	var p_USR_GRD_CD	= GetUserInfo("gUSR_GRD_CD");

	// PART_CD : BNS1 상담1파트, BNS2 상담2파트, BNS3 상담3파트, BNS4 LPP파트, GSC1 GSC직원, ORD1 주문파트, PLN1 QA파트, PLN2 불만파트, SYS1 관리파트, TEC1 테크서비스
	// USR_GRD_CD : A 관리자, B 센터장, C 그룹장, D 파트장, E 선임 상담사, F 일반 상담사, G TEC관리자, H TEC결재권자

	// 불만파트 -> 불만담당
	if (p_PART_CD == "PLN2"){
		Chk_Handler1.value = "N";
		Chk_Handler2.value = "Y";
		Chk_Handler3.value = "N";
	}else{
		// 관리자, 센터장, 그룹장, 파트장 -> 파트장
		if (p_USR_GRD_CD == "A" || p_USR_GRD_CD == "B" || p_USR_GRD_CD == "C" || p_USR_GRD_CD == "D"){
			Chk_Handler1.value = "N";
			Chk_Handler2.value = "N";
			Chk_Handler3.value = "Y";
		}
		// 상담사 -> 상담사
		else if (p_USR_GRD_CD == "E" || p_USR_GRD_CD == "F"){
			Chk_Handler1.value = "Y";
			Chk_Handler2.value = "N";
			Chk_Handler3.value = "N";
		}
		// 파트장, 상담사 제외 -> 불만담당
		else{
			Chk_Handler1.value = "N";
			Chk_Handler2.value = "Y";
			Chk_Handler3.value = "N";
		}
	}

	// 불만접수처리자이력 테이블 입력데이터 설정
	ds_dspsr_hist_in.setColumn(ds_dspsr_hist_in.rowposition,"DSPSR_ID", GetUserInfo("gUSR_ID")); // 처리자 ID
	ds_dspsr_hist_in.setColumn(ds_dspsr_hist_in.rowposition,"DSPSR_CHRG_DEPT", "고객센터"); // 처리부서 - 통합CRM센터

	// 불만접수처리 테이블 입력데이터 설정
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_ID", GetUserInfo("gUSR_ID")); // 처리자 ID
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_CHRG_DEPT", "고객센터"); // 처리부서 - 통합CRM센터
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CD", GetUserInfo("gUSR_GRD_CD")); // 상담원 등급 코드
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_OFLV_CDNM", GetUserInfo("gUSR_GRD_CDNM")); // 상담원 등급 코드명
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_TEL_NO", GetUserInfo("gPHONE_NO")); // 내선번호
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_MBPH_NO", maskPhone(GetUserInfo("gMBPH_NO"))); // 휴대전화
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSPSR_EML_ADDR", GetUserInfo("gEML_ADDR")); // 이메일
}
//------------------------------------------------------------------------------------------
//고객구분 이벤트 - 고객구분에 따라 불만유형이 바뀜
//------------------------------------------------------------------------------------------
function Rdo_Cust_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	var chkValue;	//고객구분 체크값

	//개인고객, 법인고객 구분
	switch (Rdo_Cust.value) {
		case "2" :
			chkValue = "C";
			break;
		default :
			chkValue = "P";
			break;
	}
	var ds_obj = eval(Cmb_Type.innerdataset);
	
	trace("===>"+ds_obj.saveXML());
	
	ds_obj.filter("CODE1 =='" + chkValue + "'|| CODE1==''");	

	// 고객구분 선택할때마다 콤보 초기화
	Cmb_Type.index = 0;
	ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSFT_TYP_CD", Cmb_Type.value);

	Cmb_Type_onitemchanged();
}

//------------------------------------------------------------------------------------------
//불만유형 콤보 이벤트
//------------------------------------------------------------------------------------------
function Cmb_Type_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	//불만유형 간 화면전환 시, 데이터 초기화
	Data_Clear();
	
	var ds_obj = eval(Cmb_DtlType.innerdataset);			
	ds_obj.filter("HIGH_CODE =='" + Cmb_Type.value + "'|| HIGH_CODE==''");

	// 불만유형이 바뀌면 DS에 기존 불만세부유형값이 남아있어 강제로 인덱스 설정
	Cmb_DtlType.index = 0;	
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"DSFT_TAXN_TYP_CD",Cmb_DtlType.value );
	
	// CA = 품질, CK = 혼유, CL = 주유량, CE = 탱크로리
	if(Cmb_Type.value == "CA" || Cmb_Type.value == "CK"){
		Div02_case01.visible = false;
		Div02_case02.visible = true;
		Div02_case03.visible = false;
		Div02_case04.visible = false;

		// 상담사의 경우 Kixx품질서비스차량출동 선택 불가
		var p_USR_GRD_CD = GetUserInfo("gUSR_GRD_CD");
		if (p_USR_GRD_CD == "E" || p_USR_GRD_CD == "F")
		{
			Div01.visible = false;
		}else{
			Div01.visible = true;
		}
	}else if(Cmb_Type.value == "CL"){
		Div02_case01.visible = false;
		Div02_case02.visible = false;
		Div02_case03.visible = true;
		Div02_case04.visible = false;
		Div01.visible = false;
	}else if(Cmb_Type.value == "CE"){
		Div02_case01.visible = false;
		Div02_case02.visible = false;
		Div02_case03.visible = false;
		Div02_case04.visible = true;	
		Div01.visible = false;
	}else{
		Div02_case01.visible = true;
		Div02_case02.visible = false;
		Div02_case03.visible = false;
		Div02_case04.visible = false;	
		Div01.visible = false;
	}
}

