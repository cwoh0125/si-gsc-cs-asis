	if(!cf_size_Alert(ds_cmp_in, ds_cmp_in.rowposition , "PROC_RSLT_CTT" , Txa_Result , 4000)) return;

	/* TM I/F 변경으로 인한 주석처리
	//필수값체크
	if(getTrim(Edt_HandlerID.value) == "" && Cmb_Center.value == ""){
		cf_Alert("COME0003", "처리자");  // !@ 은(는) 필수입니다.
		return;
	}
	*/
	
	//필수값체크
	if(!(Cmb_Type.value == "CN" || Cmb_Type.value == "CQ")) {
		if(getTrim(Edt_HandlerID.value) == ""){
			cf_Alert("COME0003", "처리자");  // !@ 은(는) 필수입니다.
			return;
		}
	}else {
		//불만접촉이력에서 이관
		ds_alarm.clearData();
		ds_dspsr_hist_in.clearData();
		
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_ID", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_ID_NM", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_YN_CNSLR", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_YN_DSFT_CHRG", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_YN_PART_HD", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_CHRG_DEPT", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_OFLV_CD", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_OFLV_CDNM", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_TEL_NO", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_MBPH_NO", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "DSPSR_EML_ADDR", "");
		
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE1", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE2", "");
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE3", "");
	}
	
	if(getTrim(Txa_CustCmp.value) == ""){
		cf_Alert("COME0003", "고객의 소리");  // !@ 은(는) 필수입니다.
		return;
	}
	
	var subStr = Txa_CustCmp.value;
	var subSmr = subStr.substr(0,2000);
	var subSmr1 = subStr.substr(2000,2000);
	
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"CUST_SMLRI",subSmr);
	ds_cmp_in.setColumn(ds_cmp_in.rowposition,"CUST_SMLRI1",subSmr1);	
	
	/*
	//OB이관 체크
	//TM I/F로 인한 주석(OB이관은 불만처리이력에서 처리)
	if(getTrim(Cmb_Center.value) != ""){
	
	ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE1", cmb_ob1.value);
	ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE2", cmb_ob2.value);
	ds_cmp_in.setColumn(ds_cmp_in.rowposition, "OB_CODE3", cmb_ob3.value);
	
	 if(getTrim(cmb_ob1.value)=="" || getTrim(cmb_ob2.value)=="" || getTrim(cmb_ob3.value)=="" ){
	 alert("OB이관의 경우 하단의 상세 코드는 필수입력 사항 입니다.");
	 return;
	 }
	
	}
	*/
	

	// Kixx품질서비스차량출동 체크
	if(getTrim(Div01.Cmb_Kixx.value) == "Y"){
		if(getTrim(Div01.Edt_QltCmpType.value) == ""){
			cf_Alert("COME0003", "품질불만유형");  // !@ 은(는) 필수입니다.
			return;
		}
		if(getTrim(Div01.Edt_QltCmpCause1.value) == "" && getTrim(Div01.Edt_QltCmpCause2.value) == ""){
			cf_Alert("COME0003", "품질불만원인");  // !@ 은(는) 필수입니다.
			return;
		}
	}

	if(getTrim(ds_cmp_in.getColumn(ds_cmp_in.rowposition, "DSFT_TYP_CD")) == "CE"){ // 불만유형이 탱크로리인 경우 배송차량정보 필수
		/*
		if(getTrim(ds_cmp_in.getColumn(ds_cmp_in.rowposition, "DELI_VHCL_NO")) == ""){
			cf_Alert("COME0003", "배송차량정보");  // !@ 은(는) 필수입니다.
			Div02_case04.Btn_TankSearch.setFocus();
			return;
		}
		*/
	}/*else{ // 이외의 불만유형의 경우 불만발생업체 필수
		if(getTrim(ds_cmp_in.getColumn(ds_cmp_in.rowposition, "ORGN_PTR_ST_CD")) == ""){
			cf_Alert("COME0003", "불만발생업체");  // !@ 은(는) 필수입니다.
			return;
		}
	}*/

	// 불만유형이 품질/혼유, 주유량인 경우 주유충전일과 문제발생일의 기간체크 : CA = 품질, CK = 혼유
	if(Cmb_Type.value == "CA" || Cmb_Type.value == "CK"){
		if(getTrim(Div02_case02.Cal_ChargeDate.value) - getTrim(Div02_case02.Cal_OccurDate.value) > 0){			
			cf_Alert("CAME0006", "문제발생일", "주유충전일");	 // 문제발생일 보다 주유충전일가(이) 더 큽니다.
			Div02_case02.Cal_ChargeDate.setFocus();
			return false;			
		}
	}
	else if(Cmb_Type.value == "CL"){
		if(getTrim(Div02_case03.Cal_ChargeDate.value) - getTrim(Div02_case03.Cal_OccurDate.value) > 0){			
			cf_Alert("CAME0006", "문제발생일", "주유충전일");	 // 문제발생일 보다 주유충전일가(이) 더 큽니다.
			Div02_case03.Cal_ChargeDate.setFocus();
			return false;			
		}
	}

	// 처리결과에 입력항목이 있으면 처리시간이 입력되어야 한다.
	if(getTrim(Txa_Result.value) != ""){

		//필수값체크
		if(!cf_chk_data(Cal_HandleDate,"처리일")) return;
		if(!cf_chk_data(Msk_HandleTime,"처리시간")) return;
		if(!cf_chk_data(Edt_HandlerID,"처리자")) return;
	
		//처리시간 체크
		Msk_HandleTime.value = Number(Msk_HandleTime.value);	// 01~09 -> 1~9로 초기화
		if(getTrim(Msk_HandleTime.value) < 0 || getTrim(Msk_HandleTime.value) > 24 || getTrim(Msk_HandleTime.value) == ""){
			cf_Alert("CMPE0001", "0", "24"); // !@ 와 !@ 사이의 값을 입력하여야 합니다.
			Msk_HandleTime.value = "";
			Msk_HandleTime.setFocus();
			return;
		}else{
			if(Msk_HandleTime.value > -1 && Msk_HandleTime.value < 10)
				Msk_HandleTime.value = "0" + Msk_HandleTime.value;
		}
		
		//처리일자 + 처리일시 입력
		var strHandleDate = Cal_HandleDate.value + Msk_HandleTime.value;
		ds_cmp_in.setColumn(ds_cmp_in.rowposition, "PROC_DTM", strHandleDate);
		
		//처리결과 입력일시(PROC_RSLT_INPUT_DTM-접수처리테이블), 처리여부(PROC_YN-처리자지정이력 테이블) 입력
		if(getTrim(Cal_HandleDate.value) != ""){
			ds_cmp_in.setColumn(ds_cmp_in.rowposition, "PROC_RSLT_INPUT_DTM", "true");
			ds_dspsr_hist_in.setColumn(ds_dspsr_hist_in.rowposition, "PROC_YN", "Y");
		}
	}else{
		//처리결과가 없는데, 처리일자 or 처리일시가 있는 경우 처리결과 필수체크
		if(getTrim(Cal_HandleDate.value) != ""){
			if(!cf_chk_data(Txa_Result,"처리결과")) return;
		}
		if(getTrim(Msk_HandleTime.value) != ""){
			if(!cf_chk_data(Txa_Result,"처리결과")) return;
		}
	}

	if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?

	// 전화번호 양식을 변환하여 저장
//	ds_cmp_in.setColumn(ds_cmp_in.rowposition, "MBPH_NO", Edt_CustMobileNo.value.replace("-",""));

	ds_alarm.clearData();
	if (g_alarmYn){
