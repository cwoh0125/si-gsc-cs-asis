		ds_alarm.setColumn(nRow,"ALRM_DV_CD", "1");
		ds_alarm.setColumn(nRow,"CNSLR_ID", Edt_HandlerID.value);
	}

//trace(ds_alarm.saveXML());

	var	strSvcID = "C1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "INSERT_0_IDATA=ds_cmp_in:a INSERT_1_IDATA=ds_dspsr_hist_in:a INSERT_2_IDATA=ds_alarm:a SEQSRCH_IDATA=ds_SEQ:a"; 
	var	strOutDatasets = "";
