function BNS420_onload(obj:Form, e:LoadEventInfo)
{
	//sta_cardtype1.text = "";
	//sta_cardtype2.text = "";
	
	getDsName = this.getOwnerFrame().getDsArgument;
	setDsName = this.getOwnerFrame().setDsArgument;
	
	ds_srch_cond.clearData();
	ds_srch_cond.addRow();
	ds_srch_cond.setColumn(0,"CUR_PAGE","1");
	ds_srch2_cond.clearData();
	ds_srch2_cond.addRow();
	ds_srch2_cond.setColumn(0,"CUR_PAGE","1");
	
	cal00_card01.value 		= getToday();
	cal00_card00.value 	= AddMonth(getToday(),-3);
	
	cal01_card01.value 		= getToday();
	cal01_card00.value 	= AddMonth(getToday(),-3);
	
	edt1_card_no3.setFocus();

}


