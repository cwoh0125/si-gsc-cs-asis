function btn_srch1_onclick(obj:Button,  e:ClickEventInfo)
{
	getDsName = this.getOwnerFrame().getDsArgument;
	setDsName = this.getOwnerFrame().setDsArgument;
	
				
	if (edt1_card_no1.value==null || edt1_card_no2.value==null || edt1_card_no3.value==null || edt1_card_no4.value==null ||
	    edt1_card_no1.value=="" || edt1_card_no2.value=="" || edt1_card_no3.value=="" || edt1_card_no4.value==""){

	cf_Alert("COME0016");

	}else if(cal00_card00.value==null || cal00_card01.value==null  || cal00_card00.value=="" || cal00_card01.value==""){
	
	cf_Alert("COME0002","조회 기간은 필수조건 입니다.");
	return;
	}else{
	
		var strCompDt = AddMonth(cal00_card00.value,3);
		if(strCompDt < cal00_card01.value){
			cf_Alert("COME0019","3개월");
			return;
		}

		if(getDsName.getColumn(0,'GRP_AGR_YN')=="Y"){
			//reSearch_Grp();
			reSearch();
		}else{
			reSearch();
		}
	
	}	//sta_row1.text=grd_card_list1.rowcount;
    
    //NTC 카드조회 카드번호 자동입력 (20140507 NTC 카드입력란 자동 세팅 추가 - 허선화P)
    edt_c1.value = edt1_card_no1.value
    edt_c2.value = edt1_card_no2.value
    edt_c3.value = edt1_card_no3.value
    edt_c4.value = edt1_card_no4.value
    
    edt_c4_ontextchanged();
    
}

