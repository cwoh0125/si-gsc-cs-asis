		PopupDialog("CAM031","CAM::CAM031.xfdl", 450, 300, 833, 400, false, "absolute", arrParam, false, false, true, false);
	}
}

function Edt_CustNo_onkeyup(obj:Edit, e:KeyEventInfo)
{
