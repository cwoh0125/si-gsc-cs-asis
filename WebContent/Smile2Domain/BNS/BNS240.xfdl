function setDefaultValue(){

	//콤보생성하기	
    var Combo           = ["Cmb_SiteCode","Cmb_VanderCp","Cmb_Ordsts"];    	//콤보ID
    var SearchKey       = ["NW083","NW084", "NW085"];       				//공통코드-대분류코드
    var ListMode        = ["TY", "TY","TY"];          						//CTN : [코드] 코드명  , TY : 코드명,  TN: 초기값무시
    var OptionRowValue  = [",전체", ",전체", ",전체"];      				//코드상단에 로우추가후 전체, 공백

	setComboList(Combo, SearchKey, ListMode, OptionRowValue);     			//공통콤보가져오기
}
/********************************************************************************** 
 *****	주문내역 조회
***********************************************************************************/
function Btn_Srch_onclick(obj:Button,  e:ClickEventInfo)
{ 
	ds_if_env.clearData();
	ds_if_data.clearData();
	ds_if_header.clearData();
	ds_if_order_record.clearData();
	ds_if_common.clearData();
	ds_if_orderdtls_record.clearData();
	
	if (cf_isNull(Edt_Muid.value)) {
		cf_Alert("COME0003", "고객번호");  //msgCont 은(는) 필수입니다.
		Edt_Muid.setFocus();
		return false;
	}
	if(!cf_ValidDate(Cal_OrddtRt.value, Cal_OrddtTo.value, 'N')) {
		Cal_JOINDTFR.setFocus();
		return false;
	}
	
	if(    getTrim(Cmb_SiteCode.value) 	== "" 
	    && getTrim(Cmb_VanderCp.value) 	== ""  
	    && getTrim(Edt_OrderCode.value) == "" 
	    && getTrim(Edt_ByName.value) 	== "" 
	    && getTrim(Edt_Muid.value) 		== "" 
	    && getTrim(Cmb_Ordsts.value) 	== "" 
	    && getTrim(Edt_RcName.value) 	== "" 
	    && getTrim(Edt_RcTel.value) 	== "" 
	    && getTrim(Cal_OrddtRt.value) 	== "" 
	    && getTrim(Cal_OrddtTo.value) 	== "" ){
			cf_Alert("COME0007","조회조건");			
		return false;
	}
	
	//IF_BNS240_01 환경설정
	fun_ds_srch(ds_if_env, "IF_KIND", 				"MPP");
	fun_ds_srch(ds_if_env, "REQ_SERVICE_METHOD", 	"CRMIF/CRMIF_MCPOrderList.aspx");
	fun_ds_srch(ds_if_env, "REQ_SERVICE_ID", 		"");
	fun_ds_srch(ds_if_env, "RES_HEADER_SECTION", 	"RSV_RSLT");
	fun_ds_srch(ds_if_env, "RES_COLUMN_SECTION", 	"");
	fun_ds_srch(ds_if_env, "RES_RECORD_SECTION", 	"LIST");
	
	//IF_BNS240_01 INPUT DATA
	fun_ds_srch(ds_if_data, "SITECODE", 	Cmb_SiteCode.value);		//사이트코드    
	fun_ds_srch(ds_if_data, "VANDERCP", 	Cmb_VanderCp.value);   		//벤더/CP사     
	fun_ds_srch(ds_if_data, "ORDERCODE", 	Edt_OrderCode.value);  		//주문번호      
	fun_ds_srch(ds_if_data, "BYNAME",   	Edt_ByName.value);     		//주문자명      
	fun_ds_srch(ds_if_data, "MUID",     	Edt_Muid.value);       		//고객번호      
	fun_ds_srch(ds_if_data, "ORDSTS",   	Cmb_Ordsts.value);     		//주문상태      
	fun_ds_srch(ds_if_data, "RCNAME",   	Edt_RcName.value);     		//수취인명      
	fun_ds_srch(ds_if_data, "RCTEL",    	Edt_RcTel.value);      		//수취인연락처  
	fun_ds_srch(ds_if_data, "ORDDTFR",  	Cal_OrddtRt.text);    		//주문일자 From 
	fun_ds_srch(ds_if_data, "ORDDTTO",  	Cal_OrddtTo.text);    		//주문일자 To  
		
	
	var	strSvcID = "IF_MCPOrderList"; //IF_MCPOrderList
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_env:a INTERFACE_DATA=ds_if_data:a";
	var	strOutDatasets = "ds_if_header=DS_HEADER ds_if_order_record=DS_RECORD ds_if_common=DS_COMMON";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_orderList";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
	//trace(ds_if_common.saveXML());
}


