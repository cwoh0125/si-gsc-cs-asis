function fn_callBack_orderList(svcid,strErrCode,strErrMsg){	
	
	//2015.08.15 개인정보접촉이력
	trace("=============개인정보접촉이력  이벤트 MCP 조회Start===========");

	var sAccNo = "BNS240"; //개인정보접촉이력 일련번호
	var sPerTCd = "PT1002";  // 수행업무 코드
	var sPerTDt = "ICCS_CSC 이벤트 MCP 조회";  // 수행업무 상세내용
	var sCalFun = "Btn_Srch_onclick()";  // 호출함수명 
	var sInqCon = "|" + "SITECODE="+getTrim(Cmb_SiteCode.value)
				+ "|" + "VANDERCP="+getTrim(Cmb_VanderCp.value)
				+ "|" + "ORDERCODE="+getTrim(Edt_OrderCode.value)
				+ "|" + "BYNAME="+getTrim(Edt_ByName.value)
				+ "|" + "MUID="+getTrim(Edt_Muid.text)
				+ "|" + "ORDSTS="+getTrim(Cmb_Ordsts.text)
				+ "|" + "RCNAME="+getTrim(Edt_RcName.text)
				+ "|" + "RCTEL="+getTrim(Edt_RcTel.text)
				+ "|" + "ORDDTFR="+getTrim(Cal_OrddtRt.text)
				+ "|" + "ORDDTTO="+getTrim(Cal_OrddtTo.text)
				+ "|" ;  // 조회조건 
				
	var sInqCnt = ds_if_order_record.rowcount;  // 조회건수
	var sInCustNo = ds_if_data.getColumn(0,"MUID") ;//식별키(고객번호)		
	 
	trace("개인정보접촉이력--이벤트 MCP 조회데이터 "+"    "+sAccNo 
													  +"    "+sPerTCd
													  +"    "+sPerTDt
													  +"    "+sCalFun
													  +"    "+sInqCon
													  +"    "+sInqCnt); 
									  
	trace("=============개인정보접촉이력 이벤트 MCP 조회END==========="); 
	//fn_Privcnthis(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt);
    fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);


	if(ds_if_common.getColumn(0,"HTTP_RET_CODE") == '-1'){				
		cf_Alert("COME0035", ds_if_common.getColumn(0,"HTTP_RET_MSG"));									
	}else{
		if(ds_if_header.getColumn(0,"RESULT_CODE") != '0001'){						
			cf_Alert("COME0035", ds_if_header.getColumn(0,"RESULT_MSG"));	
			Stt_OrderDtlsCnt.text = 0;
		}else{
			ds_if_order_record.keystring="S:-ORDERDATE";
			
			
			
			for(var i=0; i <ds_if_order_record.rowcount; i++){
			
				ds_if_order_record.setColumn(i ,"BYNAME",maskCustNm(ds_if_order_record.getColumn(i,"BYNAME")));
			
				if(ds_if_order_record.getColumn(i, "CANCELYN") == "Y"){
					ds_if_order_record.setColumn(i, "CANCELYNNM", "가능");
				}else if(ds_if_order_record.getColumn(i, "CANCELYN") == "N"){
					ds_if_order_record.setColumn(i, "CANCELYNNM", "불가능");
				}else if(ds_if_order_record.getColumn(i, "CANCELYN") == "C"){
					ds_if_order_record.setColumn(i, "CANCELYNNM", "취소 된건");
				}else if(ds_if_order_record.getColumn(i, "CANCELYN") == "D"){
					ds_if_order_record.setColumn(i, "CANCELYNNM", "취소성공후 CP사 반영오류");
				}									
							
			}
		}
	}				
	Stt_OrderCnt.text = ds_if_order_record.rowcount;			
}

