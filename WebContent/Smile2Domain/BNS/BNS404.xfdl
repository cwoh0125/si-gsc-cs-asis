    <Script type="xscript4.0"><![CDATA[/********************************************************************************** 
 *****	상담관리 - RF회원카드 신청내역
***********************************************************************************/

include "LIB::CommonLib.xjs";

var SVC_IF_URL 	= "XinterfaceAction.do?method=CommonInterface";
var objDsCust 	= this.parent.parent.all["ds_cust_info"];
var objCntStt	= this.parent.parent.all["Stt_CrdInfoCnt"];	

function fn_srchFormData()
{
	// 상담관리에서 조회한 고객이 없을 경우
	if(objDsCust.getColumn(0,"IO_CUST_NO") == ""){
		fn_clearFormData();
		return;
	}
	
	//날짜생성
	var objDate = new Date();
	//var strDate = objDate.toFormatString("%Y%m%d", "ko_KR");
	
	//테스트 데이터
	var strDate = '20211030';
	
	var In_cust_no = objDsCust.getColumn(0,"IO_CUST_NO");
	
	//다이나믹프라이싱 조회
	fn_dynamicPricingList(In_cust_no,strDate);
	
}

//초기화
function fn_clearFormData()
{
	ds_dynamic_pricing_record.clearData();
}

//다이나믹프라이싱 내역 IF
function fn_dynamicPricingList(In_cust_no,strDate) {

	//초기화
	ds_if_ev_pricing_env.clearData();
	ds_if_ev_pricing_data.clearData();
	ds_if_ev_pricing_header.clearData();
	ds_if_ev_pricing_column.clearData();
	ds_if_ev_pricing_common.clearData();
	ds_dynamic_pricing_record.clearData();
	
	
	//IF_BNS260_01 환경설정
	fun_ds_srch(ds_if_ev_pricing_env, "IF_KIND", 				"WAS-EVAPP"); //IF 종류
	fun_ds_srch(ds_if_ev_pricing_env, "REQ_SERVICE_METHOD", 	"api/dynamicPricing"); //인터페이스URL
	fun_ds_srch(ds_if_ev_pricing_env, "REQ_SERVICE_ID", 		"");
	fun_ds_srch(ds_if_ev_pricing_env, "RES_HEADER_SECTION", 	"dynamicPricing_Rsp"); //xml상위 헤더항목
	fun_ds_srch(ds_if_ev_pricing_env, "RES_COLUMN_SECTION", 	""); 
	fun_ds_srch(ds_if_ev_pricing_env, "RES_RECORD_SECTION", 	"dynamicPricing"); 
	
	
		
	fun_ds_srch(ds_if_ev_pricing_data, "custNo", 			In_cust_no);
	fun_ds_srch(ds_if_ev_pricing_data, "dpAplyDttm", 		strDate);
	
	
	var	strSvcID = "IF_CSC_EVT_001"; //인터페이스ID
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_ev_pricing_env:a INTERFACE_DATA=ds_if_ev_pricing_data:a"; 
	var	strOutDatasets = "ds_if_ev_pricing_header=DS_HEADER ds_if_ev_pricing_column=DS_COLUMN ds_dynamic_pricing_record=DS_RECORD ds_if_ev_pricing_common=DS_COMMON";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);		
}


//콜백함수
function fn_callBack(svcid,strErrCode,strErrMsg){

	if(svcid == "IF_CSC_EVT_001"){
		var ot_respon = ds_if_ev_pricing_header.getColumn(0,"RESULTCODE");	
		if(ot_respon == "0000"){
			//trace(ds_dynamic_pricing_record.saveXML());
			for(var i=0; i < ds_dynamic_pricing_record.rowcount; i++) {
				var objDate = new Date(ds_dynamic_pricing_record.getColumn(i, "DPAPLYFRDTTM"));
				
				var dpAplyFrDttm = objDate.toFormatString('%Y-%m-%d %H:%M:%S');
				var aplyFrTm = ds_dynamic_pricing_record.getColumn(i, "APLYFRTM");
				
				//trace("dpAplyFrDttm == > " + dpAplyFrDttm);
				//trace("aplyFrTm == > " + aplyFrTm);
			}
		}
		//건수표시
		objCntStt.text	= ds_dynamic_pricing_record.rowcount;
	}
}


function Grd_CrdInfo_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	cf_gridSort(obj, e);
}

// 셀내용복사
function Grd_CrdInfo_onkeydown(obj:Grid, e:KeyEventInfo)
{
	if(e.ctrlKey){
		if(e.keycode == 67){
			var v_clip;
			v_clip = obj.getCellValue(obj.currentrow,obj.currentcol);
			system.clearClipboard();
			system.setClipboard("CF_TEXT",v_clip);
		}	
	}	
}

// 다이나믹프라이싱 충전소 조회
function Grd_CrdInfo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	ds_get_param.clearData();
	ds_set_param.clearData();
	
	fun_ds_srch(ds_get_param, "custNo", getTrim(objDsCust.getColumn(0,"IO_CUST_NO"))); 				 //자사고객번호
	fun_ds_srch(ds_get_param, "dpId", 	getTrim(ds_dynamic_pricing_record.getColumn(e.row,"DPID"))); //다이나믹프라이싱ID
	
	var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};
	
	PopupDialog("BNS405","BNS::BNS405.xfdl", 393, 23, 816, 220, false, "absolute", arrParam, false, false, true, false);
}
]]></Script>
