function fn_srchFormData()
{
	var objDsProcYn = this.parent.parent.parent.parent.all["ds_tab_proc_yn"];
	var strTpNm 	= this.parent.name + "_" + this.name;
	var objCntStt	= this.parent.parent.parent.parent.all["Stt_HistInfoCnt"];	
