/************************************************************************************************************************
********************************************고객===========입력쪽
************************************************************************************************************************/
function Btn_cus_find_v_onclick(obj:Button,  e:ClickEventInfo)
{	
	ds_get_param.clearData();
	
	fun_ds_srch(ds_get_param, "gubun", "1");
	
	
	var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};
	PopupDialog("CMP130", "CMP::CMP130.xfdl", -1, -1, 770, 300, false, -1, arrParam, false);
	
	
	if(ds_set_param.rowcount != 0 ){
		if(Edt_corId.value == getTrim(ds_set_param.getColumn(0,"CUST_ID"))){
			cf_Alert("HELE0010", "고객"); //동일한 !@ 가(이) 존재합니다.
			return;
		}
		
		for(var i=0; i < ds_cust.rowcount; i++){	  
			if(ds_cust.getColumn(i, "CUST_ID") == getTrim(ds_set_param.getColumn(0,"CUST_ID"))){
				cf_Alert("HELE0010", "고객"); //동일한 !@ 가(이) 존재합니다.				
				return;
			}
		}
		
		for(var i=0; i < ds_tgt.rowcount; i++){	  
			if(ds_tgt.getColumn(i, "SPCL_MTTR_SEQ_NO") == tot_seqNo && ds_tgt.getColumn(i, "CUST_ID") == getTrim(ds_set_param.getColumn(0,"CUST_ID"))){
				cf_Alert("HELE0010", "고객"); //동일한 !@ 가(이) 존재합니다.				
				return;
			}
		}
		
		if(Edt_corNm.value != null && Edt_corNm.value != ""){

			var rn = ds_tgt.addRow();
			ds_tgt.setColumn(rn, 'SPCL_MTTR_SEQ_NO', tot_seqNo);
			ds_tgt.setColumn(rn, 'CUST_NM', ds_set_param.getColumn(0,"CUST_NM"));
			ds_tgt.setColumn(rn, 'CUST_ID', ds_set_param.getColumn(0,"CUST_ID"));
			ds_tgt.setColumn(rn, 'CUST_DV_CD',ds_set_param.getColumn(0,"CUST_GUBUN"));
			Edt_corNm.value 	+= ":"+getTrim(ds_set_param.getColumn(0,"CUST_NM"));
			Edt_corId.value 	+= ":"+getTrim(ds_set_param.getColumn(0,"CUST_ID"));
			
		}else{
			var rn = ds_tgt.addRow();
			ds_tgt.setColumn(rn, 'SPCL_MTTR_SEQ_NO', tot_seqNo);
			ds_tgt.setColumn(rn, 'CUST_NM', ds_set_param.getColumn(0,"CUST_NM"));
			ds_tgt.setColumn(rn, 'CUST_ID', ds_set_param.getColumn(0,"CUST_ID"));
			ds_tgt.setColumn(rn, 'CUST_DV_CD',ds_set_param.getColumn(0,"CUST_GUBUN"));
			Edt_corNm.value 	= getTrim(ds_set_param.getColumn(0,"CUST_NM"));
			Edt_corId.value 	= getTrim(ds_set_param.getColumn(0,"CUST_ID"));
		}
	}
	ds_set_param.clear();
}

