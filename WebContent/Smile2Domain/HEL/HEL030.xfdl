	//콤보생성하기	
    var Combo           = ["Cmb_prdct_s", "Cmb_prdct"];    			//콤보ID
    var SearchKey       = ["NW101", "NW101"];              		//공통코드-대분류코드
    var ListMode        = ["TY", "TY"];                    	//CTN : [코드] 코드명  , TY : 코드명,  TN: 초기값무시
