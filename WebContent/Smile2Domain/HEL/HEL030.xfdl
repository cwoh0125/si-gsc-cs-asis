/************************************************************************************************************************
***********************************************삭제
************************************************************************************************************************/
function Btn_del_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_emergency.getColumn(ds_emergency.rowposition, "ADDROW_YN") == "N"){
		if (!cf_Confirm("QASI0002"))	return;  //삭제하시겠습니까?	
		
		ds_srch.clearData();
		
		fun_ds_srch(ds_srch, "SPCL_MTTR_SEQ_NO", ds_emergency.getColumn(ds_emergency.rowposition, "SPCL_MTTR_SEQ_NO"));
		fun_ds_srch(ds_srch, "CUST_ID", ds_emergency.getColumn(ds_emergency.rowposition, "CUST_ID"));
		
		var	strSvcID = "U1";
		var	strURL = SVC_USR_URL;
		
		if(ds_emergency.getColumn(ds_emergency.rowposition, "TGT_CNT") == "0" || ds_emergency.getColumn(ds_emergency.rowposition, "TGT_CNT") == "1"){
			var	strInDatasets 	= "DELETE_0_IDATA=ds_srch:a UPDATE_1_IDATA=ds_srch:a"; 
			var	strOutDatasets 	= "ds_emergency=SEARCH_2_ODATA";
			var	strArgument 	= "DELETE_0_ID=HEL030.Set_bns_List_DELETE UPDATE_1_ID=HEL030.Set_bns_List_ENTR_UPDATE";
		}else{
			var	strInDatasets 	= "DELETE_0_IDATA=ds_srch:a"; 
			var	strOutDatasets 	= "ds_emergency=SEARCH_1_ODATA";
			var	strArgument 	= "DELETE_0_ID=HEL030.Set_bns_List_DELETE";
		}
		
		var strCallbackFunc = "fn_callBack_delete";
		var strProcessBar = true;
		
		var sync_yn = false;
		
		cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar,sync_yn);		
	}else{
		ds_emergency.deleteRow(ds_emergency.rowposition);	
	}
}
function fn_callBack_delete(){
	cf_Alert("COMI0004"); //삭제되었습니다.
	Btn_sel_onclick();
}
