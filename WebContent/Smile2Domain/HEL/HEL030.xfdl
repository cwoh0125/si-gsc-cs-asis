function fn_emgCallBack()
{	
	var nEmgCount = 0;
	nEmgCount = ds_emer.rowcount;
	var i,j;
	if(nEmgCount > 0)
	{
		ds_emergency.addColumn("EMG_YN", "string",5);
		var nListCount = ds_emergency.rowcount;
		for(i = 0; i < nEmgCount; i++)
		{
			for(j = 0; j < nListCount; j++)
			{
				if(ds_emer.getColumn(i, "SPCL_MTTR_SEQ_NO") == ds_emergency.getColumn(j, "SPCL_MTTR_SEQ_NO"))
				{					
					ds_emergency.setColumn(j, "EMG_YN", "Y");
					break;
				}
			}
		}
	}
	
	Gid_bns_oncellclick();
}
