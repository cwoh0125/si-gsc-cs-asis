/************************************************************************************************************************
***********************************************페이지 로드
************************************************************************************************************************/
function HEL030_onload(obj:Form, e:LoadEventInfo)
{
	var lastDay = AddMonth(getToday(), +1).substr(0,6)+'01';
	
	Cal_start.value = getToday().substr(0,6)+'01';
	Cal_end.value = AddDate(lastDay, -1);
	
	prdct_list();
	//setAuth();  //권한체크
	setDefaultValue();
}


