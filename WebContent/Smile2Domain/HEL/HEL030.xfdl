************************************************************************************************************************/
function Btn_sel_onclick(obj:Button,  e:ClickEventInfo)
{
	//데이터초기화
	ds_emergency.clearData();	
	ds_sel.clearData();
	ds_cust.clearData();
	
	if(!cf_ValidDate(Cal_start.value, Cal_end.value)) {
		Cal_start.setFocus();
		return false;
	}
	if(getTrim(Edt_ctmNm.value).length == 1){
		cf_Alert("COME0049", "고객명"); //!@ 을(를) 두글자 이상 입력하셔야 합니다
		Edt_ctmNm.setFocus();
		return;
	}
	if(getTrim(Edt_registrant.value).length == 1){
		cf_Alert("COME0049", "등록인"); //!@ 을(를) 두글자 이상 입력하셔야 합니다
		Edt_registrant.setFocus();
		return;
	}
	if(getTrim(Edt_tite_cont.value).length == 1){
		cf_Alert("COME0049", "제목+내용"); //!@ 을(를) 두글자 이상 입력하셔야 합니다
		Edt_tite_cont.setFocus();
		return;
	}
	
	var tite_cont = Edt_tite_cont.value;
	var tite_cont_param = "";
	if(tite_cont != null){
		tite_cont_param = "%"+tite_cont+"%";
	}else{
		tite_cont_param = null;
	}
	//조회값
	fun_ds_srch(ds_sel, "VALD_STR_DT", Cal_start.value);
	fun_ds_srch(ds_sel, "VALD_END_DT", Cal_end.value);
	fun_ds_srch(ds_sel, "REG_ID", Edt_registrant.value);
	fun_ds_srch(ds_sel, "CUST_ID", Edt_ctmId.value);
	fun_ds_srch(ds_sel, "CUST_NM", Edt_ctmNm.value);
	fun_ds_srch(ds_sel, "BRN_ID", Edt_BRN_s.value);
	fun_ds_srch(ds_sel, "BRN_NM", Edt_BRNNM_s.value);
	fun_ds_srch(ds_sel, "OILRSV_ID", Edt_OILRSV_s.value);
	fun_ds_srch(ds_sel, "OILRSV_NM", Edt_OILRSVNM_s.value);
	fun_ds_srch(ds_sel, "TITE_CONTS", tite_cont_param);
	fun_ds_srch(ds_sel, "UGC_YN", Chk_emergency.value);	
	fun_ds_srch(ds_sel, "PRDCT_GRP_CD", Cmb_prdct_s.value);
	
	var	strSvcID = "R1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_sel:a"; 
	var	strOutDatasets = "ds_emergency=SELECT_0_ODATA";
	var	strArgument = "SELECT_0_ID=HEL030.Get_emergency_list_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
	//trace(ds_emergency.saveXML());
	return;
}

