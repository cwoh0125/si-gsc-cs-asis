/************************************************************************************************************************
***********************************************저장
************************************************************************************************************************/
function Btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	for (var i=0; i < ds_emergency.rowcount; i++){		
		if (ds_emergency.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_emergency.getRowType(i) == Dataset.ROWTYPE_UPDATE){
			
			//필수 체크
			if(!cf_ValidDate(ds_emergency.getColumn(i,"VALD_STR_DT"), ds_emergency.getColumn(i,"VALD_END_DT"))) {				
				ds_emergency.rowposition = i;
				Cal_start_v.setFocus();				
				return false;
			}
			
			if(!cf_chk_data(ds_emergency.getColumn(i,"RQSR_ID"),"공지자명")){
				ds_emergency.rowposition = i;				
				Edt_usrNm.setFocus();
				Edt_usrNm.value="이재윤";
				return false;
			}
			
			if(!cf_chk_data(ds_emergency.getColumn(i,"TITE"),"제목")){
				ds_emergency.rowposition = i;				
				Edt_tite.setFocus();
				return false;
			}
			if(!cf_chk_data(ds_emergency.getColumn(i,"CTT"),"내용")){
				ds_emergency.rowposition = i;				
				Txa_cont.setFocus();
				return false;
			}				
		}
	}
	
	
	trace("===>"+ds_emergency.getColumn(0,"POP_YN"));
	
	if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?
		
	var	strSvcID = "C1";
	var	strURL = SVC_URL;	
	
	
	var	strInDatasets = "COMSAVE_0_IDATA=ds_emergency:u"+
							" COMSAVE_1_IDATA=ds_tgt:u"; 
		var	strOutDatasets = "";
		var	strArgument = "COMSAVE_0_ID=HEL030.Set_bns_List"+
						  " COMSAVE_1_ID=HEL030.Set_tgt_List";

	var strCallbackFunc = "fn_callBack_save";
	var strProcessBar = true;
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
}
