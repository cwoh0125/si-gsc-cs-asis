function Btn_new_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_props.addRow();
	
	//쪽지 발송구분 1:발송하지않음  2:처리자에게발송  
	noteGbn = "2";
	
	var	strSvcID = "R2";
	var	strURL = SVC_USR_URL;
