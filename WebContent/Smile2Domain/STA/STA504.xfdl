 * Grid oncelldblclick 이벤트 - 선택
***********************************************************************************/
function Grd_S2SInfo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_List.rowcount < 1){
		cf_Alert("BNSE0036","s2s호전환 정보");		// 조회된 s2s호전환 정보가(이) 없습니다.
		return;
	}
	
}


