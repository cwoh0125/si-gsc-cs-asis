function Cal_ToStrDt_onkillfocus(obj:Calendar, e:KillFocusEventInfo)
{
  
  Cal_FromStrDt.value = AddMonth(Cal_ToStrDt.value,-1);
}

function Cal_ToStrDt_oncloseup(obj:Calendar, e:CalendarCloseUpEventInfo)
{
 
