}

//초기값설정
function setDefaultValue() {	
	
	//콤보생성하기	
    var Combo           = ["Cmb_GUBUN","Cmb_TERM","Cmb_BIZ"];   //콤보ID
    var SearchKey       = ["NW089","NW090","NW053"]; 			//접촉이력통계구분, 기간별통계구분
    var ListMode        = ["TY","TY","TY"];                   	//CTN : [코드] 코드명  , TY : 코드명,  TN: 초기값무시
    var OptionRowValue  = [",선택",",선택",",선택"];          //코드상단에 로우추가후 전체, 공백

	setComboList(Combo, SearchKey, ListMode, OptionRowValue);     //공통콤보가져오기
		
	//사용자콤보생성
	set_user_code();

	//년도콤보 조회
	var ds_year_from = eval(Cmb_YearFrom.innerdataset);
	var ds_year_to   = eval(Cmb_YearTo.innerdataset);

	Cmb_GUBUN.value = "01"; //통계구분(조직별)
	Cmb_SCH_GRP_CD.enable = true;
	Cmb_SCH_PT_CD.enable  = true;
	
	Cmb_BIZ.value = g_GRP_CD;
	Cmb_BIZ.enable = false;
	Cmb_BIZ_DTL1.enable = false;
	Cmb_BIZ_DTL2.enable = false;
	
	Cmb_TERM.value  = "03"; //기간구분(일별)
	//Cal_SRCH_CNSL_DTM_FROM.value = getToday();  //조회기간(시작일자)
	//Cal_SRCH_CNSL_DTM_TO.value = getToday();    //조회기간(시작일자) 

    var year = getToday().substr(0,4);
    var month = getToday().substr(4,2);
    var lastDay = ( new Date( year, month, 0) ).getDate();    
    var yyyymm = getToday().substr(0,6);
 
	Cal_SRCH_CNSL_DTM_FROM.value = yyyymm + "01";
	Cal_SRCH_CNSL_DTM_TO.value = yyyymm + "" +  lastDay;
	
	Cmb_YearFrom.value  = getToday().substring(0,4);
	Cmb_YearTo.value    = getToday().substring(0,4);

	Cmb_YearFrom.visible = false;
	//txtFromTo.visible = false;
	Cmb_YearTo.visible = false;

	ChkBoxOrder.enable = false;
}

function Cmb_SCH_GRP_CD_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var ds_part = eval(Cmb_SCH_PT_CD.innerdataset);		
		ds_part.filter("HIG_CODE ==" + wrapQuote(Cmb_SCH_GRP_CD.value) + "|| HIG_CODE==''");	
	if(Cmb_SCH_GRP_CD.value == ""){	ds_part.filter("");
		ds_cnslr.filter("");
	}
	
	var obj = eval(Cmb_SCH_PT_CD.innerdataset);
	var nRow = obj.insertRow(1);
	obj.setColumn(nRow, "CODE", "C" );
	obj.setColumn(nRow, "CODE_NM", "선택" );
	
	ds_head.clearData();  //초기화
	ds_srch.clearData();  //초기화
	
	//조회값입력	
	fun_ds_srch(ds_srch, "GRP_CD", Cmb_SCH_GRP_CD.value);			
		
	//그룹코드조회
	var	strSvcID = "R1";
	var	strURL = SVC_URL;
	var	strInDatasets = "COMSRCH_0_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_title=COMSRCH_0_ODATA";
	var	strArgument = "COMSRCH_0_ID=STA010.Get_MMAsesGrpCombo_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
}

function Cmb_BIZ_onitemchanged(obj:Combo, e:ItemChangeEventInfo) {
	var ds_BizDtls3 = eval(Cmb_BIZ_DTL1.innerdataset);

	ds_BizDtls3.filter("HIG_CODE =='" + Cmb_BIZ.value + "' || CODE == ''");
	Cmb_BIZ_DTL1.value = "";

}

function Cmb_BIZ_DTL1_onitemchanged(obj:Combo, e:ItemChangeEventInfo) {
	var ds_BizDtls2 = eval(Cmb_BIZ_DTL2.innerdataset);

	ds_BizDtls2.filter("HIG_CODE2 =='" + Cmb_BIZ_DTL1.value + "' || CODE == ''");
	Cmb_BIZ_DTL2.value = "";

	// 선택한 상세1 리스트 수정
	var intBefRow = ds_sel_biz_dtl.findRow("ORDER","1");
	if(intBefRow != -1){
		ds_sel_biz_dtl.deleteRow(intBefRow);
	}
	
	if(Cmb_BIZ_DTL1.value != ""){
		ds_sel_biz_dtl.addRow();
		ds_sel_biz_dtl.setColumn(ds_sel_biz_dtl.rowposition,"DTL1_CODE",Cmb_BIZ_DTL1.value);	
		ds_sel_biz_dtl.setColumn(ds_sel_biz_dtl.rowposition,"ORDER","1");		
	}
}

/*============================================================================*/
/*  메소드 명  : Btn_R2_onclick()
/*  내용 설명  : 통계조회
/*  RETURN 값  : 
/*============================================================================*/
function Btn_R2_onclick(obj:Button,  e:ClickEventInfo) {

	//그리드
	Grid_ord_hist_dd.visible = false; //일별
	Grid_ord_hist_ym.visible = false; //월별
	Grid_ord_hist_yy.visible = false; //년도별

	//데이터셋
	ds_bt_cnslr_hist_dd_ord.clearData(); //일별
	ds_bt_cnslr_hist_ym_ord.clearData(); //월별
	ds_bt_cnslr_hist_yy_ord.clearData(); //년도별
	
	//필수조건체크
	if (cf_isNull(Cmb_GUBUN.value)) {
		cf_Alert("COME0003", "통계구분");  //통계구분 은(는) 필수입니다.
		Cmb_GUBUN.setFocus();
		return false;
	}
	
	if (Cmb_GUBUN.value=="01") // 통계구분(조직별)
	{
		if (cf_isNull(Cmb_SCH_GRP_CD.value))
		{
			cf_Alert("COME0003", "그룹");  //그룹 은(는) 필수입니다.
			Cmb_SCH_GRP_CD.setFocus();
			return false;
		}	
	}
	else //통계구분(업무구분별)
	{
		if (cf_isNull(Cmb_BIZ.value ))
		{
			cf_Alert("COME0003", "업무구분");  //업무구분 은(는) 필수입니다.
			Cmb_BIZ.setFocus();
			return false;
		}
	}
	
	if (Cmb_TERM.value == '01')  // 년도별 기간체크
	{
		if ( getTrim(Cmb_YearFrom.value) == "" )
		{
			cf_Alert("COME0007","기간");
			Cmb_YearFrom.setFocus();
			return;
		}
		if ( getTrim(Cmb_YearTo.value) == "" )
		{
			cf_Alert("COME0007","기간");
			Cmb_YearTo.setFocus();
			return;
		}
		if (getTrim(Cmb_YearFrom.value) > getTrim(Cmb_YearTo.value))
		{
			cf_Alert("COME0012");
			Cmb_YearFrom.setFocus();
			return;
		}
	}
	else if(Cmb_TERM.value == '03') //일별 기간체크
	{
		var strDate = Cal_SRCH_CNSL_DTM_FROM.value.toFormatString("%Y%m%d", "ko_KR");
		var endDate = Cal_SRCH_CNSL_DTM_TO.value.toFormatString("%Y%m%d", "ko_KR");
		var sm = strDate.substring(4,6).replace(/(^0+)/,"");
		var em = endDate.substring(4,6).replace(/(^0+)/,"");
		var sd = strDate.substring(6,8).replace(/(^0+)/,"");
		var ed = endDate.substring(6,8).replace(/(^0+)/,"");
		var diffdate = DiffDate(strDate, endDate);
		
		if(sm != em)
		{	
			if ( diffdate > 30 )
			{
				cf_Alert("COME0019","한달"); 
				Cal_SRCH_CNSL_DTM_TO.setFocus();
				return;
			}	
								
			if(sd == ed)
			{
				cf_Alert("COME0002","동일한 일자를 선택할수 없습니다."); 
				Cal_SRCH_CNSL_DTM_TO.setFocus();
				return;			
			}
			
			if((diffdate == 29 || diffdate == 30)&& sd < ed)
			{
				cf_Alert("COME0002","종료일자를 시작일자 이전으로 선택해 주세요."); 
				Cal_SRCH_CNSL_DTM_TO.setFocus();
				return;			
			}			
		}		
	}
	else
	{
		if (cf_isNull(Cmb_TERM.value )) //년,월,일
		{
			cf_Alert("COME0003", "기간구분");  //기간구분 은(는) 필수입니다.
			Cmb_TERM.setFocus();
			return false;
		}
	
		if (cf_isNull(Cal_SRCH_CNSL_DTM_FROM.value )) //시작일자
		{
			cf_Alert("COME0003", "날짜");  //기간 시작 은(는) 필수입니다.
			Cal_SRCH_CNSL_DTM_FROM.setFocus();
			return false;
		}
	}
	
	fun_ds_srch(ds_srch, "JOB_GUBUN", Cmb_GUBUN.value);
	fun_ds_srch(ds_srch, "GRP_CD", Cmb_SCH_GRP_CD.value);
	fun_ds_srch(ds_srch, "PART_CD", Cmb_SCH_PT_CD.value);
	
	fun_ds_srch(ds_srch, "BIZ_PROC_DTLS_CD1", Cmb_BIZ_DTL1.value);
	fun_ds_srch(ds_srch, "BIZ_PROC_DTLS_CD2", Cmb_BIZ_DTL2.value);
	
	fun_ds_srch(ds_srch, "BIZ_DV_CD", Cmb_BIZ.value);
	fun_ds_srch(ds_srch, "TERM", Cmb_TERM.value);
	
	//조회기간
	if (Cmb_TERM.value == '01')  // 년
	{
		fun_ds_srch(ds_srch, "FROM", getTrim(Cmb_YearFrom.value));
		fun_ds_srch(ds_srch, "TO"  , getTrim(Cmb_YearTo.value));
	}
	else if (Cmb_TERM.value == '02') //월
	{
		fun_ds_srch(ds_srch, "FROM", getTrim(Cal_SRCH_CNSL_DTM_FROM.value).substr(0,6));
	}
	else if (Cmb_TERM.value == '03') //일
	{
		fun_ds_srch(ds_srch, "FROM", Cal_SRCH_CNSL_DTM_FROM.value);;	
		fun_ds_srch(ds_srch, "TO", Cal_SRCH_CNSL_DTM_TO.value);	
	}

	// 정렬방법
	if (Cmb_GUBUN.value == "02") // 통계구분(업무구분별)
	{
		if ( ChkBoxOrder.value == false )
			fun_ds_srch(ds_srch, "ORDER_GB", "01"); //정렬방법(업무상세별)
		else
			fun_ds_srch(ds_srch, "ORDER_GB", "02"); //정렬방법(업무상세합계역순서)
	}
	
	//통계조회
	var	strSvcID = "R2";
	var	strURL = SVC_USR_URL;
	var	strOutDatasets;
	var	strInDatasets = "SELECT_0_IDATA=ds_srch:a SELECT_1_IDATA=ds_srch:a"; 
	var strArgument;

	if ( Cmb_GUBUN.value == "01" ) //통계구분(조직별)
	{
		if (Cmb_TERM.value == '01') 	// 년도별
		{
			// 통계조회 + 헤더표시용 년도조회
			strArgument = "SELECT_0_ID=STA010.Get_GRP_CNSL_HIST_YEAR_SELECT SELECT_1_ID=STA010.Get_BT_CNSL_HIST_YEAR_HEAD_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_yy_ord=SELECT_0_ODATA ds_bt_cnslr_hist_head=SELECT_1_ODATA";
		}
		else if (Cmb_TERM.value == '02')  // 월별
		{
			strArgument = "SELECT_0_ID=STA010.Get_GRP_CNSL_HIST_YM_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_ym_ord=SELECT_0_ODATA";
		}
		else if (Cmb_TERM.value == '03')  // 일별
		{
			strArgument = "SELECT_0_ID=STA010.Get_GRP_CNSL_HIST_DD_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_dd_ord=SELECT_0_ODATA";
		}
	}
	else if ( Cmb_GUBUN.value == "02" ) //통계구분(업무구분별)
	{
		if (Cmb_TERM.value == '01') 	// 년도별
		{
			// 통계조회 + 헤더표시용 년도조회
			strArgument = "SELECT_0_ID=STA010.Get_ORD_CNSL_HIST_YEAR_SELECT SELECT_1_ID=STA010.Get_BT_CNSL_HIST_YEAR_HEAD_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_yy_ord=SELECT_0_ODATA ds_bt_cnslr_hist_head=SELECT_1_ODATA";
		}
		else if (Cmb_TERM.value == '02')  // 월별
		{
			strArgument = "SELECT_0_ID=STA010.Get_ORD_CNSL_HIST_YM_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_ym_ord=SELECT_0_ODATA";
		}
		else if (Cmb_TERM.value == '03')  // 일별
		{
			strArgument = "SELECT_0_ID=STA010.Get_ORD_CNSL_HIST_DD_SELECT";
			strOutDatasets = "ds_bt_cnslr_hist_dd_ord=SELECT_0_ODATA";
		}
	}
	else
	{
		alert("통계구분값 오류입니다");
		return false;
	}
	
	ds_srch.setColumn(0,"BIZ_PROC_DTLS_CD1","SMTJ");
	trace(ds_srch.saveXML());
	
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}

