{	

	
	var sCONTHIST = 1;
	var sUSRHIST  = 0;
	var sUSRSTTC  = 0;
	var sGRPSTTC  = 0;
	
	var sTime=gds_User.getColumn(gds_User.rowposition,"GBL_TODAY");
	
	
