//Excel 출력
function Btn_E4_onclick(obj:Button,  e:ClickEventInfo) {
	if (Cmb_TERM.value=="01") //년도별
	{ 
		GridExportExcel(Grid_ord_hist_yy, ExportObject00, "sheet1");
	}
	else if (Cmb_TERM.value=="02") //월별
	{ 
