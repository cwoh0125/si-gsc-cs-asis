/*============================================================================*/
/*  메소드 명  : fn_callBack()
/*  내용 설명  : 
/*  RETURN 값  : 
/*============================================================================*/
function fn_callBack(svcid,strErrCode,strErrMsg)
 { 
	var strrtn;
	
 	if (svcid == "R1") //파트
 	{
 	}
	else if (svcid == "R2") //통계조회
	{
		if (Cmb_TERM.value=="01") //기간구분(년별)
		{
			if (ds_bt_cnslr_hist_yy_ord.rowcount == 0)
			{
				cf_Alert("COMI0006", "조회그룹");
				return false;
			}
			else
			{
				//그리드 구분명
				if ( Cmb_GUBUN.value == "01" )
					strrtn = Grid_ord_hist_yy.setCellProperty("head", 0,"text", "그룹명");
					
				else
					strrtn = Grid_ord_hist_yy.setCellProperty("head", 0,"text", "업무구분");
					

				//헤더에 년도표시
				for (var i=1; i<=10; i++)
				{				
					strrtn = Grid_ord_hist_yy.setCellProperty("head", i,"text", ds_bt_cnslr_hist_head.getColumn(0, "YEAR_"+i));
				}
												
				//통계조회 표시
				Grid_ord_hist_yy.visible = true;
			}
		}
		else if (Cmb_TERM.value=="02") //기간구분(월별)
		{
			if (ds_bt_cnslr_hist_ym_ord.rowcount == 0)
			{
				cf_Alert("COMI0006", "조회그룹");
				return false;
			}
			else
			{
				//그리드 구분명
				if ( Cmb_GUBUN.value == "01" )
					strrtn = Grid_ord_hist_ym.setCellProperty("head", 0,"text", "그룹명");
				else
					strrtn = Grid_ord_hist_ym.setCellProperty("head", 0,"text", "업무구분");

				Grid_ord_hist_ym.visible=true;
			}
		}
		else if (Cmb_TERM.value=="03") //기간구분(일별)
		{
			if (ds_bt_cnslr_hist_dd_ord.rowcount == 0)
			{
				cf_Alert("COMI0006", "조회그룹");
				return false;
			}
			else
			{
				//그리드 구분명
				if ( Cmb_GUBUN.value == "01" )
					strrtn = Grid_ord_hist_dd.setCellProperty("head", 0,"text", "그룹명");
				else
					strrtn = Grid_ord_hist_dd.setCellProperty("head", 0,"text", "업무구분");


				//기간구분-> 일별(종료일 선택 추가) 
				//선택한 종료일까지 동적 컬럼으로 보이도록 처리 (보여지는 일수는 31일까지 제한)
				Grid_ord_hist_dd.visible=true;
				
				//선택한 일자 추가
			    Grid_ord_hist_dd.enableredraw = false;
				
				//추가했던 동적 컬럼 삭제
				var cellCnt = Grid_ord_hist_dd.getCellCount('body');

				if(cellCnt > 3) 
				{								
					for(var cc = 1; cc <= cellCnt-3; cc++)
					{					
						Grid_ord_hist_dd.deleteContentsCol(1);
					}				
				}
				
				var strDate = Cal_SRCH_CNSL_DTM_FROM.value.toFormatString("%Y%m%d", "ko_KR");
				var endDate = Cal_SRCH_CNSL_DTM_TO.value.toFormatString("%Y%m%d", "ko_KR");
				
				var x = DiffDate(strDate, endDate);
				//var y = AddDate(strDate, 1);
				
				var dd = "";				
				//var columnsIds = new Array();				
								
				var sd = strDate.substring(6,8).replace(/(^0+)/,"");
				var ed = endDate.substring(6,8).replace(/(^0+)/,"");

				for(var i = 0; i <= x; i++)
				{										
					if(i == 0) dd = strDate.substring(6,8); 
					else dd = AddDate(strDate,i).substring(6,8);
											
					//columnsIds.push("CNSL_DT_"+dd);		

					var n_i =  Number(i+1);
					//헤더에 일자표시 
					Grid_ord_hist_dd.insertContentsCol(n_i);
					Grid_ord_hist_dd.setFormatColProperty(n_i,"size",70);
					Grid_ord_hist_dd.setCellProperty("head",n_i, "text",  dd.replace(/(^0+)/,"")+ ' 일'); 					
					
					Grid_ord_hist_dd.setCellProperty("body", n_i, "displaytype", "number");
					Grid_ord_hist_dd.setCellProperty("body", n_i, "edittype", "none");
										
					Grid_ord_hist_dd.enableredraw = true;				

					var columnId = "CNSL_DT_"+dd; 
					var colLen = ds_bt_cnslr_hist_dd_ord.getColCount();					
					for(var ds = 0; ds < colLen; ds++)
					{	
						var colId = ds_bt_cnslr_hist_dd_ord.getColID(ds);					
						if(columnId == colId)
						{
							var colinfo = ds_bt_cnslr_hist_dd_ord.getColumnInfo(ds);
							Grid_ord_hist_dd.setCellProperty("body", n_i, 'text', "bind:"+colinfo.name);							
						}					
					}

					Grid_ord_hist_dd.setCellProperty("body", n_i, "background", "EXPR(getColor1(ds_bt_cnslr_hist_dd_ord.getColumn(currow,'DATA_GB')))");	
					Grid_ord_hist_dd.setCellProperty("body", n_i, "background2", "EXPR(getColor2(ds_bt_cnslr_hist_dd_ord.getColumn(currow,'DATA_GB')))");	
					
				 }	
				
				Grid_ord_hist_dd.enableredraw = true;
				//일별 동적컬럼 처리 끝				
			}								
		}
		else if (svcid == "B1")
		{
		alert("접촉이력 실시간 집계 작업완료");
		}
	}
}

function Cmb_GUBUN_onitemchanged(obj:Combo, e:ItemChangeEventInfo) {
	if( Cmb_GUBUN.value == "01" ) //조직별
	{
		Cmb_SCH_GRP_CD.enable = true;
