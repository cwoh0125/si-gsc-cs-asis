	}
	else if (Cmb_TERM.value=="03") //일별
	{ 
		GridExportExcel(Grid_ord_hist_dd, ExportObject00, "sheet1");
	}
}

//구분명 백그라운드 color
function getColor1(dataGb)
{
	if ( dataGb == "S" )	//소계
		return "lightgoldenrodyellow";
	else if ( dataGb == "T" ) //합계
		return "silver";
	else
		return "white"; //mediumaquamarine
}
//데이터부  백그라운드 color
function getColor2(dataGb)
{
	if ( dataGb == "S" )	//소계
		return "lightgoldenrodyellow";
	else if ( dataGb == "T" ) //합계
		return "silver";
	else
		return "white";
}
function Cmb_TERM_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if ( Cmb_TERM.value == "01" ) //년도별 선택시만 보여줌
	{
		Cal_SRCH_CNSL_DTM_FROM.visible = false;
		Cal_SRCH_CNSL_DTM_TO.visible = false;
		
		Cmb_YearFrom.value  = getToday().substring(0,4);
		Cmb_YearTo.value    = getToday().substring(0,4);

		Cmb_YearFrom.visible = true;
		txtFromTo.visible = true;
		Cmb_YearTo.visible = true;

	}
	else if( Cmb_TERM.value == "03" ) //일별 선택시만 보여줌
	{
		Cal_SRCH_CNSL_DTM_FROM.visible = true;
		Cal_SRCH_CNSL_DTM_TO.visible = true;
		
		txtFromTo.visible = true;
		Cmb_YearFrom.visible = false;
		Cmb_YearTo.visible = false;
	}
	else
	{
		Cal_SRCH_CNSL_DTM_FROM.visible = true;
		Cal_SRCH_CNSL_DTM_TO.visible = false;

		txtFromTo.visible = false;
		Cmb_YearFrom.visible = false;
		Cmb_YearTo.visible = false;
	}
}

