}

/*
	화면의 기본값설정 함수
*/
function setDefaultValue(){

	//공통콤보세팅	
    var Combo           = ["Cmb_USE_YN1", "Cmb_QA_USAG_DV_CD","Cmb_SCH_QA_USAG_DV_CD","Cmb_FIXED_COL"];    //콤보ID
    var SearchKey       = ["NW012", "NW014", "NW014", "NW042"];               						  //공통코드-대분류코드
    var ListMode        = [ "TN", "TY", "TN", "TY"];                    								  //CTN : [코드] 코드명  , TY : 코드명,  TN: 초기값무시
    var OptionRowValue  = [ ",", ",", ",", ","];                  										  //코드상단에 로우추가후 전체, 공백

	setComboList(Combo, SearchKey, ListMode, OptionRowValue);     										  //공통콤보가져오기
	
	//사용자콤보생성
	set_user_code();	
}

/*
	1단계 조회
*/
function Btn_R1_onclick(obj:Button,  e:ClickEventInfo)
{
	if (cf_isNull(Cmb_SCH_GRP_CD.value)) {
		cf_Alert("COME0003", "조회그룹");  //msgCont 은(는) 필수입니다.
		Cmb_SCH_GRP_CD.setFocus();
		return false;
	}


	//데이터초기화
	ds_srch.clearData();
	ds_qa1list.clearData();
	ds_qa2list.clearData();
	ds_qa3list.clearData();
	ds_qa4list.clearData();
	
	//조회조건입력
	fun_ds_srch(ds_srch, "GRP_CD", Cmb_SCH_GRP_CD.value);		
	fun_ds_srch(ds_srch, "BIZ_BASC_CD", "I");
	fun_ds_srch(ds_srch, "QA_USAG_DV_CD", Cmb_SCH_QA_USAG_DV_CD.value);
	
	
	//거래시작	
	var	strSvcID = "R1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_qa1list=SELECT_0_ODATA";
	var	strArgument = "SELECT_0_ID=QAS010.Get_QaAsesInfoGrpList_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);		
		
}

/*
	콜백함수
*/
function fn_callBack(svcid,strErrCode,strErrMsg)
{	
	//조회건수표시
	Stt_SrchCnt.text = ds_qa1list.rowcount;
	Stt_SrchCnt2.text = ds_qa2list.rowcount;
	Stt_SrchCnt3.text = ds_qa3list.rowcount;
	Stt_SrchCnt4.text = ds_qa4list.rowcount;
	
	if (svcid =="R1"){	
		if (ds_qa1list.rowcount < 1){
			cf_Alert("COMI0006");
		}else{			
			Grd_qa1list_oncellclick();
		}
	}else if (svcid =="R2"){			
		if (ds_qa2list.rowcount > 0){
			Grd_qa2list_oncellclick();
		}
	}else if (svcid =="R3"){			
		if (ds_qa3list.rowcount > 0){
			Grd_qa3list_oncellclick();
		}	
	}else if (svcid =="R4"){	
	}else if (svcid =="U1" || svcid =="U2" || svcid =="U3" || svcid =="U4"){		
		cf_Alert("COMI0003");
	}else if (svcid =="D1"){
		cf_Alert("COMI0004");
		if (ds_qa1list.rowcount > 0){
			Grd_qa1list_oncellclick();
		}		
	}else if (svcid =="D2"){
		cf_Alert("COMI0004");
		if (ds_qa2list.rowcount > 0){
			Grd_qa2list_oncellclick();
		}		
	}else if (svcid =="D3"){
		cf_Alert("COMI0004");
		if (ds_qa3list.rowcount > 0){
			Grd_qa3list_oncellclick();
		}		
	}else if (svcid =="D4"){
		cf_Alert("COMI0004");
	}else if (svcid =="C5"){			
		cf_Alert("QASI0006");
		if (ds_qa1list.rowcount > 0){
			Grd_qa1list_oncellclick();
		}
	}
}

