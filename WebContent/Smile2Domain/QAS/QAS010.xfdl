/*
	1단계 저장버튼 클릭시
*/
function Btn_U1_onclick(obj:Button,  e:ClickEventInfo)
{

	//필수체크
	var cnt=0;
	
	var useYnCnt = 0;

	for (var i=0; i < ds_qa1list.rowcount; i++){
		
		if (ds_qa1list.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_qa1list.getRowType(i) == Dataset.ROWTYPE_UPDATE){
		

			if(!cf_chk_data(ds_qa1list.getColumn(i,"GRP_CD"),"그룹")) {				
				ds_qa1list.rowposition = i;				
				Cmb_GRP_CD1.setFocus();
				return false;
			}
			
			if(!cf_chk_data(ds_qa1list.getColumn(i,"TITE"),"제목")) {				
				ds_qa1list.rowposition = i;				
				Edt_TITE1.setFocus();
				return false;
			}

			if(!cf_chk_data(ds_qa1list.getColumn(i,"QA_USAG_DV_CD"),"용도")) {				
				ds_qa1list.rowposition = i;				
				Cmb_QA_USAG_DV_CD.setFocus();
				return false;
			}
	
			if(!cf_size_Alert(ds_qa1list, i , "TITE" , Edt_TITE1 , 50)) return;
			
			cnt++;
		}				
		
		if(ds_qa1list.getColumn(i,"USE_YN") == "Y"){
			useYnCnt++;
		}
	}

	if(cnt < 1){		
		cf_Alert("COMI0007");
		return;
	}
	
	if(useYnCnt > 1){
		cf_Alert("COME0031");
		return;
	}
	
	
	
	ds_srch.clearData();
	fun_ds_srch(ds_srch, "GRP_CD"		, Cmb_SCH_GRP_CD.value);		
	fun_ds_srch(ds_srch, "BIZ_BASC_CD"	, "I");
	fun_ds_srch(ds_srch, "QA_USAG_DV_CD", Cmb_SCH_QA_USAG_DV_CD.value);
	
	//거래시작	
	var	strSvcID 		= "U1";
	var	strURL 			= SVC_URL;
	var	strInDatasets 	= "COMSAVE_0_IDATA=ds_qa1list:u COMSRCH_1_IDATA=ds_srch:a"; 
	var	strOutDatasets 	= "ds_qa1list=COMSRCH_1_ODATA";
	var	strArgument 	= "COMSAVE_0_ID=QAS010.Set_QaAsesInfoGrpInf COMSRCH_1_ID=QAS010.Get_QaAsesInfoGrpList_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar 	= true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);				
}


