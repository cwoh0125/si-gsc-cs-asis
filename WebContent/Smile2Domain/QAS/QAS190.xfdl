/*
	콜백함수
*/
function fn_callBack(svcid,strErrCode,strErrMsg) {
	Stt_SrchCnt.text = ds_self_check.rowcount;
	Stt_SrchErrCnt.text = ds_cnsl.rowcount;

	if (svcid == "R1"){ //파트		
		if (g_USR_GRD_CD == "E" || g_USR_GRD_CD == "F") {
			Cmb_USR_ID.value = g_USR_ID;
			//Btn_R2_onclick();
			
			Cmb_SCH_GRP_CD.enable = false;
			Cmb_SCH_PT_CD.enable = false;
		} else {
		
			var nRow = ds_user.insertRow(0);
			ds_user.setColumn(nRow, "CODE", "");
			ds_user.setColumn(nRow, "CODE_NM", "전체");
			Cmb_USR_ID.index = 0;
		}
	}
	
	if (svcid == "U1"){		
		cf_Alert("COMI0003");
		Btn_R2_onclick();
	}
	
	if (svcid == "R2"){	
		//4차 5차는 보여 지면 안된다
		//박근영P 20170314
		ds_self_check.filter("SQN==1 || SQN==2 || SQN==3");
		if (g_USR_GRD_CD == "E" || g_USR_GRD_CD == "F") {
			Cmb_USR_ID.enable = false;
		}
		
		for(i=0 ; i < ds_self_check.rowcount ; i++){
		
			//고객명 마스킹
			//ds_self_check.setColumn(i,"CUST_NM",maskCustNm(ds_self_check.getColumn(i,"CUST_NM")));
			
			//전화번호 마스킹
			ds_self_check.setColumn(i,"CUR_TEL_NO",maskPhNo(ds_self_check.getColumn(i,"CUR_TEL_NO")));
		}
		
	}
	
	if (svcid == "R3"){
		
	}
	
	if (svcid == "R4") {
		/*
		if(ds_RLSE_YN.rowcount < 1 || ds_RLSE_YN.getColumn(ds_RLSE_YN.rowposition,"RLSE_YN") == "N"){
			cf_Alert("QASI0020");			
			return;
		}else{
			//fn_openQaEval();
		}
		*/
		setGrdTiteSetting();  //그리드 타이틀세팅	
		fn_srchQaEval();		
	}
	
	if (svcid == "R5") {
		if(g_USR_GRD_CD != "A" && g_USR_GRD_CD != "B" && g_USR_GRD_CD != "L"){
			// 관리자가 공개한 평가만 향상률 보임
			if(ds_RLSE_YN.rowcount < 1 || ds_RLSE_YN.getColumn(ds_RLSE_YN.rowposition,"RLSE_YN") == "N"){
				Edt_ASCR_21.value = "";
				Edt_ASCR_32.value = "";
			}
		}
		
		ds_qa_data.rowposition = 1;
		ds_qa_mast.setColumn(0,"CNSLR_NM",ds_self_check.getColumn(ds_self_check.rowposition,"CNSLR_NM"));		
		getDsQaStartData();  //평가데이터를 입력한다.			
	}
	
	if (svcid == "C1") {
		cf_Alert("COMI0003");	
	}
	
	if (svcid == "U2") {
		cf_Alert("COMI0003");	
	}
}

