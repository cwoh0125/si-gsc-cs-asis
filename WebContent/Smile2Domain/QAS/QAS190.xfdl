	if (g_USR_GRD_CD == "E" || g_USR_GRD_CD == "F") {
//		Cal_SRCH_CNSL_DTM_FROM.readonly = true;
//		Cal_SRCH_CNSL_DTM_TO.readonly = true;
	} else {
		Btn_save.visible = true;
	}
}

/*
	화면의 기본값설정 함수
