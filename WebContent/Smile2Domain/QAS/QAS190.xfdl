function fn_SaveSelfAses(strSaveGbn)
{

	var p_CNSLR_ID = ds_self_check.getColumn(ds_self_check.rowposition,"CNSLR_ID");	
	var nRow = 1;
	
	if (p_CNSLR_ID != g_USR_ID){
		cf_Alert("QASI0021", "본인","저장");	
		return;
	}

	/********************************
		필수체크
	*********************************/	
	
	for (var i=0; i < ds_qa_title.rowcount; i++){
		var smlclas_cdnm = getTrim(ds_qa_title.getColumn(i, "SMLCLAS_CDNM"));
		var code = getTrim(ds_qa_title.getColumn(i, "CODE"));
		if (smlclas_cdnm != ""){
			var val = getTrim(ds_qa_data.getColumn( nRow, "Q" + code));
			if (val == ""){
				cf_Alert("QASI0017", smlclas_cdnm);  //[!@]에 해당하는 점수가 입력되지 않았습니다. 확인해주십시오.
				ds_qa_start.rowposition = i;
				return false; 
			}
		}
	}
	
	if(!cf_chk_data(Txa_ASES_OPNN1,"본인총평")) {
			Txa_ASES_OPNN1.setFocus();
			return false;
		}			
			
	var	strSvcID = "";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = ""; 
	var	strOutDatasets = "";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	var msgId = "";

	/********************************
		평가상세 세팅
	*********************************/	
	var p_IMG_APND_YN="N";
	
	if (Img_Good.visible){
		p_IMG_APND_YN = "Y"
	}
	
	fun_ds_srch(ds_qa_mast, "IMG_APND_YN",  p_IMG_APND_YN);    		// 참잘했어여 여부
	
	
	//마일리지 점수가 중복으로......
	//self_check는 null로 
	//fun_ds_srch(ds_qa_mast, "IMG_CTT",  	getTrim(Edt_Img_Good.value));    // 참잘했어여 문구	
	fun_ds_srch(ds_qa_mast, "IMG_CTT",  	"");    // 문구
	
	fun_ds_srch(ds_qa_mast, "ASES_OPNN", Txa_ASES_OPNN1.value);    //평가의견- 본인		
	fun_ds_srch(ds_qa_mast, "ASCR",  ds_qa_data.getColumn(nRow,"SUM_SCO"));  //최종점수
	
	var m   = 0;
	var n   = 1;	
	
	for (var i=0; i < ds_qa_title.rowcount; i++){		
		//ds_qa_title.setColumn(i,"ASCR", 	ds_qa_data.getColumn(nRow,"Q"+ (i+1)));
		var col_id = ABC[m] + n;
		ds_qa_title.setColumn(i,"ASCR", 	ds_qa_data.getColumn(nRow,col_id));
		if (n == 10) {
			m++;
			n=0;
		}
		n++;		
		
		if (strSaveGbn == "I"){		
			ds_qa_title.setColumn(i,"CNSLR_ID", ds_self_check.getColumn(ds_self_check.rowposition,"CNSLR_ID"));		
			ds_qa_title.setColumn(i,"ASES_DTM", "");
		}else{
			ds_qa_title.setColumn(i,"CNSLR_ID", ds_qalist.getColumn(ds_qalist.rowposition,"CNSLR_ID"));		
			ds_qa_title.setColumn(i,"ASES_DTM", ds_qalist.getColumn(ds_qalist.rowposition,"ASES_DTM"));		
		}		
		ds_qa_title.setColumn(i,"SQN", 		ds_self_check.getColumn(ds_self_check.rowposition,"SQN"));		
	}
	
	/********************************
		평가상세 세팅- 끝
	*********************************/	
	
	ds_srch_list.clearData();
		
	fun_ds_srch(ds_srch_list, "GRP_CD", Cmb_SCH_GRP_CD.value);
	fun_ds_srch(ds_srch_list, "PART_CD", Cmb_SCH_PT_CD.value);
	fun_ds_srch(ds_srch_list, "CNSLR_ID", Cmb_USR_ID.value);
		
	fun_ds_srch(ds_srch_list, "FROM", getTrim(Cal_SRCH_CNSL_DTM_FROM.value).substr(0,6)); 
	fun_ds_srch(ds_srch_list, "TO", getTrim(Cal_SRCH_CNSL_DTM_TO.value).substr(0,6));
	
	fun_ds_srch(ds_srch_list, "SELF_CHCK_YN", Cmb_SELF_CHCK_YN.value);
	fun_ds_srch(ds_srch_list, "SQN", Cmb_SQN.value);
	fun_ds_srch(ds_srch_list, "USR_GRD_CD", g_USR_GRD_CD);
		
	// 상담사들은 할당받은 건만 조회
	if(g_USR_GRD_CD != "A" && g_USR_GRD_CD != "B" && g_USR_GRD_CD != "L"){
		fun_ds_srch(ds_srch_list, "SELF_CHCK_ASGN_YN", "Y");	
	}else{
		fun_ds_srch(ds_srch_list, "SELF_CHCK_ASGN_YN", "");	
	}
	
	fun_ds_srch(ds_srch_list, "BIZ_BASC_CD"			, g_BIZ_BASC_CD);		

	
	ds_qa_title.filter("SMLCLAS_CD != ''");	
	ds_qa_title_copy.copyData(ds_qa_title, true);
	ds_qa_title.filter("");	
	
	//cf_trace("ds_qa_title.saveXML()", ds_qa_title_copy.saveXML());
	
	//평점은 미리세팅된다.
	fun_ds_srch(ds_qa_mast, "BIZ_BASC_CD",  g_BIZ_BASC_CD);    // 업무 기준 코드    
