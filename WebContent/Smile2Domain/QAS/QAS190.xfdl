// QA평가 링크 - 한 화면에 구성하면서 사용안함
function fn_openQaEval()
{
	//입력데이터입력
	ds_qa_eval_tmp.clearData();
	ds_qa_eval_tmp.copyData(ds_self_check);	// Grid와 연동된 ds_self_check 전체 데이터를 ds_qa_eval_tmp copy

	var objParam = new Object();
	objParam.PGM_ID			= "QAS190";
	objParam.GRP_CD 		= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "GRP_CD");		// 그룹크드
	objParam.BIZ_BASC_CD 	= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "BIZ_BASC_CD");	// 업무기준코드
	objParam.SEQ_NO			= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "SEQ_NO");		// 일련번호
	objParam.ASES_DTM		= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "ASES_DTM");		// 평가일시
	objParam.SQN			= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "SQN");			// 회차
	objParam.CNSLR_ID		= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "CNSLR_ID");		// 상담원 ID
	objParam.CNSL_SEQ_NO	= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "CNSL_SEQ_NO");	// 상담일련번호	
	objParam.ASES_YM		= ds_qa_eval_tmp.getColumn(ds_self_check.rowposition, "ASES_YM");		// 평가년월
	
	if (!getTopFrame().getFormOpenYn("QAS020")) {
		getTopFrame().formOpen("QAS020");   // QA평가 조회 화면
		cf_Alert("COMI0013",getTopFrame().getPgmName("QAS020"));  //!@ 화면이 열려있지 않아서 열어놓았습니다. 다시한번 호출해주세요..
		FS_FRAME_MDI.all("QAS190").openstatus = "maximize";
		getTabFocus("QAS190");
		return;
	}
	getTopFrame().setFormEvent("QAS020",objParam);	
	getTabFocus("QAS020");	
}

// 저장
function Btn_save_onclick(obj:Button,  e:ClickEventInfo) {
	
	var chk = ds_self_check.getColumn(ds_self_check.rowposition,"CHK");
    
    if (ds_self_check.findRow("CHK","1") == "-1"){
	  cf_Alert("COME0006");
	  return;
	}
	
	ds_srch_list.clearData();
	ds_cnsl.clearData();
	
	ds_set.clearData();			
	ds_self_check.filter("CHK == 1");			// 체크된 항목만 filter
	ds_set.copyData(ds_self_check, true);	    // filter된 항목만 ds_set_filtered 에 copy(업데이트 입력값)
	
	fun_ds_srch(ds_srch_list, "GRP_CD", Cmb_SCH_GRP_CD.value);
	fun_ds_srch(ds_srch_list, "PART_CD", Cmb_SCH_PT_CD.value);
	fun_ds_srch(ds_srch_list, "CNSLR_ID", Cmb_USR_ID.value);
	fun_ds_srch(ds_srch_list, "CNSL_DTM_FROM", Cal_SRCH_CNSL_DTM_FROM.value);
	fun_ds_srch(ds_srch_list, "CNSL_DTM_TO", Cal_SRCH_CNSL_DTM_TO.value);
	fun_ds_srch(ds_srch_list, "SELF_CHCK_YN", Cmb_SELF_CHCK_YN.value);
	
	// 상담사들은 할당받은 건만 조회
	if(g_USR_GRD_CD != "A" && g_USR_GRD_CD != "B" && g_USR_GRD_CD != "L"){
		fun_ds_srch(ds_srch_list, "SELF_CHCK_ASGN_YN", "Y");	
	}else{
		fun_ds_srch(ds_srch_list, "SELF_CHCK_ASGN_YN", "");	
	}	
	
	// ★★★★ filter clear
	ds_self_check.filter("");
	// filter clear ★★★★
	
	var	strSvcID = "U1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "UPDATE_0_IDATA=ds_set:a SELECT_1_IDATA=ds_srch_list:a"; 
	var	strOutDatasets = "ds_self_check=SELECT_1_ODATA";
	var	strArgument = "UPDATE_0_ID=QAS190.SetQA_Ases_UPDATE SELECT_1_ID=QAS190.GetSelfChecklList_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;	

	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
	return;	
}

function Grd_self_check_oncellclick(obj:Grid, e:GridClickEventInfo){
	//------------------------------------------------------------------------------------------
	//  녹취청취- 시작
	//------------------------------------------------------------------------------------------
	if(e != null && e.col == 9){
	
	  if(ds_self_check.getColumn(ds_self_check.rowposition,"PH_REC_SEQ_NO") == "") return;	  
	
	  ds_get_param.clearData();
	  ds_set_param.clearData();

	  //GUBUN Default (1) - TOBE , 2 : ASIS
	  var p_GUBUN 			= "1";
	  var p_PH_REC_SEQ_NO 	= getTrim(ds_self_check.getColumn(ds_self_check.rowposition,"PH_REC_SEQ_NO"));
	  var p_CNSL_DT 		= getTrim(ds_self_check.getColumn(ds_self_check.rowposition,"CNSL_DTM").substr(0,8));
	  var p_EXT_NO 			= getTrim(ds_self_check.getColumn(ds_self_check.rowposition,"EXT_NO"));
	  var p_USR_ID 			= getTrim(ds_self_check.getColumn(ds_self_check.rowposition,"REG_ID"));
	  
	  if (p_CNSL_DT < GetConfig("AS_IS_DT")){
	     p_GUBUN = "2";
	     //p_USR_ID = "70186";	     //as-is 테스트
	     //p_CNSL_DT = "20101111";   //as-is 테스트
	  }
	  
	  fun_ds_srch(ds_get_param, "GUBUN", 			p_GUBUN);
