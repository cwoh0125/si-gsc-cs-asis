function Div04_Tab_Work_tabpage1_Grd_Undo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_call.getColumn(ds_call.rowposition,"BIZ_DV_CD") == "C")
	{
		//상담관리
		if (!getTopFrame().getFormOpenYn("BNS010")) {
		    //if(!getTopFrame().formOpen("BNS010")) return;   //상담화면 프로그램 권한이 있는 경우 열린다.
			cf_Alert("COMI0013","보너스카드 - 상담관리");  //!@ 화면이 열려있지 않아서 열어놓았습니다. 다시한번 호출해주세요..
	    	//FS_FRAME_MDI.all("COM030").openstatus = "maximize";		
			//getTabFocus("COM030");
			//getTabFocus("HOME");
			return;
		}

		var objParam 		= new Object();
		objParam.custId 	= ds_call.getColumn(ds_call.rowposition,"CUST_ID");
		objParam.rsvSeqNo 	= ds_call.getColumn(ds_call.rowposition,"RSV_SEQ_NO");	
		objParam.frmPgmId 	= "COM030_tabpage1";

		getTopFrame().setFormEvent("BNS010",objParam);	
		getTabFocus("BNS010");
		
	}
	else
	{
		//주문관리	
		ds_get_param.clearData();
		fun_ds_srch(ds_get_param, "CUST_ID", ds_call.getColumn(ds_call.rowposition,"CUST_ID"));		
		fun_ds_srch(ds_get_param, "RSV_SEQ_NO", ds_call.getColumn(ds_call.rowposition,"RSV_SEQ_NO"));	
		fun_ds_srch(ds_get_param, "frmPgmId", "COM030_tabpage1");	

		var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};

		if(FS_POPUP_ORD010 != null) return;
		application.mainframe.position.height = 135;
		var nLeft = application.mainframe.position.left;
		var nTop = application.mainframe.position.top + application.mainframe.position.height;
		getTopFrame().COMPopup("ORD010", "ORD::ORD010.xfdl", "", nLeft, nTop, 660, 830, false, "", arrParam , true, false, false, false);
	 }
	          
}


