function Div03_Tab_Noti_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{      
	if (Div03.Tab_Noti.tabindex == 0){
		 //버튼활성화하기
		 if(ds_A_Noti.rowcount > cnt){
			Btn_More.visible = true;
		 }else{
			Btn_More.visible = false;
		 }         
	}else if (Div03.Tab_Noti.tabindex == 1){
		 //버튼활성화하기
		 if(ds_W_Noti.rowcount > cnt){
			Btn_More.visible = true;
		 }else{
			Btn_More.visible = false;
		 }	
	}else{
		 //버튼활성화하기
		 if(ds_N_Noti.rowcount > cnt){
			Btn_More.visible = true;
		 }else{
			Btn_More.visible = false;
		 }	
	}		
	
}





//------------------------------------------------------------------------------------------
//  callback
//------------------------------------------------------------------------------------------
function fn_callBack(svcid,strErrCode,strErrMsg)
{

    if(svcid == "R1")
    {      
    
       if(ds_cnt.rowcount == 0 )
       {
         Div03.Stt_Cnt.text = "0";
       }
       else 
       {
         Div03.Stt_Cnt.text = ds_cnt.getColumn(0,"CNT"); 
     
         //버튼활성화하기
		 if(ds_A_Noti.rowcount > cnt){
			Btn_More.visible = true;
			A_T_cnt = ds_A_Noti.rowcount - 1 ;
		 }else{
			Btn_More.visible = false;
			A_T_cnt = ds_A_Noti.rowcount ;
		 }  
		 
		 
		 //공지사항-전체공지를 조회한다.[시작]===================================                          
         for (var i=0; i< A_T_cnt; i++){
			var A_nRow = ds_A_Noti_Empty.addRow();
			ds_A_Noti_Empty.copyRow(A_nRow,ds_A_Noti, i); 
         }        
	     A_str_inx = end_inx;
	     
		 if(ds_W_Noti.rowcount > cnt){
			W_T_cnt = ds_W_Noti.rowcount - 1 ;
		 }else{
			W_T_cnt = ds_W_Noti.rowcount;
		}
		 //공지사항-업무공지를 조회한다.[시작]===================================         
         for (var i=0; i< W_T_cnt ; i++){
			var W_nRow = ds_W_Noti_Empty.addRow();
			ds_W_Noti_Empty.copyRow(W_nRow,ds_W_Noti, i);  
         }   
         W_str_inx = end_inx;
         
		 if(ds_N_Noti.rowcount > cnt){
			N_T_cnt = ds_N_Noti.rowcount - 1 ;
		 }else{
			N_T_cnt = ds_N_Noti.rowcount;
		 }
		  //공지사항-일반공지를 조회한다.[시작]===================================         
         for (var i=0; i< N_T_cnt ; i++){
			var N_nRow = ds_N_Noti_Empty.addRow();
			ds_N_Noti_Empty.copyRow(N_nRow,ds_N_Noti, i);   
         } 
         N_str_inx = end_inx;
             
         
          //예약리스트 전화번호 mask
          for (var i=0; i < ds_call.rowcount; i++){       
             ds_call.setColumn(i,"CTFC",maskPhone(ds_call.getColumn(i,"CTFC")));
          }
        
          Div04.Tab_Work.tabpage1.text = "예약리스트(" + ds_call.rowcount + ")";
          Div04.Tab_Work.tabpage2.text = "메일(" + ds_mail.rowcount + ")";
          Div04.Tab_Work.tabpage3.text = "설문(" + ds_qust.rowcount + ")";
          Div04.Tab_Work.tabpage4.text = "건의사항(" + ds_props.rowcount + ")";
          Div04.Tab_Work.tabpage5.text = "불만(" + ds_dsft.rowcount + ")";
       }
      
    }
    else if (svcid == "R2")
    {
       Div03.Stt_Cnt.text = ds_cnt.getColumn(0,"CNT"); 
       
		 if(Div03.Tab_Noti.tabindex == 0 )
		 {
			 //버튼활성화하기
			 if(ds_A_Noti.rowcount <= cnt){
				Btn_More.visible = true;
				A_T_cnt = ds_A_Noti.rowcount ;
			 }else{
				Btn_More.visible = false;
				A_T_cnt = ds_A_Noti.rowcount - 1 ;
			 }  
			 			 
			 //공지사항-전체공지를 조회한다.[시작]===================================                          
			 for (var i=0; i< A_T_cnt; i++){
				var A_nRow = ds_A_Noti_Empty.addRow();
				ds_A_Noti_Empty.copyRow(A_nRow,ds_A_Noti, i); 
			 } 
			A_str_inx = A_end_inx;
			 
		  } 
		  else if (Div03.Tab_Noti.tabindex == 1 )
		  {
			 if(ds_W_Noti.rowcount <= cnt){
			    Btn_More.visible = true;
				W_T_cnt = ds_W_Noti.rowcount;
			 }else{
			    Btn_More.visible = false;
				W_T_cnt = ds_W_Noti.rowcount - 1 ;
			}
			 //공지사항-업무공지를 조회한다.[시작]===================================         
			 for (var i=0; i< W_T_cnt ; i++){
				var W_nRow = ds_W_Noti_Empty.addRow();
				ds_W_Noti_Empty.copyRow(W_nRow,ds_W_Noti, i);  
			 }   
			 W_str_inx = W_end_inx; 
		  }
		  else
		  {
			 if(ds_N_Noti.rowcount <= cnt){
			    Btn_More.visible = true;
				N_T_cnt = ds_N_Noti.rowcount;
			 }else{
			    Btn_More.visible = false;
				N_T_cnt = ds_N_Noti.rowcount - 1 ;
			 }
			  //공지사항-일반공지를 조회한다.[시작]===================================         
			 for (var i=0; i< N_T_cnt ; i++){
				var N_nRow = ds_N_Noti_Empty.addRow();
				ds_N_Noti_Empty.copyRow(N_nRow,ds_N_Noti, i);   
			 }
             N_str_inx = N_end_inx;		     			 
		   }  
       Div03_Tab_Noti_onchanged();
    }
    
	
}


