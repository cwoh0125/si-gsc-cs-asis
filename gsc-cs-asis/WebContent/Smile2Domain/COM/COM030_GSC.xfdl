function getPopup(seq,ncnt){

	ds_get_param.clearData();
	
	fun_ds_srch(ds_get_param, "pgmid", "COM030");		
	fun_ds_srch(ds_get_param, "SEQ_NO", seq);		
	fun_ds_srch(ds_get_param, "INQ_NCNT", ncnt);	

	var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};

	PopupDialog("COM040", "COM::COM040.xfdl", -1, -1, 770, 300, false, -1, arrParam, false);	
	
	ds_A_Noti_Empty.setColumn(ds_A_Noti_Empty.rowposition,"INQ_NCNT",toNumber(ds_A_Noti_Empty.getColumn(ds_A_Noti_Empty.rowposition,"INQ_NCNT")) + 1);
	ds_W_Noti_Empty.setColumn(ds_W_Noti_Empty.rowposition,"INQ_NCNT",toNumber(ds_W_Noti_Empty.getColumn(ds_W_Noti_Empty.rowposition,"INQ_NCNT")) + 1);
	ds_N_Noti_Empty.setColumn(ds_N_Noti_Empty.rowposition,"INQ_NCNT",toNumber(ds_N_Noti_Empty.getColumn(ds_N_Noti_Empty.rowposition,"INQ_NCNT")) + 1);
	
	var cnt;
	
	if(ds_A_Noti_Empty.getColumn(ds_A_Noti_Empty.rowposition,"CNT") == "Y")
	{
	  cnt = toNumber(ds_cnt.getColumn(0,"CNT")) - 1;
	}
	else if(ds_W_Noti_Empty.getColumn(ds_W_Noti_Empty.rowposition,"CNT") == "Y")
	{
	  cnt = toNumber(ds_cnt.getColumn(0,"CNT")) - 1;
	}
	else if(ds_N_Noti_Empty.getColumn(ds_N_Noti_Empty.rowposition,"CNT") == "Y")
	{
	  cnt = toNumber(ds_cnt.getColumn(0,"CNT")) - 1;	
	}
	else 
	{
	  cnt = ds_cnt.getColumn(0,"CNT");
	}
	ds_cnt.setColumn(0,"CNT",cnt);
	
	Div03.Stt_Cnt.text = ds_cnt.getColumn(0,"CNT");	
	
	
	
}


