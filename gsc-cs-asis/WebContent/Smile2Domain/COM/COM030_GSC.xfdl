function setSchedule(){
    
    ds_Schedule.clearData();
    ds_M_Schedule.clearData();
    ds_A_Noti_Empty.clearData();
    ds_W_Noti_Empty.clearData();
    ds_N_Noti_Empty.clearData();
	ds_A_Noti.clearData();
	ds_W_Noti.clearData();
	ds_N_Noti.clearData();
    
    str_inx = str_inx;
    end_inx = str_inx + cnt ;
        
    fun_ds_srch(ds_srch, "STR_INX", str_inx);	
    fun_ds_srch(ds_srch, "END_INX", end_inx);	
    
    if(gds_User.getColumn(0, "GBL_GRP_CD") != "ORD")
    {
		fun_ds_srch(ds_srch, "GRP_CD", "1");	//상담일반그룹
	}
	else
	{ 
	   fun_ds_srch(ds_srch, "GRP_CD", "2");     //주문그룹
	}
    
	var	strSvcID = "R1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_srch:a SELECT_1_IDATA=ds_srch:a SELECT_2_IDATA=ds_srch:a SELECT_3_IDATA=ds_srch:a SELECT_4_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_Schedule=SELECT_0_ODATA ds_M_Schedule=SELECT_1_ODATA ds_A_Noti=SELECT_2_ODATA ds_W_Noti=SELECT_3_ODATA ds_N_Noti=SELECT_4_ODATA ds_cnt=SELECT_5_ODATA ds_props=SELECT_6_ODATA ds_mail=SELECT_7_ODATA ds_qust=SELECT_8_ODATA ds_call=SELECT_9_ODATA ds_dsft=SELECT_10_ODATA ds_birth=SELECT_11_ODATA";
    var	strArgument = "SELECT_0_ID=COM030.GetSchedule_SELECT SELECT_1_ID=COM030.GetMSchedule_SELECT SELECT_2_ID=COM030.GetAllNotice_SELECT SELECT_3_ID=COM030.GetWNotice_SELECT SELECT_4_ID=COM030.GetNNotice_SELECT SELECT_5_ID=COM030.GetUnChkCnt_SELECT SELECT_6_ID=COM030.GetPropsList_SELECT SELECT_7_ID=COM030.GetMailList_SELECT SELECT_8_ID=COM030.GetQustList_SELECT SELECT_9_ID=COM030.GetCallRsv_SELECT SELECT_10_ID=COM030.Get_CMP_List_SELECT SELECT_11_ID=COM030.Get_BirthList_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
}



