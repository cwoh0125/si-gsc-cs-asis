/************************************************************************************************************************
***********************************************업무구분관리 관련함수
************************************************************************************************************************/
function fn_setBzReadOnly()
{
	if(ds_bz_proc_ct.getColumn(ds_bz_proc_ct.rowposition,"SAVED_YN") == "Y"){
		Cmb_BzGbn.readonly		= true;
		Edt_BzDtlCd_1.readonly	= true;
		Edt_BzDtlCd_2.readonly	= true;
	}else{
		Cmb_BzGbn.readonly		= false;
		Edt_BzDtlCd_1.readonly	= false;
		Edt_BzDtlCd_2.readonly	= false;	
	}
}

function fn_chkBzValid()
{
	var blnValid = true;
					
	if(!(blnValid = cf_chk_data(Cmb_BzGbn,"업무구분")))					
			return blnValid;
	if(!(blnValid = cf_chk_data(Edt_BzDtlCd_1,"업무상세코드1")))
			return blnValid;
	if(!(blnValid = cf_chk_data(Edt_BzDtlNm_1,"업무상세명1")))
			return blnValid;
	if(!(blnValid = cf_chk_data(Edt_BzDtlCd_2,"업무상세코드2")))
			return blnValid;
	if(!(blnValid = cf_chk_data(Edt_BzDtlNm_2,"업무상세명2")))
			return blnValid;
	if(!(blnValid = cf_chk_data(Cmb_BzUse_Yn,"사용여부")))
			return blnValid;										
					
	return blnValid;
	
}

// 조회조건 업무구분 선택변경
function Cmb_SelBzGbn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	// 업무상세1 필터링
	var ds_bz_dtl1 = eval(Cmb_SelBzDtl_1.innerdataset);
	ds_bz_dtl1.filter("HIG_CODE =='" + Cmb_SelBzGbn.value + "' ");
	
	// 전체로 세팅
	ds_bz_dtl1.insertRow(0);							
	ds_bz_dtl1.setColumn(0,"CODE","");	
	ds_bz_dtl1.setColumn(0,"CODE_NM","전체");
	Cmb_SelBzDtl_1.value = "";
	
	ds_srch_bz_dtl2.clearData();	
	ds_list_bz_dtl2.clearData();	
	fun_ds_srch(ds_srch_bz_dtl2, "BIZ_DV_CD", getTrim(Cmb_SelBzGbn.value));
	fun_ds_srch(ds_srch_bz_dtl2, "BIZ_PROC_DTLS_CD1", getTrim(Cmb_SelBzDtl_1.value));	
	
	//거래시작	
	var	strSvcID = "R5";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_srch_bz_dtl2:a"; 
	var	strOutDatasets = "ds_list_bz_dtl2=SELECT_0_ODATA";
	var	strArgument = "SELECT_0_ID=SYS001.Get_CmbBizDtls2_SELECT";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = false;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);					
}

