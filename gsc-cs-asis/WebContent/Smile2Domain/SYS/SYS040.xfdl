/************************************************************************************************************************
********************************************기준구분관리 수정-저장    
************************************************************************************************************************/
function Btn_sd_save_onclick(obj:Button,  e:ClickEventInfo)
{	
	for (var i=0; i < ds_sd_dv_ct.rowcount; i++){		
		if (ds_sd_dv_ct.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_sd_dv_ct.getRowType(i) == Dataset.ROWTYPE_UPDATE){
			
			//필수 체크
			if(!cf_chk_data(ds_sd_dv_ct.getColumn(i,"BSVL_CD"),"기준구분코드")){
				ds_sd_dv_ct.rowposition = i;				
				Edt_sd_cd_v.setFocus();
				return false;
			}
			
			if(!cf_chk_data(ds_sd_dv_ct.getColumn(i,"BSVL_CD_TITE"),"기준구분명")){
				ds_sd_dv_ct.rowposition = i;				
				Edt_sd_nm_v.setFocus();
				return false;
			}
			
			if(!cf_chk_data(ds_sd_dv_ct.getColumn(i,"BSVL_CD_CTT"),"기준값")){
				ds_sd_dv_ct.rowposition = i;				
				Edt_sd_val_v.setFocus();
				return false;
			}			
		}
	}
	
	if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?
	
	//조회값
	//fun_ds_srch(ds_sd_sel, "BSVL_CD", Edt_sd_cd.value);
	//fun_ds_srch(ds_sd_sel, "BSVL_CD_TITE", Edt_sd_nm.value);
	
	var	strSvcID = "C2";
	var	strURL = SVC_URL;
	
	
	var	strInDatasets = "COMSAVE_0_IDATA=ds_sd_dv_ct:u"; 
	var	strOutDatasets = "ds_sd_dv_ct=COMSRCH_1_ODATA";
	var	strArgument = "COMSAVE_0_ID=SYS001.Set_sd_List";

	var strCallbackFunc = "fn_callBack_sd_save";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);

	return;
}
function fn_callBack_sd_save(){
	cf_Alert("COMI0003"); //저장되었습니다.
	Edt_sd_cd_v.readonly = true;
	Btn_sd_sel_onclick();
}

