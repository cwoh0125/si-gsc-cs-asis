// 업무상세2 조회 후 처리
function fn_setCmbBizDtl2()
{
	// 업무구분세팅
	if(getTrim(Cmb_SelBzDtl_1.value) != ""){	
		var ds_biz_dtls1 = eval(Cmb_SelBzDtl_1.innerdataset);
		var intRow = ds_biz_dtls1.findRow("CODE",getTrim(Cmb_SelBzDtl_1.value));
		Cmb_SelBzGbn.value = ds_biz_dtls1.getColumn(intRow,"HIG_CODE");
	}
		
	// 업무상세2 콤보세팅
	ds_list_bz_dtl2.insertRow(0);							
	ds_list_bz_dtl2.setColumn(0,"CODE","");	
	ds_list_bz_dtl2.setColumn(0,"CODE_NM","전체");
	
	Cmb_SelBzDtl_2.value = "";

}


