/************************************************************************************************************************
***********************************************업무구분관리 신규버튼클릭
************************************************************************************************************************/
function Btn_bz_new_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_bz_proc_ct.rowcount > 0 && ds_bz_proc_ct.getColumn(ds_bz_proc_ct.rowposition,"SAVED_YN")=="N"){
		if(!fn_chkBzValid())	return;
	}
	
	var intRow = ds_bz_proc_ct.addRow();
	ds_bz_proc_ct.setColumn(intRow,"SAVED_YN","N");	
	
	//  default 세팅
	if(getTrim(Cmb_SelBzGbn.value) != ""){
		Cmb_BzGbn.value = Cmb_SelBzGbn.value;
		ds_bz_proc_ct.setColumn(intRow,"BIZ_DV_CDNM",Cmb_SelBzGbn.text);
	}

	if(getTrim(Cmb_SelBzDtl_1.value) != ""){
		Edt_BzDtlCd_1.value	= Cmb_SelBzDtl_1.value;
		Edt_BzDtlNm_1.value = Cmb_SelBzDtl_1.text;
	}
		
	Cmb_BzUse_Yn.value = "Y";
	
	fn_setBzReadOnly();	
}

