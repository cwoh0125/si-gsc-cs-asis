	}else{
		lm_use = 1;
	}
}

/************************************************************************************************************************
********************************************비밀번호 변경
************************************************************************************************************************/
function change_save(){
	//변환된 콤보값
	var val_index	= Cmb_pw_val_v.index//3
	var ch_index	= Cmb_pw_ch_v.index//5
	var min_index	= Cmb_pw_min_v.index//1
	var max_index	= Cmb_pw_max_v.index//2	
	var lm_index	= Cmb_pw_lm_v.index//6
	
	//변환된 에디트값
	var min_val		= Edt_pw_min_v.value;
	var max_val		= Edt_pw_max_v.value;
	var ch_val		= Edt_pw_ch_v.value;	
	var lm_val		= Edt_pw_lm_v.value;
	
	//사용자 ID와의 일치여부
	if(val_use != val_index){
		var rVal = ds_pw_cf.findRow( "CHCK_CD", "3" );
		if(val_index == 0){
			colVal = 'Y';
		}else{
			colVal = 'N';
		}
		ds_pw_cf.setColumn(rVal,'USE_YN',colVal);
		ds_pw_cf.setColumn(rVal,'CPHR', 'N');
	}
	
	//비밀번호 변경주기
	if(ch_use != ch_index || ch_v != ch_val){
		var rVal = ds_pw_cf.findRow( "CHCK_CD", "5" );
		if(ch_index == 0){
			colVal = 'Y';
		}else{
			colVal = 'N';
		}
		ds_pw_cf.setColumn(rVal,'USE_YN',colVal);
		ds_pw_cf.setColumn(rVal,'CPHR', Edt_pw_ch_v.value);
	}
	
	//비밀번호 최소자리
	if(min_use != min_index || min_v != min_val){
		var rVal = ds_pw_cf.findRow( "CHCK_CD", "1" );
		if(min_index == 0){
			colVal = 'Y';
		}else{
			colVal = 'N';
		}
		ds_pw_cf.setColumn(rVal,'USE_YN',colVal);
		ds_pw_cf.setColumn(rVal,'CPHR',Edt_pw_min_v.value);
	}
	
	//비밀번호 최대자리
	if(max_use != max_index || max_v != max_val){
		var rVal = ds_pw_cf.findRow( "CHCK_CD", "2" );
		if(max_index == 0){
			colVal = 'Y';
		}else{
			colVal = 'N';
		}
		ds_pw_cf.setColumn(rVal,'USE_YN',colVal);
		ds_pw_cf.setColumn(rVal,'CPHR',Edt_pw_max_v.value);
	}	
	
	//로그인 제한횟수
	if(lm_use != lm_index || lm_v != lm_val){
		var rVal = ds_pw_cf.findRow( "CHCK_CD", "6" );
		if(lm_index == 0){
			colVal = 'Y';
		}else{
			colVal = 'N';
		}
		ds_pw_cf.setColumn(rVal,'USE_YN',colVal);
		ds_pw_cf.setColumn(rVal,'CPHR',Edt_pw_lm_v.value);
	}
}

