function pw_combo(count){

	for(var i = 0 ; i < count ; i++){
		var toUse= ds_pw_cf.getColumn(i, "USE_YN");
		var toCode = ds_pw_cf.getColumn(i, "CHCK_CD");
		if(toCode == '3'){
			if(toUse == 'Y'){
				Cmb_pw_val_v.index = 0;
			}else{
				Cmb_pw_val_v.index = 1;
			}
		}else if(toCode == '1'){
			if(toUse == 'Y'){
				Cmb_pw_min_v.index = 0;

			}else{
				Cmb_pw_min_v.index = 1;
			}
		}else if(toCode == '2'){
			if(toUse == 'Y'){
				Cmb_pw_max_v.index = 0;
			}else{
				Cmb_pw_max_v.index = 1;
			}
		}else if(toCode == '5'){
			if(toUse == 'Y'){
				Cmb_pw_ch_v.index = 0;
			}else{
				Cmb_pw_ch_v.index = 1;
			}
		}else if(toCode == '6'){
			if(toUse == 'Y'){
				Cmb_pw_lm_v.index = 0;
			}else{
				Cmb_pw_lm_v.index = 1;
			}
		}
	}
}

