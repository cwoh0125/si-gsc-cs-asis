	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
}

/************************************************************************************************************************
***********************************************페이지로드 콜백
************************************************************************************************************************/
function fn_callBack_com_cd(svcid,strErrCode,strErrMsg){
	
	for(var i = 0 ; i < ds_Cmb_cd_list.rowcount ; i++){
		ds_inner.addRow();
		ds_inner.copyRow( i, ds_Cmb_cd_list, i);
	}
	var j = 0;
	for(var i = 1 ; i <= ds_inner.rowcount ; i++){
		if(i == ds_Cmb_cd_list.rowcount){
			ds_Cmb_cd_list.addRow();
		}
		ds_Cmb_cd_list.copyRow(i , ds_inner, j)
		j++;
	}
	ds_Cmb_cd_list.setColumn(0, "LRGCLAS_CD", "전체");
	Cmb_lg_cd.index = 0;
}

