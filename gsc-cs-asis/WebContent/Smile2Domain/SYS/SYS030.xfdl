function fn_callBack(svcid,strErrCode,strErrMsg){
	Stt_LgCnt.text = ds_lgroup.rowcount;
	Stt_SgCnt.text = ds_sgroup.rowcount;
	if(svcid == "R1"){			
		if(ds_lgroup.rowcount > 0){
			ds_srch.clearData();
			
			fun_ds_srch(ds_srch, "LRGCLAS_CD", ds_lgroup.getColumn(ds_lgroup.rowposition, "LRGCLAS_CD"));
			
			var	strSvcID = "R4";
			var	strURL = SVC_USR_URL;
			var	strInDatasets = "SELECT_0_IDATA=ds_srch:a"; 
			var	strOutDatasets = "ds_sgroup=SELECT_0_ODATA";
			var	strArgument = "SELECT_0_ID=SYS001.Get_Sgroup_List_SELECT";
			var strCallbackFunc = "fn_callBack_srch";
			var strProcessBar = true;
			
			cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
		}else if(svcid == "R5"){
			ds_srch.clearData();
		}else{			
			cf_Alert("COMI0006");//조회된건이 없습니다.
		}		
	}
}

