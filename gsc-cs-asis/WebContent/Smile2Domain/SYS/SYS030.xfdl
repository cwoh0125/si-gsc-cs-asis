/************************************************************************************************************************
***********************************************대분류 저장
************************************************************************************************************************/
function Btn_lg_save_onclick(obj:Button,  e:ClickEventInfo)
{	
	
	
	for (var i=0; i < ds_lgroup.rowcount; i++){		
		if (ds_lgroup.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_lgroup.getRowType(i) == Dataset.ROWTYPE_UPDATE){
			
			//필수 체크
			if(!cf_chk_data(ds_lgroup.getColumn(i,"LRGCLAS_CD"),"대분류코드")){
				ds_lgroup.rowposition = i;				
				Edt_lg_lcd_v.setFocus();
				return false;
			}
			
			if(!cf_chk_data(ds_lgroup.getColumn(i,"LRGCLAS_CDNM"),"대분류코드명")){
				ds_lgroup.rowposition = i;				
				Edt_lg_lnm_v.setFocus();
				return false;
			}
			
			if(!cf_chk_data(ds_lgroup.getColumn(i,"USE_YN"),"사용여부")){
				ds_lgroup.rowposition = i;				
				Cmb_lg_use_yn.setFocus();
				return false;
			}
			
			if(ds_lgroup.getRowType(i) == Dataset.ROWTYPE_INSERT){
				for(var j = 0 ; j < ds_lgroup_insert.rowcount ; j++){
					if(ds_lgroup_insert.getColumn(j,"LRGCLAS_CD") == ds_lgroup.getColumn(i,"LRGCLAS_CD")){
					cf_Alert("TSSE0006", "대분류코드");	//TSSE0006 이미 등록된 !@ 입니다.
					ds_lgroup.rowposition = i;	
					Edt_lg_lcd_v.setFocus();
					return false;
					}
				}
			}
		}
	}
	
	if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?
			
    var	strSvcID = "U1";
	var	strURL = SVC_URL;
	
	var	strInDatasets = "COMSAVE_0_IDATA=ds_lgroup:u"; 
	var	strOutDatasets = "";
	var	strArgument = "COMSAVE_0_ID=SYS001.Set_lgroup";
	
	var strCallbackFunc = "fn_callBack_lg_save";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	//alert(Edt_lg_lcd_v.value);}
}
function fn_callBack_lg_save(){	
	cf_Alert("COMI0003"); //저장되었습니다.
	Btn_srch_onclick();
}


