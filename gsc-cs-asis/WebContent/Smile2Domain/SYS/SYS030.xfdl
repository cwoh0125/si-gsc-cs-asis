function srch_lgroup(){
	ds_group_srch.clearData();
        				
	var	strSvcID = "R3";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_group_srch:a"; 
	var	strOutDatasets = "ds_lgroup_insert=SELECT_0_ODATA";
	var	strArgument = "SELECT_0_ID=SYS001.Get_Lgroup_List_SELECT";
	var strCallbackFunc = "fn_callBack";
