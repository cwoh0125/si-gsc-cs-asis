function Gid_lgroup_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_lgroup.getColumn(ds_lgroup.rowposition, "ADDROW_YN") == "N"){
		var sgroup_view = ds_lgroup.getColumn(Gid_lgroup.currentrow,'LRGCLAS_CD');
		
		//데이터초기화
		//ds_lgroup.clearData();
		ds_sgroup.clearData();
		ds_sb_group.clearData();
		ds_sgroup_cp.clearData();
		//ds_lgroup.deleteRow(ds_lgroup.rowposition);

		fun_ds_srch(ds_group_srch, "LRGCLAS_CD", sgroup_view);
		
		var	strSvcID = "R3";
		var	strURL = SVC_USR_URL;
		var	strInDatasets = "SELECT_0_IDATA=ds_group_srch:a"; 
		var	strOutDatasets = "ds_sgroup=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=SYS001.Get_Sgroup_List_SELECT";
		var strCallbackFunc = "fn_callBack_lgroup";
		var strProcessBar = true;
		
		cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
		
		Edt_lg_lcd_v.readonly = true;
	}else{
		Edt_lg_lcd_v.readonly = false;
	}
}

