function Copy_Prevention_onkeydown(obj:Form, e:KeyEventInfo)
{
	if(copy_Yn == "N"){
		if(e.ctrlKey && e.keycode == 67){
			if(e.keycode == 67 ){			
					system.setClipboard("CF_TEXT","");
					alert("복사를 하실수 없습니다.");			
			}	
		}
	}
}

function Copy_onrbuttondown(obj:Form, e:MouseEventInfo)
{
	if(copy_Yn == "N"){
		if(e.button == "rbutton")
		{
		   alert("마우스 우클릭을 사용할 수 없습니다.");
		   return;
		}
	}

}
