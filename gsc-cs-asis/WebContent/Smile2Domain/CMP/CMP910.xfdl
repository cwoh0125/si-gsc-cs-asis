	ds_cmp_list.clearData();
	ds_record_list.clearData();	

	var	strSvcID = "D1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "UPDATE_0_IDATA=ds_cmp_del:a SELECT_1_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_cmp_list=SELECT_1_ODATA";
	var	strArgument = "UPDATE_0_ID=CMP180.Set_CMP_DELETE SELECT_1_ID=CMP180.Get_CMP_List_SELECT_temp";
