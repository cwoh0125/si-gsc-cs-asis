// 초기화 버튼 클릭시
function Btn_ClearCond_onclick(obj:Button,  e:ClickEventInfo)
{	
	// 기간구분
	Cmb_TermGbn.value = "1";
	Cmb_TermGbn_onitemchanged();		
	Cmb_Type.index		= 0;	// 불만유형
	Cmb_DtlType.index	= 0;	// 불만세부유형
	Btn_DelCmpCorp_onclick();	// 불만발생업체
	Cmb_Contact.index	= 0;	// 접촉채널
	Cmb_SrchDept.index	= 0;	// 부문, 본부
	// 처리팀
	ds_handler_team.clearData();
	ds_handler_team.insertRow(0);
	ds_handler_team.setColumn(0,"CODE_NM","전체");
	Cmb_SrchHandlerTeam.index = 0;
	Edt_SrchHandlerNM.value		= "";	// 처리자명
	Cmb_SrchHC.index			= 0;	// Happy Call
	Cmb_SrchHCAttiSatis.index	= 0;	// 태도만족도
	Cmb_SrchHCRsltSatis.index	= 0;	// 결과만족도
	Cmb_HCimpos.index			= 0;	// Happy Call 불가사유
	Cmb_SrchOilCode.index		= 0;	// 불만제품
	Cmb_SrchState.index			= 0;	// 현상
	Cmb_SrchSample.index		= 0;	// 시료 품질 점검 결과
	Cmb_SrchKixx.index			= 0;	// Kixx 품질서비스차량 출동
	Cmb_SrchCompen.index		= 0;	// 보상여부
	// 보상금액
	Cmb_SrchCpsAmt.index		= 0;
	Cmb_SrchCpsAmt_onitemchanged();
	Chk_Srch_Handler1.value		= "N";
	Chk_Srch_Handler2.value		= "N";
	Chk_Srch_Handler3.value		= "N";
}

