 *****	조회조건 변경 이벤트 처리
***********************************************************************************/

// 기간구분 변경시
function Cmb_TermGbn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(getTrim(Cmb_TermGbn.value))
	{
		case "1" :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			break;
		
		case "2" :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= true;
			Cmb_Quarter.visible		= false;
					
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();					
			Cmb_HalfYearly.index	= 0;
			break;
		
		case "3" :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= true;
			
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			Cmb_Quarter.index		= 0;
			break;
		
		case "4" :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy-MM";
			cal_day_1.editformat	= "yyyy-MM";
			cal_day_1.value			= getToday();			
			break;
		
		case "5" :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
					
			cal_day_1.dateformat	= "yyyy-MM-dd";
			cal_day_1.editformat	= "yyyy-MM-dd";
			cal_day_1.value			= getToday();					
			break;
		
		default :
			cal_day_1.visible 		= true;
			Cmb_HalfYearly.visible	= false;
			Cmb_Quarter.visible		= false;
			
			cal_day_1.dateformat	= "yyyy";
			cal_day_1.editformat	= "yyyy";
			cal_day_1.value			= getToday();			
			break;
	}
}

// 조회구분 라디오 변경시(그리드 변경되어야함.)
function Rdo_SrchGbn_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	fn_ClearListData();
	switch(getTrim(Rdo_SrchGbn.value))
	{
		// 조직별(불만유형)
		case "1" :
			Grd_Cmp.visible		= true;
			Grd_Cmp1.visible	= false;
			Grd_Cmp2.visible	= false;
			Grd_Cmp4.visible	= false;
			break;
			
		// 처리팀별(처리일수)
		case "2" :
			Grd_Cmp.visible		= false;
			Grd_Cmp1.visible	= true;
			Grd_Cmp2.visible	= false;
			Grd_Cmp4.visible	= false;
			break;
		// 조직별(불만세부유형)
		case "3" :
			Grd_Cmp.visible		= false;
			Grd_Cmp1.visible	= false;
			Grd_Cmp2.visible	= true;
			Grd_Cmp4.visible	= false;
			break;
		// 조직별(문제현상)
		case "5" :
			Grd_Cmp.visible		= false;
			Grd_Cmp1.visible	= false;
			Grd_Cmp2.visible	= false;
			Grd_Cmp4.visible	= true;
			break;
		default :
			break;
	}
}

