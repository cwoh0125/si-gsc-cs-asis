{
	if(getTrim(Rdo_SrchGbn.value) == "1") {
		if(ds_result.rowcount < 1) {
			cf_Alert("COME0013","리스트");		
			return;
		}
		var objGridId = eval("Grd_Cmp");
	} else if(getTrim(Rdo_SrchGbn.value) == "2") {
		if(ds_result_2.rowcount < 1) {
			cf_Alert("COME0013","리스트");
			return;
		}	
		var objGridId = eval("Grd_Cmp1");
	} else if(getTrim(Rdo_SrchGbn.value) == "3") {
		if(ds_result_3.rowcount < 1) {
			cf_Alert("COME0013","리스트");
			return;
		}	
		var objGridId = eval("Grd_Cmp2");	
