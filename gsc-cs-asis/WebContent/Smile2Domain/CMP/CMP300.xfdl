// 초기화 버튼 클릭시
function Btn_ClearCond_onclick(obj:Button,  e:ClickEventInfo)
{	
	// 기간구분
	Cmb_TermGbn.value = "1";
	Cmb_TermGbn_onitemchanged();		
	Cmb_Type.index		= 0;	// 불만유형
	Cmb_DtlType.index	= 0;	// 불만세부유형
	Btn_DelCmpCorp_onclick();	// 불만발생업체
	Cmb_Contact.index	= 0;	// 접촉채널
	Cmb_SrchDept.index	= 0;	// 부문, 본부
	// 처리팀
