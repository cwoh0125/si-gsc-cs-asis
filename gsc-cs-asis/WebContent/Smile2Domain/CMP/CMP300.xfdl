// 조회 열이 [월]인 경우 기간구분에 따른 그리드 열 사이즈 제어.
function fn_setGridSizeTermGbn(strGridId)
{
	var intStrtIndx = 1;	// 구분컬럼 수
	var objGrid = eval("this.all['" + strGridId + "']");

	if(strGridId == "Grd_Cmp1"){
		intStrtIndx = 1;
	}else if(strGridId == "Grd_Cmp3" || strGridId == "Grd_Cmp5"){
		intStrtIndx = 2;
	}
	
	// 하드코딩좀 했어요
	// 기간구분에 따른 그리드 셀 넓이 조정.	
	if(getTrim(Cmb_TermGbn.value) == "1") {
		for(i=intStrtIndx;i<objGrid.getFormatColCount();i++){
			objGrid.setFormatColProperty(i,"size",76);
		}
	}else if(getTrim(Cmb_TermGbn.value) == "2") {			// 반기
		if(getTrim(Cmb_HalfYearly.value) == ""){			// 전체
			for(i=intStrtIndx;i<objGrid.getFormatColCount();i++){
				objGrid.setFormatColProperty(i,"size",76);
			}
		}else if(getTrim(Cmb_HalfYearly.value) == "1"){		// 상반기
			for(i=intStrtIndx;i<intStrtIndx+9;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}
			for(i=intStrtIndx+9;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}				
		}else if(getTrim(Cmb_HalfYearly.value) == "2"){		// 하반기
			for(i=intStrtIndx;i<intStrtIndx+9;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}
			for(i=intStrtIndx+9;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}				
		}
	}else if(getTrim(Cmb_TermGbn.value) == "3") {			// 분기
		if(getTrim(Cmb_Quarter.value) == ""){				// 전체
			for(i=intStrtIndx;i<objGrid.getFormatColCount();i++){
				objGrid.setFormatColProperty(i,"size",76);
			}
		}else if(getTrim(Cmb_Quarter.value) == "1"){		// 1분기
			for(i=intStrtIndx;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}				
			for(i=intStrtIndx;i<intStrtIndx+4;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}				
		}else if(getTrim(Cmb_Quarter.value) == "2"){		// 2분기
			for(i=intStrtIndx;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}				
			for(i=intStrtIndx+4;i<intStrtIndx+8;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}			
		}else if(getTrim(Cmb_Quarter.value) == "3"){		// 3분기
			for(i=intStrtIndx;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}				
			for(i=intStrtIndx+9;i<intStrtIndx+13;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}				
		}else if(getTrim(Cmb_Quarter.value) == "4"){		// 4분기
			for(i=intStrtIndx;i<objGrid.getFormatColCount()-1;i++){
				objGrid.setFormatColProperty(i,"size",0);
			}				
			for(i=intStrtIndx+13;i<intStrtIndx+17;i++){
				objGrid.setFormatColProperty(i,"size",76);
			}			
		}			
	}else if(getTrim(Cmb_TermGbn.value) == "4") {			// 월별
		var strSelMm = getTrim(cal_day_1.value).substr(4,2);
		
		switch(strSelMm)		// 선택한 월만 보이도록.
		{
			case "01" :
