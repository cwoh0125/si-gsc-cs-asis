// 조회버튼 클릭
function Btn_CmpSrch_onclick(obj:Button,  e:ClickEventInfo)
{
	
	if(Cmb_TermGbn.value == "1" || Cmb_TermGbn.value == "4" || Cmb_TermGbn.value == "5"){	
		if(getTrim(cal_day_1.value) == "" ){
			cf_Alert("COME0007","기간");
			cal_day_1.setFocus();
			return;
		}
	}
	
	if(getTrim(Cmb_SrchCpsAmt.value) != "") {
		if(getTrim(Cmb_SrchAcount.value) == "") {
			cf_Alert("COME0008","보상금액");
			Cmb_SrchAcount.setFocus();
			return;
		}
	}
	
	if(getTrim(Cmb_SrchOilCode.value) != "") {
		if(getTrim(Cmb_Type.value) != "" && getTrim(Cmb_Type.value) != "CA" && getTrim(Cmb_Type.value) != "CK") {
			cf_Alert("COME0008","불만제품은 불만유형 중 전체 또는 품질, 혼유");		
			return;
		}
	}	
		
	if(getTrim(Cmb_SrchState.value) != "") {
		if(getTrim(Cmb_Type.value) != "" && getTrim(Cmb_Type.value) != "CA" && getTrim(Cmb_Type.value) != "CK") {
			cf_Alert("COME0008","문제현상은 불만유형 중 전체 또는 품질, 혼유");		
			return;
		}
	}
	
	if(getTrim(Cmb_SrchSample.value) != "") {
		if(getTrim(Cmb_Type.value) != "" && getTrim(Cmb_Type.value) != "CA" && getTrim(Cmb_Type.value) != "CK") {
			cf_Alert("COME0008","시료품질 점검결과는 불만유형 중 전체 또는 품질, 혼유");		
			return;
		}
	}
	
	if(getTrim(Cmb_SrchKixx.value) != "") {
		if(getTrim(Cmb_Type.value) != "" && getTrim(Cmb_Type.value) != "CA" && getTrim(Cmb_Type.value) != "CK") {
			cf_Alert("COME0008","Kixx 품질서비스차량 출동은 불만유형 중 전체 또는 품질, 혼유");		
			return;
		}
	}
	
	if(Rdo_SrchGbn.value == "2" || Rdo_SrchGbn.value == "4" || Rdo_SrchGbn.value == "6"){ // 조직별(불만세부유형)
		if(getTrim(Cmb_TermGbn.value) == "5"){	// 일자별은 조회 안됨.
			cf_Alert("CAME0015","월","일별(기간구분)");
			Cmb_TermGbn.setFocus();
			return;
		}		
	}		
		
	if(Rdo_SrchGbn.value == "5" || Rdo_SrchGbn.value == "6"){
		if(getTrim(Cmb_Type.value) != "" && getTrim(Cmb_Type.value) != "CA" && getTrim(Cmb_Type.value) != "CK"){
			cf_Alert("COME0008","문제현상과 연관된 통계의 불만유형은 전체 또는 품질, 혼유");
			Cmb_Type.setFocus();
			return;
		}
	}
		
	ds_srch.clearData();		// 조회조건 데이터셋
	
	var in_data = cal_day_1.text.replace("-","");				
	fun_ds_srch(ds_srch, "TIME_KEY", in_data);
	fun_ds_srch(ds_srch, "CMB_GUBUN", getTrim(Cmb_TermGbn.value));
	
	if(Cmb_TermGbn.value == "2"){
		fun_ds_srch(ds_srch, "CMB_HALF", getTrim(Cmb_HalfYearly.value));
	} else {
		fun_ds_srch(ds_srch, "CMB_HALF", "");	
	}
	
	if(Cmb_TermGbn.value == "3"){
		fun_ds_srch(ds_srch, "CMB_QUA", getTrim(Cmb_Quarter.value));
	} else {
		fun_ds_srch(ds_srch, "CMB_QUA", "");	
	}
	
	// 불만유형이 전체일때 => REAL 전체 / 품질, 혼유만 포함된 전체로 구분.
	if(getTrim(Cmb_Type.value) == ""){
		if(getTrim(Cmb_SrchOilCode.value) == "" && getTrim(Cmb_SrchState.value) == "" && getTrim(Cmb_SrchSample.value) == "" && getTrim(Cmb_SrchKixx.value) == ""){
			fun_ds_srch(ds_srch, "DSFT_TYP_TOT_GBN", "1");
		} else{
			fun_ds_srch(ds_srch, "DSFT_TYP_TOT_GBN", "2");
		}
	}
	
	fun_ds_srch(ds_srch, "DSFT_TYP_CD", getTrim(Cmb_Type.value));						// 불만유형
	fun_ds_srch(ds_srch, "DSFT_TAXN_TYP_CD", getTrim(Cmb_DtlType.value));				// 불만세부유형
	fun_ds_srch(ds_srch, "ORGN_PTR_ST_CD", getTrim(Edt_SrchCmpCorpCd.value));			// 불만발생업체코드
	fun_ds_srch(ds_srch, "CONN_CHNL_KIND_CD", getTrim(Cmb_Contact.value));				// 접촉채널
	fun_ds_srch(ds_srch, "HGRK_DEPT_CD", getTrim(Cmb_SrchDept.value));					// 부문,본부
	fun_ds_srch(ds_srch, "DEPT_CD", getTrim(Cmb_SrchHandlerTeam.value));				// 처리팀
	fun_ds_srch(ds_srch, "DSPSR_ID_NM", getTrim(Edt_SrchHandlerNM.value));				// 처리자명
	fun_ds_srch(ds_srch, "HC_YN", getTrim(Cmb_SrchHC.value));							// Happy Call
	fun_ds_srch(ds_srch, "ATTD_SFT_CD", getTrim(Cmb_SrchHCAttiSatis.value));			// 태도만족도	
	fun_ds_srch(ds_srch, "RSLT_SFT_CD", getTrim(Cmb_SrchHCRsltSatis.value));			// 결과만족도
	fun_ds_srch(ds_srch, "HC_UABL_RSN_CD", getTrim(Cmb_HCimpos.value));					// Happy Call 불가사유
	fun_ds_srch(ds_srch, "PRDCT_CD", getTrim(Cmb_SrchOilCode.value));					// 불만제품
	fun_ds_srch(ds_srch, "PRBLM_PENOM_CD", getTrim(Cmb_SrchState.value));				// 현상
	fun_ds_srch(ds_srch, "VHCL_QLIT_DSFT_CAUS_CD", getTrim(Cmb_SrchSample.value));		// 시료품질 점검결과
	fun_ds_srch(ds_srch, "VHCL_BIZTR_QLIT_DSFT_TYP_CD", getTrim(Cmb_DsftCaus.value));	// 품질불만유형
	fun_ds_srch(ds_srch, "VHCL_BIZTR_YN", getTrim(Cmb_SrchKixx.value));					// Kixx 품질서비스차량 출동
	fun_ds_srch(ds_srch, "RWRD_YN", getTrim(Cmb_SrchCompen.value));						// 보상여부			
	
	if(getTrim(Cmb_SrchCpsAmt.value) == "") {
		fun_ds_srch(ds_srch, "CPS_AMT_GBN", getTrim(Cmb_SrchCpsAmt.value));				// 보상금액구분
		fun_ds_srch(ds_srch, "CPS_AMT", "");											// 보상금액
	} else {
		fun_ds_srch(ds_srch, "CPS_AMT_GBN", getTrim(Cmb_SrchCpsAmt.value));				// 보상금액구분
		fun_ds_srch(ds_srch, "CPS_AMT", getTrim(Cmb_SrchAcount.value));					// 보상금액	
	}
	
	// 처리자구분에 체크항목이 하나도 없는 경우, 전체데이터가 조회된다.
	var p_CMP_HANDLER = "";
	if (Chk_Srch_Handler1.value == "N" && Chk_Srch_Handler2.value == "N" && Chk_Srch_Handler3.value == "N")			p_CMP_HANDLER = "";
	else if (Chk_Srch_Handler1.value == "Y" && Chk_Srch_Handler2.value == "N" && Chk_Srch_Handler3.value == "N")	p_CMP_HANDLER = "1";
	else if (Chk_Srch_Handler1.value == "Y" && Chk_Srch_Handler2.value == "Y" && Chk_Srch_Handler3.value == "N")	p_CMP_HANDLER = "2";
	else if (Chk_Srch_Handler1.value == "Y" && Chk_Srch_Handler2.value == "N" && Chk_Srch_Handler3.value == "Y")	p_CMP_HANDLER = "3";
	else if (Chk_Srch_Handler1.value == "Y" && Chk_Srch_Handler2.value == "Y" && Chk_Srch_Handler3.value == "Y")	p_CMP_HANDLER = "4";
	else if (Chk_Srch_Handler1.value == "N" && Chk_Srch_Handler2.value == "Y" && Chk_Srch_Handler3.value == "N")	p_CMP_HANDLER = "5";
	else if (Chk_Srch_Handler1.value == "N" && Chk_Srch_Handler2.value == "N" && Chk_Srch_Handler3.value == "Y")	p_CMP_HANDLER = "6";
	else if (Chk_Srch_Handler1.value == "N" && Chk_Srch_Handler2.value == "Y" && Chk_Srch_Handler3.value == "Y")	p_CMP_HANDLER = "7";
	fun_ds_srch(ds_srch, "CMP_HANDLER", p_CMP_HANDLER);
	
	
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "SELECT_0_IDATA=ds_srch:a";	
	if(getTrim(Rdo_SrchGbn.value) == "1") {					// 조직별(불만유형)
		ds_result.clearData();
		var	strSvcID = "R2";
		var	strOutDatasets = "ds_result=SELECT_0_ODATA";			
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT1_SELECT_1";		
	
	} else if(getTrim(Rdo_SrchGbn.value) == "2") {			// 처리팀별(월)
		ds_result_2.clearData();
		var	strSvcID = "R3";		
		var	strOutDatasets = "ds_result_2=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT2_SELECT_1";	
		
	} else if(getTrim(Rdo_SrchGbn.value) == "3") {			// 조직별(불만세부유형)	
		ds_result_3.clearData();
		var	strSvcID = "R4";				
		var	strOutDatasets = "ds_result_3=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT3_SELECT_1";	
		
	} else if(getTrim(Rdo_SrchGbn.value) == "4") {			// 불만세부유형별(월)
		ds_result_4.clearData();
		var	strSvcID = "R5";		
		var	strOutDatasets = "ds_result_4=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT4_SELECT_1";	
		
	} else if(getTrim(Rdo_SrchGbn.value) == "5") {			// 조직별(문제현상) 사용하지 않음 201712
		ds_result_5.clearData();
		var	strSvcID = "R6";		
		var	strOutDatasets = "ds_result_5=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT5_SELECT";	
	} else if(getTrim(Rdo_SrchGbn.value) == "6") {			// 문제현상별(월) 사용하지 않음 201712
		ds_result_6.clearData();
		var	strSvcID = "R7";		
		var	strOutDatasets = "ds_result_6=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT6_SELECT";	
	} else if(getTrim(Rdo_SrchGbn.value) == "7") {			// 문제원인별 사용하지 않음 201712
		ds_result_7.clearData();
		var	strSvcID = "R8";		
		var	strOutDatasets = "ds_result_7=SELECT_0_ODATA";
		var	strArgument = "SELECT_0_ID=CMP300.Get_CMP_STT7_SELECT";	
	}
	
		
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
}


