					objGrid.setFormatColProperty(i,"size",0);
				}
				objGrid.setFormatColProperty(intStrtIndx+5,"size",76);				
				break;
			case "06" :
				for(i=intStrtIndx;i<objGrid.getFormatColCount()-1;i++){
					objGrid.setFormatColProperty(i,"size",0);
				}
				objGrid.setFormatColProperty(intStrtIndx+6,"size",76);				
