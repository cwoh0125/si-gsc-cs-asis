    <Script type="xscript4.0"><![CDATA[
function chartfx_onload(obj:Form, e:LoadEventInfo)
{
	Button01_onclick();
}

function Button01_onclick(obj:Button,  e:ClickEventInfo)
{

	
	var title = XChart1.get_Titles(0);
	title.Alignment = "StringAlignment_Center";
	title.Font.Name = "Times New Roman";
	title.Font.Size = 14;
	title.Font.Bold = true;
	title.Text = "PRODUCT SALES";
	title.TextColor = "RED";
	
	title = XChart1.get_Titles(1);
	title.Alignment = "StringAlignment_Center";
	title.Font.Name = "Times New Roman";
	title.Font.Size = 12;
	title.Font.Bold = true;
	title.Text = "By Region. Q2-2001";
	title.TextColor = "blue";
	
	title = XChart1.get_Titles(2);
	title.Alignment = "StringAlignment_Far";
	title.Font.Name = "Times New Roman";
	title.Font.Size = 9;
	title.Font.Italic = true;
	title.Text = "Chart created by John Doe";
	title.TextColor = "black";
	title.DockArea = "DockArea_Bottom";
	title.Gap = 10;
	
	XChart1.OpenData("COD_Values", 1, 6);
	
	for(var i=0;i<6;i++)
	{
		/*sparrow CWE-330 수정처리
		XChart1.set_Value(0, i, (Math.random(1,9)) * 5.5);
		*/
		var rand = new Date().getTime();
		rand = ((rand*9301+49297) % 233280)/(233280.0);
		
		XChart1.set_Value(0, i, rand * 5.5);
	}	

	
	XChart1.CloseData("COD_Values");
	
	XChart1.Gallery = "Gallery_Pie";
	XChart1.PointLabels = true;
}
]]></Script>
