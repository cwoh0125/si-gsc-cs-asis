function jilmon_create(obj)
{	
	AXMSIE.visible=true;
	var FORM_S = "<form id='frmCkEditor'><div style='background-color:#f5f5f5;border:1px solid #D9D9D9;width:1628px;height:690px;overflow-y:scroll;'>";
	var FORM_E = "</div></form>";
	
	var TABLE_S = "<table border='1' cellspacing='0' cellpadding='0' bordercolor='#b5c0cb' class='sHeadListTbl' width='100%'><tbody>";
	var TABLE_E = "</tbody></table>";
	
	var TR_S = "<tr>";
	var TR_E = "</tr>";
	
	var TD_1 = "<td width='150'";
	var TD_2 = "<td>";
	var TD_E = "</td>";
	
	var CHKBOX_S = "<input onclick='return btnCntCheck(this)' type=checkbox  value='checkbox' name=q";
	var CHKBOX_E = "</input>";
	var INPUT_S = "<INPUT type='text' class='inputleft' style='WIDTH: 99%' name=q";	
	var INPUT_E = "</input>";
	var NBSP = "&nbsp;";
	var RESULT = "<INPUT id=result type=hidden name=result><INPUT id=btnResult type=hidden name=btnResult LANGUAGE=javascript onclick='return btnResult_OnClick()'>";
	var QST_TCNT = "<INPUT id=qst_cnt type=hidden name=qst_cnt value=";
	var qst_cnt=0;
	
	strHTML =  strHTML + TABLE_S;

	for (var i=0; i < ds_jobqstdet2.rowcount; i++){	
		
		strHTML =  strHTML + TR_S;
		
			strHTML =  strHTML + "<th width=150>";
			strHTML =  strHTML + "질문" + (i+1) + NBSP + "<span style='color:#ce5f1b;'>(" + ds_jobqstdet2.getColumn(i,"ALTM_SCOR") + "점) </span>";
			strHTML =  strHTML + "</th>";
		
			strHTML =  strHTML + TD_2;
			strHTML =  strHTML + ds_jobqstdet2.getColumn(i,"QUES_CTT");
			if ( ds_jobqstdet2.getColumn(i, "QUES_TYP_CD") == "0" ) {			
				strHTML =  strHTML + "(" + ds_jobqstdet2.getColumn(i,"ANSW_CNT") + "개)"; 
			}
			strHTML =  strHTML + TD_E;
		
		strHTML =  strHTML + TR_E;

		
		strHTML =  strHTML + TR_S;
		
			strHTML =  strHTML + "<th>";
			strHTML =  strHTML + "답안" + (i+1) ;
			strHTML =  strHTML + "</th>";

			strHTML =  strHTML + TD_2;
			if ( ds_jobqstdet.getColumn(i, "QUES_TYP_CD") == "0" ) {
				var rqst = ds_jobqstdet2.getColumn(i,"ANSW_CTT");
				var arr = getTrim(rqst).split("@");				
				
				for( var j=1; j < arr.length; j++){					
					strHTML =  strHTML + CHKBOX_S;					
					strHTML =  strHTML + i;
					strHTML =  strHTML + " cnt=" + ds_jobqstdet2.getColumn(i,"ANSW_CNT");					
					strHTML =  strHTML + ">";
					strHTML =  strHTML + NBSP + arr[j];
					strHTML =  strHTML + CHKBOX_E;
				}
			}else{				
				strHTML =  strHTML + INPUT_S + i + ">";
				strHTML =  strHTML + INPUT_E;					
			}
			strHTML =  strHTML + TD_E;

		strHTML =  strHTML + TR_E;
		
		qst_cnt++;
	}

	strHTML =  strHTML + TABLE_E;
	
	strHTML =  strHTML + RESULT;	
	
	strHTML =  FORM_S + strHTML + QST_TCNT + qst_cnt + ">" + FORM_E;		
	
	AXMSIE.Document.body.innerHTML = strHTML;

}



function Edt_ANSR_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	var answer_arr1;
	var answer_arr2;
	var answer_arr3;
	var answer_arr4;
	var answer_arr5;
	
	edt_answer1.value="";
	edt_answer2.value="";
	edt_answer3.value="";
	edt_answer4.value="";
	edt_answer5.value="";
	
	if(getTrim(Edt_ANSR.value)==""){
		return;
		}
			
	answer_arr1=(Edt_ANSR.value).split("@",2);
	answer_arr2=(Edt_ANSR.value).split("@",3);
	answer_arr3=(Edt_ANSR.value).split("@",4);
	answer_arr4=(Edt_ANSR.value).split("@",5);
	answer_arr5=(Edt_ANSR.value).split("@",6);
		
		
	edt_answer1.value=answer_arr1;
	edt_answer2.value=answer_arr2;
	edt_answer3.value=answer_arr3;
	edt_answer4.value=answer_arr4;
	edt_answer5.value=answer_arr5;
	
	edt_answer2.value=(edt_answer2.text).replace(answer_arr1,"");
	edt_answer3.value=(edt_answer3.text).replace(answer_arr2,"");
	edt_answer4.value=(edt_answer4.text).replace(answer_arr3,"");
	edt_answer5.value=(edt_answer5.text).replace(answer_arr4,"");
	
	edt_answer1.value=(edt_answer1.text).replace("<BR>","");
	edt_answer2.value=(edt_answer2.text).replace("<BR>","");
	edt_answer3.value=(edt_answer3.text).replace("<BR>","");
	edt_answer4.value=(edt_answer4.text).replace("<BR>","");
	edt_answer5.value=(edt_answer5.text).replace("<BR>","");
	
	edt_answer1.value=(edt_answer1.text).replace(",","");
	edt_answer2.value=(edt_answer2.text).replace(",","");
	edt_answer3.value=(edt_answer3.text).replace(",","");
	edt_answer4.value=(edt_answer4.text).replace(",","");
	edt_answer5.value=(edt_answer5.text).replace(",","");
}

function edt_answer_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	if(getTrim(edt_answer1.value)!="" && getTrim(edt_answer2.value)!="" && getTrim(edt_answer3.value)!="" && getTrim(edt_answer4.value)!="" && getTrim(edt_answer5.value)!=""){
	
	Edt_ANSR.value="";
	Edt_ANSR.value="@"+edt_answer1.value+"<BR>@"+edt_answer2.value+"<BR>@"+edt_answer3.value+"<BR>@"+edt_answer4.value+"<BR>@"+edt_answer5.value;

	}else if(getTrim(edt_answer1.value)!="" && getTrim(edt_answer2.value)!="" && getTrim(edt_answer3.value)=="" && getTrim(edt_answer4.value)=="" && getTrim(edt_answer5.value)==""){
	
	Edt_ANSR.value="";
	Edt_ANSR.value="@"+edt_answer1.value+"<BR>@"+edt_answer2.value;
	
	}else if(getTrim(edt_answer1.value)!="" && getTrim(edt_answer2.value)!="" && getTrim(edt_answer3.value)!="" && getTrim(edt_answer4.value)=="" && getTrim(edt_answer5.value)==""){
	
	Edt_ANSR.value="";
	Edt_ANSR.value="@"+edt_answer1.value+"<BR>@"+edt_answer2.value+"<BR>@"+edt_answer3.value;
	
	}else if(getTrim(edt_answer1.value)!="" && getTrim(edt_answer2.value)!="" && getTrim(edt_answer3.value)!="" && getTrim(edt_answer4.value)!="" && getTrim(edt_answer5.value)==""){
	
	Edt_ANSR.value="";
	Edt_ANSR.value="@"+edt_answer1.value+"<BR>@"+edt_answer2.value+"<BR>@"+edt_answer3.value+"<BR>@"+edt_answer4.value;
	
	}
}

