/*
	직무퀴즈상세저장
*/
function Btn_save_job_dt_onclick(obj:Button,  e:ClickEventInfo)
{
	//정답등록모델
	var raArr= new Array();
	for ( i = 0 ; i < 5 ; i++ )
	{
		var rowData = ds_jobqstdet.getColumn(ds_jobqstdet.rowposition, "ANSW_CTT"+(i+1));
		if(rowData == "Y") {
			//raArr.push(raArr + (i+1));
			raArr.push(i+1);
		}
	}
	var ra = raArr.join("|");
	ds_jobqstdet.setColumn(ds_jobqstdet.rowposition,"ANSR",  ra);
	ds_jobqstdet.setColumn(ds_jobqstdet.rowposition,"QUIZ_SEQ_NO", ds_jobqst.getColumn(ds_jobqst.rowposition,"QUIZ_SEQ_NO"));
	
	//비고추가
	ds_jobqstdet.setColumn(ds_jobqstdet.rowposition,"ANSWER_RMK", Edt_QUES_ANSWER.value);
	
	//구분추가
	//ds_jobqstdet.setColumn(ds_jobqstdet.rowposition,"QUIZ_GUBUN", cmb_quiz_gubun.value);
		
				
	//필수체크
	var cnt=0;
	for (var i=0; i < ds_jobqstdet.rowcount; i++){
		
		if (ds_jobqstdet.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_jobqstdet.getRowType(i) == Dataset.ROWTYPE_UPDATE){		

			//유형을 선택 안했을경우
			if(!cf_chk_data(ds_jobqstdet.getColumn(i,"QUES_TYP_CD"),"유형")) {				
				ds_jobqstdet.rowposition = i;				
				Cmb_QUES_TYP_CD.setFocus();				
				return false;
			}								
			
			//배점을 입력안했을경우
			if(!cf_chk_data(ds_jobqstdet.getColumn(i,"ALTM_SCOR"),"배점")) {				
				ds_jobqstdet.rowposition = i;				
				Msk_ALTM_SCOR.setFocus();
				return false;
			}
			
			//질문을 입력안했을경우
			if(!cf_chk_data(ds_jobqstdet.getColumn(i,"QUES_CTT"),"질문")) {				
				ds_jobqstdet.rowposition = i;				
				Edt_QUES_CTT.setFocus();
				return false;
			}			
			
			//질문사이즈
			if(!cf_size_Alert(ds_jobqstdet, i , "QUES_CTT" , Edt_QUES_CTT , 255)) return;
			
		
			//접수유형이 객관식일경우 답변갯수에 따라서 답갯수를 체크한다.		
				
			var chHtml=(Edt_ANSR.value).replace('#','<BR>');	
			
			Edt_ANSR.value=chHtml;
				
			var ra_arr = getTrim(ds_jobqstdet.getColumn(i,"ANSR")).split("|");									           
			var ans_arr = getTrim(ds_jobqstdet.getColumn(i,"ANSW_CTT")).split("@");
						
			//cf_trace("ANSR",ds_jobqstdet.getColumn(i,"ANSR"));
			//cf_trace("ANSW_CTT",ds_jobqstdet.getColumn(i,"ANSW_CTT"));
			
			if (ds_jobqstdet.getColumn(i,"QUES_TYP_CD")=="0"){				

				//응답을 입력안했을경우
				if(!cf_chk_data(ds_jobqstdet.getColumn(i,"ANSR"),"응답")) {				
					ds_jobqstdet.rowposition = i;				
					Edt_ANSR.setFocus();
					return false;
				}
			
				if( Number(ds_jobqstdet.getColumn(i,"ANSW_CNT")) != Number(ra_arr.length)){					
					cf_Alert("HELE0001");  //응답갯수가 틀립니다.
					return false;
				}
				if( Number(ds_jobqstdet.getColumn(i,"ANSW_CNT")) > Number(ans_arr.length-1)){
					cf_Alert("HELE0002");  //답변갯수가 틀립니다.
					return false;
				}
			}			
			cnt++;
		}				
	}

	if(cnt < 1){		
		cf_Alert("COMI0007");
		return;
	}
	
	var	strSvcID = "S3";
	var	strURL = SVC_URL;
		
	var	strInDatasets = "COMSAVE_0_IDATA=ds_jobqstdet:u COMSRCH_1_IDATA=ds_srch:a"; 
	//var	strInDatasets = "UPDATE_0_IDATA=ds_jobqstdet:a COMSRCH_1_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_jobqstdet=COMSAVE_0_IDATA ds_jobqstdet=COMSRCH_1_ODATA";
	//var	strOutDatasets = "ds_jobqstdet=UPDATE_0_IDATA ds_jobqstdet=COMSRCH_1_ODATA";
	var	strArgument = "COMSAVE_0_ID=HEL100.GetJobQstDet COMSRCH_1_ID=HEL100.GetJobQstDet_SELECT"; 	
	//var	strArgument = "UPDATE_0_ID=HEL100.setUpdateQA COMSRCH_1_ID=HEL100.GetJobQstDet_SELECT"; 
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);			
}

