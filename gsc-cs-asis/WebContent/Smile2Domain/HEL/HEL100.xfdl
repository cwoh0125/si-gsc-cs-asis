function Btn_USER_ADD_onclick(obj:Button,  e:ClickEventInfo)
{
	var addrow_yn= true;
	var usr_cnt = ds_user.rowcount;
	var usr_job_cnt = ds_user_job.rowcount;
	
	for (var i=0; i< usr_cnt ;i++){		
		var chk_yn			= ds_user.getColumn(i,"CHKGB");	
		var grp_cd			= ds_user.getColumn(i,"GRP_CD");
		var grp_cd_nm		= ds_user.getColumn(i,"GRP_CD_NM");
		var part_cd			= ds_user.getColumn(i,"PART_CD");
		var partcd_nm		= ds_user.getColumn(i,"PARTCD_NM");
		var usr_id			= ds_user.getColumn(i,"USR_ID");
		var usr_nm			= ds_user.getColumn(i,"USR_NM");

		if (chk_yn== "1"){												
			for (var j=0; j< usr_job_cnt ;j++){
				var usr_job_usrid = ds_user_job.getColumn(j,"USR_ID");
				if (usr_id == usr_job_usrid) {
					addrow_yn = false;
				}
			}	
			
			if (addrow_yn){
				var nRow = ds_user_job.addRow();
				ds_user_job.setColumn(nRow,"GRP_CD",grp_cd);						
				ds_user_job.setColumn(nRow,"GRP_CD_NM",grp_cd_nm);
				ds_user_job.setColumn(nRow,"PART_CD",part_cd);
				ds_user_job.setColumn(nRow,"PARTCD_NM",partcd_nm);		
				ds_user_job.setColumn(nRow,"USR_ID",usr_id);
				ds_user_job.setColumn(nRow,"USR_NM",usr_nm);				
			}
		}
		ds_user.setColumn(i,"CHKGB","0");  //추가후 체크지움.
		addrow_yn = true;
	}
	setHeaderChkClear(Grd_user);
}

function Btn_USER_DEL_onclick(obj:Button,  e:ClickEventInfo)
{
	var usr_cnt = ds_user_job.rowcount-1;
		
		for (var i=usr_cnt; i + 1 > 0 ;i--){				
			var chk_yn = ds_user_job.getColumn(i,"CHKGB");
			if (chk_yn == "1"){						
				ds_user_job.deleteRow(i);			
			}
		}		
	setHeaderChkClear(Grd_user_job);	
}

