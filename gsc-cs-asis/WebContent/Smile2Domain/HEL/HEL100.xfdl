/*
	직무퀴즈저장
*/
function Btn_save_job_onclick(obj:Button,  e:ClickEventInfo)
{

	//필수체크
	var cnt=0;
	for (var i=0; i < ds_jobqst.rowcount; i++){
		
		if (ds_jobqst.getRowType(i) == Dataset.ROWTYPE_INSERT || ds_jobqst.getRowType(i) == Dataset.ROWTYPE_UPDATE){		

			if(!cf_chk_data(ds_jobqst.getColumn(i,"QUIZ_TITE"),"직무퀴즈명")) {				
				ds_jobqst.rowposition = i;				
				Edt_QUIZ_TITE.setFocus();				
				return false;
			}								
			
			if(!cf_chk_data(ds_jobqst.getColumn(i,"ALTM_DRTM"),"직무퀴즈시간")) {				
				ds_jobqst.rowposition = i;				
				Msk_ALTM_DRTM.setFocus();
				return false;
			}
			
			if(!cf_size_Alert(ds_jobqst, i , "QUIZ_TITE" , Edt_QUIZ_TITE , 100)) return;
			
			cnt++;
		}				
	}

	if(cnt < 1){		
		cf_Alert("COMI0007");
		return;
	}
	
	//조회모델
	fun_ds_srch(ds_srch, "REG_DTM_FROM", Cal_SCG_REG_DTM_FROM.value);
	fun_ds_srch(ds_srch, "REG_DTM_TO", Cal_SCH_REG_DTM_TO.value);
	fun_ds_srch(ds_srch, "QUIZ_TITE", Edt_SCH_QUIZ_TITE.value);	

	var	strSvcID = "S1";
	var	strURL = SVC_URL;
	var	strInDatasets = "COMSAVE_0_IDATA=ds_jobqst:u COMSRCH_1_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_jobqst=COMSAVE_0_IDATA ds_jobqst=COMSRCH_1_ODATA";
	var	strArgument = "COMSAVE_0_ID=HEL100.GetJobQst COMSRCH_1_ID=HEL100.GetJobQst_SELECT"; 	
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
}

