function fn_callBack(svcid,strErrCode,strErrMsg){

	//조회건수표시
	Stt_SrchCnt.text = ds_jobqst.rowcount;
	Stt_SrchCnt2.text = ds_jobqstdet.rowcount;
	Stt_SrchCnt3.text = ds_jobqstasg.rowcount;
	
	switch(svcid){	
		case "R1" : 	
			if (ds_jobqst.rowcount < 1){
				cf_Alert("COMI0006");
			}else{			
				Grd_jobqst_oncellclick();
			}
			break;
			
		case "R2" : 	
			if (ds_jobqstdet.rowcount < 1){
				cf_Alert("COMI0006");
			}
			set_enable("G3");
			
			//edt_answer1.value="";
			//edt_answer2.value="";
			//edt_answer3.value="";
			//edt_answer4.value="";
			//edt_answer5.value="";
				
			Edt_ANSR_ontextchanged();					
			break;
			
		case "S1" : 							
			cf_Alert("COMI0003");
			set_enable("G1");
			break;		
				
		case "S2" : 							
			cf_Alert("COMI0003");
			set_enable("G1");
			if(ds_jobqst.rowcount > 0){					
				Grd_jobqst_oncellclick();
			}
			break;	

		case "S3" : 							
			cf_Alert("COMI0003");
			set_enable("G3");
			
			
			
			break;	
					
		case "S4" : 							
			cf_Alert("COMI0003");			
			set_enable("G3");			
			break;							
		case "S5" : 							
			cf_Alert("COMI0003");
			set_enable("G5");
			break;						
		case "R5" : 
			break;
		case "R3" : 
			set_enable("G5");
			
			//조회건수가 있을경우 최초 첫번째 로우에 있는값을 세팅한다.
			if(ds_jobqstasg.rowcount != 0){
				Cmb_qst_cd.value = ds_jobqstasg.getColumn(0,"QUIZ_SEQ_NO");
				Grd_jobqstasg_oncellclick();
			}					
			break;
			
		case "TEST" : 
			jilmon_create();
			break;
			
		default :
			break;
	}
}


/*
	*************************************************************
	활성화여부
	*************************************************************
*/
function set_enable(flag){
	switch(flag)
	{
		//최초조회
		case "R1" : 			
			Btn_new_job.enable = true;
			Btn_save_job.enable = false;
			Btn_del_job.enable = false;
			Btn_new_job_dt.enable = false;
			Btn_save_job_dt.enable = false;
			Btn_del_job_dt.enable = false;				
			Btn_new.enable = true;
			Btn_save.enable = false;				
			break;		
		//직무퀴즈 선택시	
		case "G1" :
			Btn_new_job.enable = true;	
			Btn_save_job.enable = true;
			Btn_del_job.enable = true;
			break;	
		//직무퀴즈 신규번튼눌렀을경우	
		case "G2" :			
			Btn_new_job.enable = false;	
			Btn_save_job.enable = true;
			Btn_del_job.enable = true;			
			break;	
		//직무퀴즈상세 선택시		
		case "G3" :	
			Btn_new_job_dt.enable = true;	
			Btn_save_job_dt.enable = true;
			Btn_del_job_dt.enable = true;			
			break;										
		//직무퀴즈상세 신규번튼눌렀을경우	
		case "G4" :	
			Btn_new_job_dt.enable = false;	
			Btn_save_job_dt.enable = true;
			Btn_del_job_dt.enable = true;	
			
			edt_answer1.value = "";
			edt_answer2.value = "";
			edt_answer3.value = "";
			edt_answer4.value = "";
			edt_answer5.value = "";
			
			break;	
		//직무배정 선택시
		case "G5" :	
			Btn_new.enable = true;	
			Btn_save.enable = true;								
			
			//미진행중일때만 직무퀴즈를 변경할수 있다.						
			if (Cmb_TP_BAJUNG.value != "S0003"){				
				//Cmb_qst_cd.enable = false;
				Cmb_qst_cd.readonly = true;
			}else{
				//Cmb_qst_cd.enable = true;
				Cmb_qst_cd.readonly = false;
			}
			
			break;	
		//직무배정  신규번튼눌렀을경우
		case "G6" :	
			Btn_new.enable = false;	
			Btn_save.enable = true;	
			//Cmb_qst_cd.enable = true;
			Cmb_qst_cd.readonly = false;
			break;																	
		default:
			break;
			//cf_Alert("COME0003");
	}
}

