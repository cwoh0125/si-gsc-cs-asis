/*
	직무퀴즈유형에따라서 입력할수있항목이 달라진다.
*/
function Cmb_QUES_TYP_CD_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var val = Cmb_QUES_TYP_CD.value;		
	//주관식
	if(val == "1"){						
		Cmb_ANSW_CNT.value="1";		
		Edt_ANSR.value ="";		
		Cmb_ANSW_CTT1.value = "N";
		Cmb_ANSW_CTT2.value = "N";
		Cmb_ANSW_CTT3.value = "N";
		Cmb_ANSW_CTT4.value = "N";
		Cmb_ANSW_CTT5.value = "N";		
		
		Cmb_ANSW_CNT.enable = false;
		Edt_ANSR.enable = false;
		Cmb_ANSW_CTT1.enable = false;
		Cmb_ANSW_CTT2.enable = false;
		Cmb_ANSW_CTT3.enable = false;
		Cmb_ANSW_CTT4.enable = false;
		Cmb_ANSW_CTT5.enable = false;
	//객관식
	}else{
		Cmb_ANSW_CNT.value="1";	
		Cmb_ANSW_CNT.enable = true;
		Edt_ANSR.enable = true;
		Cmb_ANSW_CTT1.enable = true;
		Cmb_ANSW_CTT2.enable = true;
		Cmb_ANSW_CTT3.enable = true;
		Cmb_ANSW_CTT4.enable = true;
		Cmb_ANSW_CTT5.enable = true;	
	}			
}


