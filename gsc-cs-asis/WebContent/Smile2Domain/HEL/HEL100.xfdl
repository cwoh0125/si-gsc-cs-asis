		return;
	}
	
	fun_ds_srch(ds_srch, "REG_DTM_FROM", Cal_SCG_REG_DTM_FROM.value);
	fun_ds_srch(ds_srch, "REG_DTM_TO", Cal_SCH_REG_DTM_TO.value);
	fun_ds_srch(ds_srch, "QUIZ_TITE", Edt_SCH_QUIZ_TITE.value);		
	fun_ds_srch(ds_srch, "QUIZ_SEQ_NO", ds_jobqst.getColumn(ds_jobqst.rowposition,"QUIZ_SEQ_NO"));
	
	var	strSvcID = "S2";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "UPDATE_0_IDATA=ds_srch:a SELECT_1_IDATA=ds_srch:a"; 
	var	strOutDatasets = "ds_jobqst=UPDATE_0_ODATA ds_jobqst=SELECT_1_ODATA";
	var	strArgument = "UPDATE_0_ID=HEL100.GetJobQst_DELETE SELECT_1_ID=HEL100.GetJobQst_SELECT"; 				
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);				
}


