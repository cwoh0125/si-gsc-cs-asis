function fn_callBack_dtls(strSvcID){


	if(strSvcID == "IF_CouponDtList"){
	
		//2015.08.15 개인정보접촉이력
		trace("=============개인정보접촉이력  이벤트 GS&쿠폰 상세조회Start===========");

		var sAccNo = "BNS770"; //개인정보접촉이력 일련번호
		var sPerTCd = "PT1002";  // 수행업무 코드
		var sPerTDt = "ICCS_CSC 이벤트 GSC APP 쿠폰 상세조회";  // 수행업무 상세내용
		var sCalFun = "Coupon_DtList()";  // 호출함수명 
		var sInqCon = "|" + "CupnNo="+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CUPN_NO")				
					+ "|" ;  // 조회조건 
			
		var sInqCnt = ds_if_dtls_record.rowcount;  // 조회건수
		var sInCustNo = ds_if_dtls_record.getColumn(ds_if_dtls_record.rowposition, "CUPN_NO") ;//식별키		
		
		trace("============쿠폰상세===================");
		trace(ds_if_dtls_env.saveXML());	  
	    trace(ds_if_dtls_header.saveXML());		
		trace(ds_if_dtls_record.saveXML());	
		trace("===============================");
		//사용내역 초기화
		ds_if_app_dtls_record.clearData();
									
		trace("개인정보접촉이력--이벤트 GS&쿠폰 상세조회 "+"    "+sAccNo 
														  +"    "+sPerTCd
														  +"    "+sPerTDt
														  +"    "+sCalFun
														  +"    "+sInqCon
														  +"    "+sInqCnt); 
										  
		trace("=============개인정보접촉이력 이벤트 GS&쿠폰 상세조회END==========="); 
		fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);

		if(ds_if_dtls_common.getColumn(0,"HTTP_RET_CODE") == '-1')
		{
			cf_Alert("COME0035", ds_if_dtls_common.getColumn(0,"HTTP_RET_MSG"));				
		}
		else
		{
			if(ds_if_dtls_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
			{
				var value = ds_if_dtls_header.getColumn(0,"ENCOUT");				
					var ansList = value.split("&");  
					
					for(var i=0; i < ansList.length; i++){ 
						var ans = ansList[i].split("=");    
						var strcolumn = ans[0];			  
						var strvalue = ans[1];
						//컬럼 셋팅
						ds_if_dtls_record.addColumn(strcolumn, "string");					
					}
					ds_if_dtls_record.addRow();
					for(var i=0; i < ansList.length; i++){ 
						var ans = ansList[i].split("=");    
						var strcolumn = ans[0];			  
						var strvalue = ans[1];
						//데이터셋팅
						ds_if_dtls_record.setColumn(0 , strcolumn , strvalue);
					}			
					trace(ds_if_dtls_record.saveXML());
					Stt_Dtls_Cnt.text = ds_if_dtls_record.rowcount;					
				
			}
			else
			{
				if(ds_if_dtls_record.rowcount < 1){	
					//그리드 nodatatext로 표기
					Grid01.nodatatext = "조회된 결과가 없습니다.";
				}else{
					cf_Alert("COME0035", ds_if_dtls_header.getColumn(0,"RETURNMSG"));	
				}
			}		
		}
	}else if(strSvcID == "IF_CouponAppDtList"){
	
		trace(ds_if_app_dtls_env.saveXML());
	    trace(ds_if_app_dtls_data.saveXML());
	    trace(ds_if_app_dtls_header.saveXML());
	    trace(ds_if_app_dtls_common.saveXML());
		trace(ds_if_app_dtls_record.saveXML());
		
		trace(ds_if_app_dtls_header.getColumn(0,"RETURNCODE"));
		Stt_App_Cupon_Cnt.text = ds_if_app_dtls_record.rowcount;
			if(ds_if_app_dtls_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
			{	
				if(ds_if_app_dtls_record.rowcount < 1)
				{
					Grid02.nodatatext = "조회된 결과가 없습니다.";
				}	
			}
			else
			{
				
				cf_Alert("COME0035", ds_if_app_dtls_header.getColumn(0,"RETURNMSG"));	
				
			}	
	
	}
}

