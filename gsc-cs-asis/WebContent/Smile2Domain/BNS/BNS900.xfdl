function fn_callBack(svcid,strErrCode,strErrMsg)
{		
	
	if(svcid == "IF_01"){		
		//var httpcd = ds_if_cust_common.getColumn(0,"HTTP_RET_CODE");		
		trace("복원 가능 여부 확인::" + ds_if_cust_record.saveXML());
		trace(ds_if_cust_header.getColumn(0,"OT_RESPON_CODE"));
		if(ds_if_cust_common.getColumn(0,"HTTP_RET_CODE")=="0"){	
			if(ds_if_cust_header.getColumn(0,"OT_RESPON_CODE")=="00000"){
				
				Txa_PrcoCtt.value = ds_if_cust_header.getColumn(0,"OT_RES_MSG")+"\n복원 가능!!";
								
				//복원 가능 고객 이면.
				Btn_Re_Cust_Ins.enable = true ;
								
			}else{
				
				Txa_PrcoCtt.value = ds_if_cust_header.getColumn(0,"OT_RES_MSG");				
			}
		 }	
	}
	
	if(svcid == "IF_02"){		
		if(ds_if_cust_common.getColumn(0,"HTTP_RET_CODE")=="0"){
			if(ds_if_cust_header.getColumn(0,"OT_RESPON_CODE")=="00000" || ds_if_cust_header.getColumn(0,"OT_RESPON_CODE") == "93000"){
						
				trace(ds_if_cust_header.saveXML());
				Txa_PrcoCtt.value = ds_if_cust_header.getColumn(0,"OT_RES_MSG");				
				//복원 완료 후
				Btn_Re_Cust_Ins.enable = false ;
				
				
				//복원 완료 고객 자동 조회
				//상담화면 초기화
				//trace(ds_if_cust_header.saveXML());
				this.parent.opener.Btn_SrchClear_onclick();			
				
				//고객 조회
				//통합 고객 번호 인지 자사 인지 구분 하여 조회				
				if(getTrim(ds_if_cust_header.getColumn(0,"OT_CUST_NO")).substr(0,1) == "G"){				
					this.parent.opener.Edt_SrchIntgCustNo.value = getTrim(ds_if_cust_header.getColumn(0,"OT_CUST_NO"));			
				}else{					
					this.parent.opener.Edt_SrchCustNo.value = getTrim(ds_if_cust_header.getColumn(0,"OT_CUST_NO"));
				}
		
				
				this.parent.opener.Btn_SrchCustInfo_onclick();
				
			}else{
				Txa_PrcoCtt.value = ds_if_cust_header.getColumn(0,"OT_RES_MSG") ;	
			}
		 }	
	}
	if(svcid == "IF_03"){	
		
		if(ds_if_cust_common.getColumn(0,"HTTP_RET_CODE")=="0"){
			if(ds_if_cust_header.getColumn(0,"OT_RESPON_CODE")=="00000"){							
				
			}else{
				Txa_PrcoCtt.value = ds_if_cust_header.getColumn(0,"OT_RES_MSG");
				trace("ds_if_cust_header.getColumn(0,OT_RESPON_CODE)==>"+ds_if_cust_header.getColumn(0,"OT_RESPON_CODE"));
			}
		 }	
	}
}

