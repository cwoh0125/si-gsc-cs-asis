function Cmb_SERCH_KEY_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	
	if(Cmb_SERCH_KEY.value == 1){	
		Edt_Text_18.visible = false; //이름
		Edt_Text_19.visible = true;  //휴대폰
		Edt_Text_20.visible = false; //생년 
		Edt_Text_21.visible = false; //카드
		Edt_Text_22.visible = false; //웹ID	
	}; 
	
	if(Cmb_SERCH_KEY.value == 2) {				
		Edt_Text_18.visible = true;
		Edt_Text_19.visible = false;
		Edt_Text_20.visible = false;
		Edt_Text_21.visible = false;
		Edt_Text_22.visible = false;	
	}; 
	
	
	if(Cmb_SERCH_KEY.value == 3) {	
		Edt_Text_18.visible = false;
		Edt_Text_19.visible = false;
		Edt_Text_20.visible = true;
		Edt_Text_21.visible = false;
		Edt_Text_22.visible = false;	
	}; 
	
	
	if(Cmb_SERCH_KEY.value == 4) {	
		Edt_Text_18.visible = false;
		Edt_Text_19.visible = false;
		Edt_Text_20.visible = false;
		Edt_Text_21.visible = true;
		Edt_Text_22.visible = false;	
	}; 
	
	
	if(Cmb_SERCH_KEY.value == 5) {	
		Edt_Text_18.visible = false;
		Edt_Text_19.visible = false;
		Edt_Text_20.visible = false;
		Edt_Text_21.visible = false;
		Edt_Text_22.visible = true;	
	}; 
	
}
function Btn_Re_Cust_onclick(obj:Button,  e:ClickEventInfo)
{	
	ds_if_cust_env.clearData();
	ds_if_cust_data.clearData();
	ds_if_cust_common.clearData();
	ds_if_cust_header.clearData();
	ds_if_cust_column.clearData();
	ds_if_cust_record.clearData();	

    
    if(Rdo_SrchDiv.value == 1){
		if(Edt_Text_02.value == "" || Edt_Text_03.value == ""){
			cf_Alert("COME0007", "이름,생년월일"); 
			return; 
		}else{			
			fun_ds_srch(ds_if_cust_data, "in_user_name"	, Edt_Text_02.value);
			fun_ds_srch(ds_if_cust_data, "in_birth_day"	, getTrim(Edt_Text_03.value).replace("-","")); 
					
	
			//이름
			if(Cmb_SERCH_KEY.value == 1) { fun_ds_srch(ds_if_cust_data, "in_user_name",getTrim(Edt_Text_19.value)); }	
			//카드번호
			if(Cmb_SERCH_KEY.value == 2) { fun_ds_srch(ds_if_cust_data, "in_hp_no",getTrim(Edt_Text_18.value).replace("-","")); }	
			//생년월일
			if(Cmb_SERCH_KEY.value == 3) { fun_ds_srch(ds_if_cust_data, "in_birth_day",getTrim(Edt_Text_20.value).replace("-","")); }	
			//카드번호
			if(Cmb_SERCH_KEY.value == 4) { fun_ds_srch(ds_if_cust_data, "in_crd_no",getTrim(Edt_Text_21.value).replace("-","")); }	
			//웹ID
			if(Cmb_SERCH_KEY.value == 5) { fun_ds_srch(ds_if_cust_data, "in_web_id",Edt_Text_22.value); }
		}						
    }else if(Rdo_SrchDiv.value == 2){
		if(Edt_Text_00.value == "" || Edt_Text_01.value == "") {			
			cf_Alert("COME0007", "이름,휴대폰");  //이름 또는 휴대폰 을(를) 입력하셔야 합니다.
			return;
		}else{							
			
			fun_ds_srch(ds_if_cust_data, "in_user_name"	, Edt_Text_00.value);
			fun_ds_srch(ds_if_cust_data, "in_hp_no"    	, getTrim(Edt_Text_01.value).replace("-","")); 
			
			if(Cmb_SERCH_KEY.value == 1) { fun_ds_srch(ds_if_cust_data, "in_user_name",getTrim(Edt_Text_19.value)); }	
			if(Cmb_SERCH_KEY.value == 2) { fun_ds_srch(ds_if_cust_data, "in_hp_no",getTrim(Edt_Text_18.value).replace("-","")); }	
			if(Cmb_SERCH_KEY.value == 3) {fun_ds_srch(ds_if_cust_data, "in_birth_day",getTrim(Edt_Text_20.value).replace("-","")); }	
			if(Cmb_SERCH_KEY.value == 4) {fun_ds_srch(ds_if_cust_data, "in_crd_no",getTrim(Edt_Text_21.value).replace("-","")); }	
			if(Cmb_SERCH_KEY.value == 5) {fun_ds_srch(ds_if_cust_data, "in_web_id",Edt_Text_22.value); }
		}
    }else if(Rdo_SrchDiv.value == 3){
		if(Edt_Text_04.value == "" || Edt_Text_05.value == ""){
		  cf_Alert("COME0007", "카드번호,이름"); 
		  return;
		}else{		
