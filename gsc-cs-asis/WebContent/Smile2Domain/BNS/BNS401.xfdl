    <Script type="xscript4.0"><![CDATA[/********************************************************************************** 
 *****	상담관리 - 회원카드목록(제휴회원카드,RF회원카드)
***********************************************************************************/

include "LIB::CommonLib.xjs";

var SVC_IF_URL 	= "XinterfaceAction.do?method=CommonInterface";
var objDsCust 	= this.parent.parent.all["ds_cust_info"];
var objCntStt	= this.parent.parent.all["Stt_CrdInfoCnt"];	

function fn_srchFormData()
{		
	// 상담관리에서 조회한 고객이 없을 경우
	if(objDsCust.getColumn(0,"IO_CUST_NO") == ""){
		fn_clearFormData();
		return;
	}
	
	//그리드 중첩으로 인한 사용안함
	objCntStt.text = "0";
	
	var In_cust_no = objDsCust.getColumn(0,"IO_CUST_NO");
	
	//제휴회원목록 IF_FN
	fn_affiMemList(In_cust_no);
	
}

function fn_clearFormData()
{
	ds_affimem_card_info_record.clearData();
}

//제휴회원목록 IF
function fn_affiMemList(In_cust_no) {

	//초기화
	ds_if_ev_affimem_env.clearData();
	ds_if_ev_affimem_data.clearData();
	ds_if_ev_affimem_header.clearData();
	ds_if_ev_affimem_column.clearData();
	ds_if_ev_affimem_common.clearData();
	ds_affimem_card_info_record.clearData();
	
	
	//IF_BNS260_01 환경설정
	fun_ds_srch(ds_if_ev_affimem_env, "IF_KIND", 				"WAS-EVAPP"); //IF 종류
	fun_ds_srch(ds_if_ev_affimem_env, "REQ_SERVICE_METHOD", 	"customer/affiMemcardInfo"); //인터페이스URL
	fun_ds_srch(ds_if_ev_affimem_env, "REQ_SERVICE_ID", 		"");
	fun_ds_srch(ds_if_ev_affimem_env, "RES_HEADER_SECTION", 	"affiMemcardInfo_Rsp"); //xml상위 헤더항목
	fun_ds_srch(ds_if_ev_affimem_env, "RES_COLUMN_SECTION", 	""); 
	fun_ds_srch(ds_if_ev_affimem_env, "RES_RECORD_SECTION", 	"affiMemcardInfo"); 
	
	
		
	fun_ds_srch(ds_if_ev_affimem_data, "custNo", 		In_cust_no);
	
	
	var	strSvcID = "IF_CSC_CCD_001"; //인터페이스ID
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_ev_affimem_env:a INTERFACE_DATA=ds_if_ev_affimem_data:a"; 
	var	strOutDatasets = "ds_if_ev_affimem_header=DS_HEADER ds_if_ev_affimem_column=DS_COLUMN ds_affimem_card_info_record=DS_RECORD ds_if_ev_affimem_common=DS_COMMON";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);		
}


//콜백함수
function fn_callBack(svcid,strErrCode,strErrMsg){

	if(svcid == "IF_CSC_CCD_001"){
		trace(ds_affimem_card_info_record.saveXML());
		//건수표시
		objCntStt.text	= ds_affimem_card_info_record.rowcount;
		for(var i = 0; i < ds_affimem_card_info_record.rowcount; i++) {
			if(ds_affimem_card_info_record.getColumn(i,'EVPARTNERCD') == 0002) {
				ds_affimem_card_info_record.setColumn(i, "RFCARDNM", "기아_화이트");
			} else if(ds_affimem_card_info_record.getColumn(i,'EVPARTNERCD') == 0003) {
				ds_affimem_card_info_record.setColumn(i, "RFCARDNM", "기아_블랙");
			} 
		}
		
	}
}


function Grd_CrdInfo_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	cf_gridSort(obj, e);
}

// 셀내용복사
function Grd_CrdInfo_onkeydown(obj:Grid, e:KeyEventInfo)
{
	if(e.ctrlKey){
		if(e.keycode == 67){
			var v_clip;
			v_clip = obj.getCellValue(obj.currentrow,obj.currentcol);
			system.clearClipboard();
			system.setClipboard("CF_TEXT",v_clip);
		}	
	}	
}

// 카드혜택조회
function Grd_CrdInfo_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	ds_get_param.clearData();
	ds_set_param.clearData();

	fun_ds_srch(ds_get_param, "RFCARDNO", getTrim(ds_affimem_card_info_record.getColumn(e.row,"RFCARDNO"))); //회원카드목록
	
	var arrParam = {getDsArgument:ds_get_param, setDsArgument:ds_set_param};
	
	PopupDialog("BNS403","BNS::BNS403.xfdl", 393, 23, 816, 220, false, "absolute", arrParam, false, false, true, false);
}
