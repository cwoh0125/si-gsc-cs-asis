		fun_ds_srch(ds_if_env, "IF_KIND", 				"GSC-APP");
		fun_ds_srch(ds_if_env, "REQ_SERVICE_METHOD", 	"SendHist");
		fun_ds_srch(ds_if_env, "REQ_SERVICE_ID", 		"");
