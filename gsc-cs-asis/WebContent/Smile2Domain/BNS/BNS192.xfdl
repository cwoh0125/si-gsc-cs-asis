		if(getTrim(g_IntgCustNo)== "" ){				
			alert("통합 고객 번호 확인이 필요 합니다.");			
			return;
		}else{
			fun_ds_srch(ds_if_data, "user_key", 	g_IntgCustNo);//통합고객번호
		}
		
