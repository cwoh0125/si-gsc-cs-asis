		}		
	}
	// 부정 선택시 
	//20151111 허선화p 적립불가 체크부분 빼달라 
	/*20151111 이전은
	if(Cmb_WrngCustDiv.value == "02"){
	 if(Chk_ImposSave.value == false || Chk_ImposUse.value == false){
			cf_Alert("BNSE0027");
			Chk_ImposSave.value = true;
			Chk_ImposUse.value	= true;			
			return;
	 }
	}
    */
	if(Cmb_WrngCustDiv.value == "02"){
