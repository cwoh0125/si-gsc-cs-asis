		edt_chkYn.style.color 	= "orangered";
		edt_chkYn.style.font = "Dotum,8,bold";
	}
	
	Btn_authNoPass.enable = true;	//강제인증은 무조건 활성화
	Btn_excute.enable = false;		//고객합산버튼은 무조건 비활성화
}

