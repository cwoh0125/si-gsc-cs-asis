// 상세조회 후 데이터 형식 수정
//==========================================================================
function fn_fixFormCustInfo()
{
	// 전화번호 및 포인트정보 세팅
	var str_RsBzNo 		= getTrim(ds_m_cust_info.getColumn(0,"OT_HEAD_RS_BZ_NO"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_TAIL_RS_BZ_NO"));	
	var str_HomePhn		= getTrim(ds_m_cust_info.getColumn(0,"OT_HOME_PHONE_DDD")) + getTrim(ds_m_cust_info.getColumn(0,"OT_HOME_PHONE_FSTNO")) + getTrim(ds_m_cust_info.getColumn(0,"OT_HOME_PHONE_LASTNO"));
	var str_OffPhn		= getTrim(ds_m_cust_info.getColumn(0,"OT_OFF_PHONE_DDD"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_OFF_PHONE_FSTNO"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_OFF_PHONE_LASTNO"));
	var str_HndPhn		= getTrim(ds_m_cust_info.getColumn(0,"OT_HND_PHONE_DDD"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_HND_PHONE_FSTNO"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_HND_PHONE_LASTNO"));
	var str_FaxPhn		= getTrim(ds_m_cust_info.getColumn(0,"OT_FAX_PHONE_DDD"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_FAX_PHONE_FSTNO"))  + getTrim(ds_m_cust_info.getColumn(0,"OT_FAX_PHONE_LASTNO"));	
	var strCurPtSet    	= "";
	var strBfIntgPtSet 	= "";
		
	// 일반
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_GNRL_PT")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_GNRL_PT")) != "0"){
		strCurPtSet = strCurPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_GNRL_PT")) + " / ";
	}else{
		strCurPtSet = strCurPtSet + "0" + " / ";	
	}
	// 특별
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_SPCL_PT")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_SPCL_PT")) != "0"){
		strCurPtSet = strCurPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_SPCL_PT")) + " / ";
	}else{
		strCurPtSet = strCurPtSet + "0" + " / ";
	}		
	// 제휴
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_CPRT_PT")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_CPRT_PT")) != "0"){
		strCurPtSet = strCurPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_CPRT_PT"));
	}else{
		strCurPtSet = strCurPtSet + "0";		
	}

	// GSC
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSC")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSC")) != "0"){
		strBfIntgPtSet = strBfIntgPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSC")) + " / ";
	}else{
		strBfIntgPtSet = strBfIntgPtSet + "0" + " / ";	
	}
	// GSH
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSH")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSH")) != "0"){
		strBfIntgPtSet = strBfIntgPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSH")) + " / ";
	}else{
		strBfIntgPtSet = strBfIntgPtSet + "0" + " / ";	
	}
	// GSR
	if(getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSR")) != "" && getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSR")) != "0"){
		strBfIntgPtSet = strBfIntgPtSet + getTrim(ds_m_cust_info.getColumn(0,"OT_BF_INTG_PT_GSR"));
	}else{
		strBfIntgPtSet = strBfIntgPtSet + "0";	
	}	
	
	ds_m_cust_info.setColumn(0,"OT_RS_BZ_NO",str_RsBzNo);	
	ds_m_cust_info.setColumn(0,"OT_HOME_PHONE_NO",maskPhone(str_HomePhn));
	ds_m_cust_info.setColumn(0,"OT_OFF_PHONE_NO",maskPhone(str_OffPhn));
	ds_m_cust_info.setColumn(0,"OT_HND_PHONE_NO",maskPhone(str_HndPhn));
	ds_m_cust_info.setColumn(0,"OT_FAX_PHONE_NO",maskPhone(str_FaxPhn));
	ds_m_cust_info.setColumn(0,"OT_CUR_PT_SET",strCurPtSet);
	ds_m_cust_info.setColumn(0,"OT_BF_INTG_PT_SET",strBfIntgPtSet);
}

