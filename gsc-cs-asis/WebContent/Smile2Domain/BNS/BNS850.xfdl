function fn_callBack(svcid,strErrCode,strErrMsg)
{	
	//======================
	//★SMS채번/발송후 처리
	if(svcid == "SMS_AUTH_SEND"){    
		Btn_ARSAuthNo.enable = true;	//ARS인증번호 활성화
	
		//SMS인증발송 목록 조회
		ds_SMS_AUTHN_NO_HIST.clearData();	//조회전 클리어
		//alert(edt_Auth_telNo.value);
		fn_SrchSmsAuthSeqNo(sGenDv, edt_Auth_telNo.value);
		return;
	}
	
	//======================
	//★SMS발송목록조회
	if(svcid == "SMS_AUTH_SRCH"){    	
		
		//금일 해당화면에서 발송한 내역이 있으면 ARS확인버튼 활성화
		if(ds_SMS_AUTHN_NO_HIST.rowcount > 0){
			Btn_ARSAuthNo.enable = true;
		}
		return;
	}
	
	
	//======================
	// 고객DW등급조회
	if(svcid == "IF_CSC_ITRT_043"){
		var strHttpCd 	= ds_if_class_sel_common.getColumn(0,"HTTP_RET_CODE");
		var strHttpMsg 	= ds_if_class_sel_common.getColumn(0,"HTTP_RET_MSG");
		
		if(strHttpCd=="0"){
			var strRtnCd	= ds_if_class_sel_header.getColumn(0,"OT_RESPON_CODE");
			var strRtnMsg	= ds_if_class_sel_header.getColumn(0,"OT_RES_MSG");
			if(strRtnCd=="00000"){
				// 등급세팅
				fn_setCustCls();
			}else{
//				cf_Alert("COME0035",strRtnMsg);	
			}			
		}else{
//			cf_Alert("COME0035",strHttpMsg);
		}	
	}
	
	
	//======================
	// 고객정보상세조회
	if(svcid == "IF_CSC_ITRT_024"){	
		trace(">>>>>>>>>>>>>>>>>"+ds_if_cust_dtl_record.saveXML());
		if(ds_if_cust_dtl_common.getColumn(0,"HTTP_RET_CODE")=="0"){
			if(ds_if_cust_dtl_record.getColumn(0,"OT_RESPON_CODE")=="00000"){
				if(ds_if_cust_dtl_record.rowcount < 1){
					cf_Alert("COMI0006");					
					return;	
				}else{				
					// 삭제고객은 조회하지 않음.					
					if(ds_if_cust_dtl_record.getColumn(0,"OT_CUST_ST_CD") == "04"){
						cf_Alert("BNSE0046");						
						return;
					}else if(ds_if_cust_dtl_record.getColumn(0,"OT_CUST_ST_CD") == "03"){
						alert("탈퇴한 고객입니다");						
						return;
					}else{				
						
						
						ds_m_cust_info.copyData(ds_if_cust_dtl_record,false);
												
						ds_m_cust_info.getColumn();						
												
						var objNum1 = new Number(edt_s_totPt.value);
						var objNum2 = new Number(edt_m_totPt.value);

						edt_totPt_Sum.value = objNum1+ objNum2;
						//fn_setCustInfo();			// 고객상세정보세팅
						fn_fixFormCustInfo();	// 고객정보 형식변경
					}								
				}
			}else{
				cf_Alert("COME0035","[고객상세]" + ds_if_cust_dtl_record.getColumn(0,"OT_RES_MSG"));	
			}			
		}else{
			cf_Alert("COME0035","[고객상세]" + ds_if_cust_dtl_common.getColumn(0,"HTTP_RET_MSG"));
		}
	}
	
	//======================
	//고객통합
	if(svcid == "IF_WAS_309"){
		var strHttpCd 	= ds_if_custintg_ins_common.getColumn(0,"HTTP_RET_CODE");
		var strHttpMsg 	= ds_if_custintg_ins_common.getColumn(0,"HTTP_RET_MSG");

		if(strHttpCd=="0"){
			var strRtnCd	= ds_if_custintg_ins_header.getColumn(0,"OT_RESPON_CODE");
			var strRtnMsg	= ds_if_custintg_ins_header.getColumn(0,"OT_RES_MSG");			
			
			if(strRtnCd=="00000"){
				var getDsName  = this.getOwnerFrame().getCustDsArgument;
				
				fn_saveBizProc();	//업무처리정보 등록
				this.opener.fn_srchCustDtlsAftrProc(edt_m_custNo.value);	//통합후 재조회(통합되는 고객으로)
				
			
				cf_Alert("TSSI0007","고객통합");		
			}else{
				cf_Alert("COME0035",strRtnMsg);	
			}			
		}else{
			cf_Alert("COME0035",strHttpMsg);
		}
	}
	
	//======================
	//고객통합제약초기화
	if(svcid == "IF_WAS_310"){
		var strHttpCd 	= ds_if_intgrestr_ins_common.getColumn(0,"HTTP_RET_CODE");
		var strHttpMsg 	= ds_if_intgrestr_ins_common.getColumn(0,"HTTP_RET_MSG");
		
		//2015.08.15 개인정보접촉이력
		trace("=============개인정보접촉이력  회원통합제약초기화 Start===========");
		
		var sAccNo = "BNS850"; //개인정보접촉이력 일련번호
		var sPerTCd = "PT1003";  // 수행업무 코드
		var sPerTDt = "ICCS_CSC 회원통합 제약초기화";  // 수행업무 상세내용
		var sCalFun = "fn_IntgrestrIns()";  // 호출함수명 
		var sInqCon = "|" + "input_user_id="+getTrim(GetUserInfo("gUSR_ID"))
