function Grid_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	cf_gridSort(obj, e);	
}

function BNS221_onload(obj:Form, e:LoadEventInfo){

	var getDsName = this.getOwnerFrame().getDsArgument;		
	
	g_CustNo 			= getDsName.getColumn(0,"CUST_NO");			//고객번호 "9999999998";//
	
	
//	Cal_StartDt.value = AddMonth(getToday(), -3);
	Cal_StartDt.value = getToday();
	Cal_StartDt00.value = getToday();
	
	Edt_CustNo.value = g_CustNo;
	Edt_CustNo00.value = g_CustNo;
	
	
	
}

/********************************************************************************** 
 *****	조회
***********************************************************************************/
function Btn_frSrch_onclick(obj:Button,  e:ClickEventInfo)
{
	ds_if_recomm_env.clearData();
	ds_if_recomm_data.clearData();
	ds_if_recomm_header.clearData();
	ds_if_recomm_record.clearData();
	ds_if_recomm_common.clearData();
	
	if (cf_isNull(Edt_CustNo.value)) {
		cf_Alert("COME0003", "고객번호");  //msgCont 은(는) 필수입니다.
		Edt_CustNo.setFocus();
		return false;
	}	

	
	
	//IF_BNS220_01 환경설정
	fun_ds_srch(ds_if_recomm_env, "IF_KIND", 				"WAS-APP");
	fun_ds_srch(ds_if_recomm_env, "REQ_SERVICE_METHOD", 	"event/recomm-status");
	fun_ds_srch(ds_if_recomm_env, "REQ_SERVICE_ID", 		"");
	fun_ds_srch(ds_if_recomm_env, "RES_HEADER_SECTION", 	"recommStatus_Rsp");
	fun_ds_srch(ds_if_recomm_env, "RES_COLUMN_SECTION", 	"");
	fun_ds_srch(ds_if_recomm_env, "RES_RECORD_SECTION", 	"recommStatus");
	
	//IF_BNS220_01 INPUT DATA
	fun_ds_srch(ds_if_recomm_data, "custNo", 	getTrim(Edt_CustNo00.value));
	fun_ds_srch(ds_if_recomm_data, "searchDate", 	getTrim(Cal_StartDt00.value));

	trace(ds_if_recomm_data.saveXML());
	
	var	strSvcID = "IF_recommList"; //IF_EventEntryWinList
	var	strURL = SVC_IF_URL;	
	var	strInDatasets = "INTERFACE_ENV=ds_if_recomm_env:a INTERFACE_DATA=ds_if_recomm_data:a";
	var	strOutDatasets = "ds_if_recomm_header=DS_HEADER ds_if_recomm_record=DS_RECORD ds_if_recomm_common=DS_COMMON";
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_recommList";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);		
}


//------------------------------------------------------------------------------------------
//친구추천이벤트  조회콜백(IF)
//------------------------------------------------------------------------------------------

function fn_callBack_recommList(svcid,strErrCode,strErrMsg){


if(svcid == "IF_recommList"){
	trace(">>>>>>>>>>>>>>>>>>>>>>>>>>>>");		
		trace("친구추천::"+ds_if_recomm_record.saveXML());

		
		//2015.08.15 개인정보접촉이력
		trace("=============개인정보접촉이력  이벤트 E+친구추천 조회Start===========");

		var sAccNo = "BNS221"; //개인정보접촉이력 일련번호
		var sPerTCd = "PT1002";  // 수행업무 코드
		var sPerTDt = "ICCS_CSC 자사앱 친구추천이벤트";  // 수행업무 상세내용
		var sCalFun = "Btn_Srch_onclick()";  // 호출함수명 
		var sInqCon = "|" + "CUSTNO="+getTrim(Edt_CustNo.value)			
					+ "|" ;  // 조회조건 
					
		var sInqCnt = ds_if_recomm_record.rowcount;  // 조회건수
		
		var sInCustNo = ds_if_recomm_data.getColumn(0,"EVNTCD") ;//식별키 이벤트 코드
		
		trace("개인정보접촉이력--이벤트 LMP 조회데이터 "+"    "+sAccNo 
														  +"    "+sPerTCd
														  +"    "+sPerTDt
														  +"    "+sCalFun
														  +"    "+sInqCon
														  +"    "+sInqCnt); 
										  
		trace("=============개인정보접촉이력 이벤트 LMP 조회수END==========="); 
		//fn_Privcnthis(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt);
		fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);	
		if(ds_if_recomm_common.getColumn(0 , "HTTP_RET_CODE") == "0"){
			if(ds_if_recomm_header.getColumn(0, "RESULTCODE") == "1999"){
				ds_if_recomm_header.clearData();
				alert("해당일자의 이벤트 정보가 없습니다.");
				
			}else{
				Stt_Cnt.text = ds_if_recomm_record.rowcount;		
			}
		
		}else{
			alert(ds_if_recomm_common.getColumn(0 , "HTTP_RET_MSG"));
		}
	}
}

