//스템프 이벤트  조회콜백(IF)
//------------------------------------------------------------------------------------------

function fn_callBack_stampList(svcid,strErrCode,strErrMsg){


	if(svcid == ""){
		
		trace(ds_if_stamp_record00.saveXML());		
		
	}else{


		trace("스탬프이벤트");
		trace(ds_if_stamp_record.saveXML());
	
		
		
		//2015.08.15 개인정보접촉이력
		trace("=============개인정보접촉이력  이벤트 스탬프 조회Start===========");

		var sAccNo = "BNS221"; //개인정보접촉이력 일련번호
		var sPerTCd = "PT1002";  // 수행업무 코드
		var sPerTDt = "ICCS_CSC 자사앱 스탬프 이벤트";  // 수행업무 상세내용
		var sCalFun = "Btn_Srch_onclick()";  // 호출함수명 
		var sInqCon = "|" + "CUSTNO="+getTrim(Edt_CustNo00.value)			
					+ "|" ;  // 조회조건 
					
		var sInqCnt = ds_if_stamp_record.rowcount;  // 조회건수
		
		var sInCustNo = ds_if_stamp_data.getColumn(0,"EVNTCD") ;//식별키 이벤트 코드
		
		trace("개인정보접촉이력--이벤트 LMP 조회데이터 "+"    "+sAccNo 
														  +"    "+sPerTCd
														  +"    "+sPerTDt
														  +"    "+sCalFun
														  +"    "+sInqCon
														  +"    "+sInqCnt); 
										  
		trace("=============개인정보접촉이력 이벤트 LMP 조회수END==========="); 
		//fn_Privcnthis(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt);
		fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);	
		
		
		
			
		if(ds_if_stamp_common.getColumn(0,"HTTP_RET_CODE") == '-1'){				
			cf_Alert("COME0035", ds_if_stamp_common.getColumn(0,"HTTP_RET_MSG"));									
		}
				
		Stt_Cnt00.text = ds_if_stamp_record.rowcount;	
	}
}




