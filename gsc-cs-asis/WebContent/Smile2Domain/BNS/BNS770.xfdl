//스마트포인트 취소
function fn_SmartCancel(){
	
	ds_if_cancel_env.clearData();
	ds_if_cancel_data.clearData();
	ds_if_cancel_header.clearData();	
	ds_if_cancel_common.clearData();
	ds_if_cancel_record.clearData();
	
	if(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMSTS") == "N") return;    //결제상태:N
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"CUSTNO"), 		        "고객번호"))return; //고객번호
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"CLICOISSUPAYMNO"), 		"주문번호"))return; //주문번호
	if(!cf_chk_data(Txa_MESE_CTT.value, 															        "취소사유"))return; //취소사유
	
	if(!cf_size_Alert2(Txa_MESE_CTT.value, Txa_MESE_CTT ,2000)) return;	
		
	if (!cf_Confirm("COMI0024"))	return;  //취소하시겠습니까?
			
	fun_ds_srch(ds_if_cancel_env, "IF_KIND",           "MPP");
	fun_ds_srch(ds_if_cancel_env, "REQ_SERVICE_METHOD","CRMIF/CRMIF_CuponCancel.aspx");
	fun_ds_srch(ds_if_cancel_env, "REQ_SERVICE_ID",    "");	
	fun_ds_srch(ds_if_cancel_env, "RES_HEADER_SECTION","CRMIF_COUPONCANCEL_RESULT");
	fun_ds_srch(ds_if_cancel_env, "RES_COLUMN_SECTION","");
	fun_ds_srch(ds_if_cancel_env, "RES_RECORD_SECTION","RSV_RSLT");
		
	
	//고객번호, 주문번호, 취소사유, 취소요청상담사
	fun_ds_srch(ds_if_cancel_data, "MUID", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CUSTNO"));
	fun_ds_srch(ds_if_cancel_data, "ORDERCODE", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CLICOISSUPAYMNO"));
	fun_ds_srch(ds_if_cancel_data, "CANCELMEMO", Txa_MESE_CTT.value);
	//fun_ds_srch(ds_if_cancel_data, "CANCELMEMO", "테스트");
	fun_ds_srch(ds_if_cancel_data, "CANCELTM", GetUserInfo("gUSR_ID"));
				
	
	//거래시작	조회
	var	strSvcID = "IF_CouponCancel_Smart"; //IF_CouponDtList
	var	strURL = SVC_IF_URL;
	var	strInDatasets = "INTERFACE_ENV=ds_if_cancel_env:a INTERFACE_DATA=ds_if_cancel_data:a";	
	var	strOutDatasets = "ds_if_cancel_common=DS_COMMON ds_if_cancel_header=DS_HEADER ds_if_cancel_record=DS_RECORD";	
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_SmartCancel";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}

function fn_callBack_SmartCancel(){
	if(ds_if_cancel_common.getColumn(0,"HTTP_RET_CODE") == '-1'){
		cf_Alert("COME0035", ds_if_cancel_common.getColumn(0,"HTTP_RET_MSG"));				
	}else{
	    trace("스마트취소>>>>>>>>>>>>"+ds_if_cancel_record.saveXML());
	
		if(ds_if_cancel_record.getColumn(0,"RESULT_CODE") == '0001'){
		    Btn_srch_onclick();				//재조회
		}else{
		    cf_Alert("COME0035", ds_if_cancel_record.getColumn(0,"RESULT_MSG"));
		}
	}
}


//GS쿠폰취소
function fn_GsCopunCanCel(){
	
	ds_if_gscancel_env.clearData();
	ds_if_gscancel_data.clearData();
	ds_if_gscancel_header.clearData();	
	ds_if_gscancel_common.clearData();
	ds_if_gscancel_record.clearData();
	
	if(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMSTS") == "N" 
	   || ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMSTS") == "C" ) return;                                   //결제상태:N
	if(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"USEYN") == "Y") return;                                         //사용여부:Y
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"CUSTNO"), 		        "고객번호"))return;      //고객번호
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"ISSUREQCD"), 		    "상품코드"))return;      //상품코드
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"CLICOISSUPAYMNO"), 	    "주문번호"))return;      //주문번호
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMQTY"), 		        "수량"))    return;       //수량
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMAMT"), 		        "결제금액"))return;      //결제금액
	if(!cf_chk_data(ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition,"PAYMDT"), 		        "입금확인일자"))return; //입금확인일자
		
		
	if (!cf_Confirm("COMI0024"))	return;  //취소하시겠습니까?
			
	fun_ds_srch(ds_if_gscancel_env, "IF_KIND",           "GSCP");
	fun_ds_srch(ds_if_gscancel_env, "REQ_SERVICE_METHOD","COOP_IF_AccountCancelRequest");
	fun_ds_srch(ds_if_gscancel_env, "REQ_SERVICE_ID",    "");	
	fun_ds_srch(ds_if_gscancel_env, "RES_HEADER_SECTION","return");
	fun_ds_srch(ds_if_gscancel_env, "RES_COLUMN_SECTION","");
	fun_ds_srch(ds_if_gscancel_env, "RES_RECORD_SECTION","");
		
	
	//고객번호, CP사코드, 상품코드(발급요청코드), 주문고유코드, 수량, 결제금액, 입금확인일자, 사용자ID(20150120 추가)
	fun_ds_srch(ds_if_gscancel_data, "Muid", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CUSTNO"));
	fun_ds_srch(ds_if_gscancel_data, "Puid", "101");
	fun_ds_srch(ds_if_gscancel_data, "Suid", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "ISSUREQCD"));
	fun_ds_srch(ds_if_gscancel_data, "Ordercode", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CLICOISSUPAYMNO"));
	fun_ds_srch(ds_if_gscancel_data, "Bidqnt", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMQTY"));
	fun_ds_srch(ds_if_gscancel_data, "Payamt", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMAMT"));
	fun_ds_srch(ds_if_gscancel_data, "Payokdate", ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMDT"));
	fun_ds_srch(ds_if_gscancel_data, "UserId", getTrim(GetUserInfo("gUSR_ID")));
				
	trace("고객번호>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CUSTNO"));
	trace("발급요청코드>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "ISSUREQCD"));
	trace("주문번호>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CLICOISSUPAYMNO"));
	trace("수량>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMQTY"));
	trace("결제금액>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMAMT"));
	trace("입금확인일자>>>>>>>"+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "PAYMDT"));
	trace("사용자ID>>>>>>>"+GetUserInfo("gUSR_ID"));
	trace(ds_if_gscancel_data.saveXML());
		
	//거래시작	조회
	var	strSvcID = "IF_CouponCancel_Gs"; //IF_CouponDtList
	var	strURL = SVC_IF_URL;
	var	strInDatasets = "INTERFACE_ENV=ds_if_gscancel_env:a INTERFACE_DATA=ds_if_gscancel_data:a";	
	var	strOutDatasets = "ds_if_gscancel_common=DS_COMMON ds_if_gscancel_header=DS_HEADER ds_if_gscancel_record=DS_RECORD";	
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack_GsCancel";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}

