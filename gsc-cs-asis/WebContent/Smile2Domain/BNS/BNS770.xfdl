	if(ds_if_dtls_common.getColumn(0,"HTTP_RET_CODE") == '-1')
	{
		cf_Alert("COME0035", ds_if_dtls_common.getColumn(0,"HTTP_RET_MSG"));				
	}
	else
	{
		trace(ds_if_coupondtls_record.saveXML());
		trace(ds_if_dtls_header.saveXML());
		Stt_Dtls_Cnt.text = ds_if_coupondtls_record.rowcount;
		if(ds_if_dtls_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
		{	
			if(ds_if_coupondtls_record.rowcount < 1)
			{
				cf_Alert("COMI0006");		
			}
			
			for(var i= 0; i < ds_if_coupondtls_record.rowcount; i++)
			{
				ds_if_coupondtls_record.setColumn(i,"RCVRTELNO",maskPhone(ds_if_coupondtls_record.getColumn(i,"RCVRTELNO")));
				ds_if_coupondtls_record.setColumn(i,"CALLBKNO",maskPhone(ds_if_coupondtls_record.getColumn(i,"CALLBKNO")));
			}
		}
		else
		{
			cf_Alert("COME0035", ds_if_dtls_header.getColumn(0,"RETURNMSG"));	
			
		}
			
	}
}

