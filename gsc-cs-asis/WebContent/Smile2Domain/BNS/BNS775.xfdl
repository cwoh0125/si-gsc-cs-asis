function Coupon_ApprList(){

		ds_if_ev_appr_env.clearData();
	    ds_if_ev_appr_data.clearData();
	    ds_if_ev_appr_header.clearData();	
	    ds_if_ev_appr_common.clearData();
		ds_if_ev_appr_column.clearData();
		
		//사용내역 초기화
		ds_if_ev_appr_record.clearData();
		
		
		fun_ds_srch(ds_if_ev_appr_env, "IF_KIND",           "GSCPDTL-APP");
		fun_ds_srch(ds_if_ev_appr_env, "REQ_SERVICE_METHOD","GSCMPCouponSearch");
		fun_ds_srch(ds_if_ev_appr_env, "REQ_SERVICE_ID",    "");	
		fun_ds_srch(ds_if_ev_appr_env, "RES_HEADER_SECTION","return");
		fun_ds_srch(ds_if_ev_appr_env, "RES_COLUMN_SECTION","");
		fun_ds_srch(ds_if_ev_appr_env, "RES_RECORD_SECTION","coupApproList");
			
		fun_ds_srch(ds_if_ev_appr_data, "Req_Div_Cd", "01"); //발급요청구분코드(01:GSMbiz상품코드)
		fun_ds_srch(ds_if_ev_appr_data, "Issu_Req_Val", ds_if_evcoup_record.getColumn(ds_if_evcoup_record.rowposition, "PROD_CD")); //발급요청코드
		fun_ds_srch(ds_if_ev_appr_data, "Search_Div", "01"); //조회 구분 01:쿠폰번호 02:고객사발급결제번호,순번
		fun_ds_srch(ds_if_ev_appr_data, "Receive_Div", "99"); //결과 구분 01:기본 02:기본+발송내역 03:기본+승인 99:전체
		fun_ds_srch(ds_if_ev_appr_data, "Cupn_No", ds_if_evcoup_record.getColumn(ds_if_evcoup_record.rowposition, "CUPN_NO")); //쿠폰번호	
		
		trace(">>>>>>>>>>>>"+ds_if_ev_appr_data.saveXML());
		
		
		//거래시작	조회
		var	strSvcID = "IF_CouponApprList"; 
		var	strURL = SVC_IF_URL;
		var	strInDatasets = "INTERFACE_ENV=ds_if_ev_appr_env:a INTERFACE_DATA=ds_if_ev_appr_data:a";	
		var	strOutDatasets = "ds_if_ev_appr_common=DS_COMMON ds_if_ev_appr_header=DS_HEADER ds_if_ev_appr_column=DS_COLUMN ds_if_ev_appr_record=DS_RECORD";	
		var	strArgument = "";
		var strCallbackFunc = "fn_callBack";
		var strProcessBar = true;
		
		cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	


}


function fn_callBack_dtls(strSvcID){


	if(strSvcID == "IF_CouponDtList"){
	
		//2015.08.15 개인정보접촉이력
		trace("=============개인정보접촉이력  이벤트 GS&쿠폰 상세조회Start===========");

		var sAccNo = "BNS770"; //개인정보접촉이력 일련번호
		var sPerTCd = "PT1002";  // 수행업무 코드
		var sPerTDt = "ICCS_CSC 이벤트 GSC APP 쿠폰 상세조회";  // 수행업무 상세내용
		var sCalFun = "Coupon_DtList()";  // 호출함수명 
		var sInqCon = "|" + "CupnNo="+ds_if_coupon_record.getColumn(ds_if_coupon_record.rowposition, "CUPN_NO")				
					+ "|" ;  // 조회조건 
			
		var sInqCnt = ds_if_dtls_record.rowcount;  // 조회건수
		var sInCustNo = ds_if_dtls_record.getColumn(ds_if_dtls_record.rowposition, "CUPN_NO") ;//식별키		
		
		trace("============쿠폰상세===================");
		trace(ds_if_dtls_env.saveXML());	  
	    trace(ds_if_dtls_header.saveXML());		
		trace(ds_if_dtls_record.saveXML());	
		trace("===============================");
		//사용내역 초기화
		ds_if_app_dtls_record.clearData();
									
		trace("개인정보접촉이력--이벤트 GS&쿠폰 상세조회 "+"    "+sAccNo 
														  +"    "+sPerTCd
														  +"    "+sPerTDt
														  +"    "+sCalFun
														  +"    "+sInqCon
														  +"    "+sInqCnt); 
										  
		trace("=============개인정보접촉이력 이벤트 GS&쿠폰 상세조회END==========="); 
		fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);

		if(ds_if_dtls_common.getColumn(0,"HTTP_RET_CODE") == '-1')
		{
			cf_Alert("COME0035", ds_if_dtls_common.getColumn(0,"HTTP_RET_MSG"));				
		}
		else
		{
			if(ds_if_dtls_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
			{
				var value = ds_if_dtls_header.getColumn(0,"ENCOUT");				
					var ansList = value.split("&");  
					
					for(var i=0; i < ansList.length; i++){ 
						var ans = ansList[i].split("=");    
						var strcolumn = ans[0];			  
						var strvalue = ans[1];
						//컬럼 셋팅
						ds_if_dtls_record.addColumn(strcolumn, "string");					
					}
					ds_if_dtls_record.addRow();
					for(var i=0; i < ansList.length; i++){ 
						var ans = ansList[i].split("=");    
						var strcolumn = ans[0];			  
						var strvalue = ans[1];
						//데이터셋팅
						ds_if_dtls_record.setColumn(0 , strcolumn , strvalue);
					}			
					trace(ds_if_dtls_record.saveXML());
					Stt_Dtls_Cnt.text = ds_if_dtls_record.rowcount;					
				
			}
			else
			{
				if(ds_if_dtls_record.rowcount < 1){	
					//그리드 nodatatext로 표기
					Grid01.nodatatext = "조회된 결과가 없습니다.";
				}else{
					cf_Alert("COME0035", ds_if_dtls_header.getColumn(0,"RETURNMSG"));	
				}
			}		
		}
	}else if(strSvcID == "IF_CouponAppDtList"){
	
		trace(ds_if_app_dtls_env.saveXML());
	    trace(ds_if_app_dtls_data.saveXML());
	    trace(ds_if_app_dtls_header.saveXML());
	    trace(ds_if_app_dtls_common.saveXML());
		trace(ds_if_app_dtls_record.saveXML());
		
		trace(ds_if_app_dtls_header.getColumn(0,"RETURNCODE"));
		Stt_App_Cupon_Cnt.text = ds_if_app_dtls_record.rowcount;
			if(ds_if_app_dtls_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
			{	
				if(ds_if_app_dtls_record.rowcount < 1)
				{
					Grid02.nodatatext = "조회된 결과가 없습니다.";
				}	
			}
			else
			{
				
				cf_Alert("COME0035", ds_if_app_dtls_header.getColumn(0,"RETURNMSG"));	
				
			}	
	
	}
}

function Edt_Phone_ontextchanged(obj:Edit,  e:TextChangedEventInfo)
{
	obj.value = maskPhone(e.posttext);
	obj.setCaretPos(-1);	
}


function Today()
{
	var sToday = "";
	var objDate = new Date();
	var sToday  = objDate.getFullYear().toString();
	sToday += Right("0" + (objDate.getMonth() + 1), 2);
	sToday += Right("0" + objDate.getDate(), 2);

	return sToday;
}	


