	var getDsName = this.getOwnerFrame().getDsArgument;		
	
	
	Edt_CustNo.value	= getDsName.getColumn(0,"INTG_CUST_NO");
	Edt_CustNo00.value   = getDsName.getColumn(0,"CUST_NO");
	
