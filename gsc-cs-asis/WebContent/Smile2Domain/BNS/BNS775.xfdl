function Btn_srch_onclick(obj:Button,  e:ClickEventInfo)
{	
	//초기화
	ds_if_evcoup_env.clearData();
	ds_if_evcoup_data.clearData();
	ds_if_evcoup_header.clearData();	
	ds_if_evcoup_common.clearData();
	ds_if_evcoup_column.clearData();
	ds_if_evcoup_record.clearData();
	
	//승인내역 초기화
	ds_if_ev_appr_record.clearData();
	
	
	Stt_Cupon_Cnt.text = 0;
		
	ev_coupon_list_srch();
}

function ev_coupon_list_srch(){
	
	if(!cf_ValidDate(Cal_Dtm_From.value, Cal_Dtm_To.value, 'N')) {
		Cal_Dtm_From.setFocus();
		return false;
	}
	
	if(    getTrim(Cal_Dtm_From.value) 	== "" 
	    && getTrim(Cal_Dtm_To.value) 	== ""
	    && getTrim(Edt_CustNo.value)    == ""
	    ){
			cf_Alert("COME0007","고객번호 및 검색 날짜는 필수 입력 입니다.");			
		return false;
	}
	
	//IF 환경설정
	fun_ds_srch(ds_if_evcoup_env, "IF_KIND",           "GSCP-APP");
	fun_ds_srch(ds_if_evcoup_env, "REQ_SERVICE_METHOD","GSCEVCouponSearchList");
	fun_ds_srch(ds_if_evcoup_env, "REQ_SERVICE_ID",    "");	
	fun_ds_srch(ds_if_evcoup_env, "RES_HEADER_SECTION","return");
	fun_ds_srch(ds_if_evcoup_env, "RES_COLUMN_SECTION","");
	fun_ds_srch(ds_if_evcoup_env, "RES_RECORD_SECTION","couponList");
		
			
	//IF INPUT DATA
	fun_ds_srch(ds_if_evcoup_data, "Cust_No_List",           getTrim(Edt_CustNo.value)+"^"+getTrim(Edt_CustNo00.value));      	
	fun_ds_srch(ds_if_evcoup_data, "Event_Cd",           	"");           								//이벤트 코드	          
	fun_ds_srch(ds_if_evcoup_data, "Use_Div", 		    cmb_cupn_status.value);                       //사용구분
	fun_ds_srch(ds_if_evcoup_data, "Cupn_Div", 		cmb_cupn_kind.value);                       //쿠폰 종류
	fun_ds_srch(ds_if_evcoup_data, "Order_By", 		cmb_cupn_orderby.value);                       //정렬 순서
	fun_ds_srch(ds_if_evcoup_data, "Search_Start_Dy", 		Cal_Dtm_From.value);                       //기간(From)	
	fun_ds_srch(ds_if_evcoup_data, "Search_End_Dy", 		Cal_Dtm_To.value);	                       //기간(To)
	
	//trace(ds_if_data.saveXML());
		
	var	strSvcID = "IF_EVCouponList"; //IF_CouponList
	var	strURL = SVC_IF_URL;
	var	strInDatasets = "INTERFACE_ENV=ds_if_evcoup_env:a INTERFACE_DATA=ds_if_evcoup_data:a";	
	//var	strOutDatasets = "ds_if_common=DS_COMMON ds_if_header=DS_ROOT ds_if_column=DS_COLUMN ds_if_coupon_record=DS_RECORD";	
	var	strOutDatasets = "ds_if_evcoup_common=DS_COMMON ds_if_evcoup_header=DS_HEADER ds_if_evcoup_column=DS_COLUMN ds_if_evcoup_record=DS_RECORD";	
	var	strArgument = "";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;
	
	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);	
}
