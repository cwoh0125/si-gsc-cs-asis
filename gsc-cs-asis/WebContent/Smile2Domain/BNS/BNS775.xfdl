function fn_callBack(svcid,strErrCode,strErrMsg){

	switch(svcid){		
		case "IF_EVCouponList" :
			
			//2015.08.15 개인정보접촉이력
			trace("=============개인정보접촉이력  이벤트 EV모바일 쿠폰 조회Start===========");
			trace(">>>>>"+ds_if_evcoup_record.saveXML());

			var sAccNo = "BNS775"; //개인정보접촉이력 일련번호
			var sPerTCd = "PT1002";  // 수행업무 코드
			var sPerTDt = "ICCS_CSC 이벤트 EV모바일 쿠폰 조회";  // 수행업무 상세내용
			var sCalFun = "ev_coupon_list_srch()";  // 호출함수명 
			var sInqCon = "|" + "Cust_No_List="+getTrim(Edt_CustNo.value)
						+ "|" + "Search_Start_Dy="+getTrim(Cal_Dtm_From.value)
						+ "|" + "Search_End_Dy="+getTrim(Cal_Dtm_To.value)
						+ "|" ;  // 조회조건 
						
			var sInqCnt = ds_if_evcoup_record.rowcount;  // 조회건수
			var sInCustNo = getTrim(Edt_CustNo.value) ;//식별키	
          									
			trace("개인정보접촉이력--이벤트 EV모바일쿠폰 조회 "+"    "+sAccNo 
															  +"    "+sPerTCd
															  +"    "+sPerTDt
															  +"    "+sCalFun
															  +"    "+sInqCon
															  +"    "+sInqCnt); 
											  
			trace("=============개인정보접촉이력 이벤트 EV모바일쿠폰 조회END==========="); 
				

			//fn_Privcnthis(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt);
			fn_Privcnthis_info(sAccNo, sPerTCd, sPerTDt, sCalFun, sInqCon, sInqCnt,sInCustNo);
				
			if(ds_if_evcoup_common.getColumn(0,"HTTP_RET_CODE") == '-1'){				
				cf_Alert("COME0035", ds_if_evcoup_common.getColumn(0,"HTTP_RET_MSG"));									
			}else{
			    Stt_Cupon_Cnt.text = ds_if_evcoup_record.rowcount;
				if(ds_if_evcoup_header.getColumn(0,"RETURNCODE") == '00000'){	//00000: 정상, 10001 필수입력값 오류
					var inputData = "";
					if(ds_if_evcoup_record.rowcount < 1){
						alert("조회 내용이 없습니다.");					
					} else {
						for(var i = 0; i < ds_if_evcoup_record.rowcount; i++) {
							inputData = ds_if_evcoup_record.getColumn(i, "MCHT_NM") + "[" + ds_if_evcoup_record.getColumn(i, "MCHT_CD") + "]";
							ds_if_evcoup_record.setColumn(i, "MCHT_NM", inputData);
						}
					}
				}else{
					cf_Alert("COME0035", ds_if_evcoup_header.getColumn(0,"RETURNMSG"));	
				}
			}		
	
			break;	       
			
		case "IF_CouponApprList" :
			trace(ds_if_ev_appr_header.getColumn(0,"RETURNCODE"));
			trace("ds_if_ev_appr_record ====> " + ds_if_ev_appr_record.saveXML());
			
			Stt_App_Cupon_Cnt.text = ds_if_ev_appr_record.rowcount;
			if(ds_if_ev_appr_header.getColumn(0,"RETURNCODE") == '00000')//00000: 정상, 10001 필수입력값 오류
			{	
				if(ds_if_ev_appr_record.rowcount < 1)
				{
					Grid01.nodatatext = "조회된 결과가 없습니다.";
				}	
			}
			else
			{
				cf_Alert("COME0035", ds_if_ev_appr_header.getColumn(0,"RETURNMSG"));	
			}
		     
		     
		default : break;				
	}
}

