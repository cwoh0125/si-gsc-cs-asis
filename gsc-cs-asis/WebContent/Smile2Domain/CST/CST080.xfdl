function Rdo_Key_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	if(Rdo_Key.value == "1")
	{
	  // Edt_Ju.enable = false; 
	  // Edt_Jumin.enable = false;
	   Msk_Sp.enable = false;
	   Msk_Ep.enable = false;
	   Cmb_Adv.enable = true;
	   Cmb_Grp.enable = true;
	   Cmb_Part.enable = true;
	   Edt_custId.enable = false;
	 //  Edt_Ju.value = "";
	 //  Edt_Jumin.value = "";
	   Msk_Sp.value = "";
	   Msk_Ep.value = "";
	   Edt_custId.value = "";	   
	}
	else if (Rdo_Key.value == "2")
	{
	//  Edt_Ju.enable = false; 
	//  Edt_Jumin.enable = false;
	  Msk_Sp.enable = true;
	  Msk_Ep.enable = true;
	  Cmb_Adv.enable = true;
	  Cmb_Grp.enable = true;
	  Cmb_Part.enable = true;
	  Edt_custId.enable = false;
	//  Edt_Ju.value = "";
	//  Edt_Jumin.value = "";
	  Cmb_Adv.value = "";
	  Edt_custId.value = "";
	}
	else if (Rdo_Key.value == "3")
	{
	  Msk_Sp.enable = false;
	  Msk_Ep.enable = false;
	  Cmb_Adv.enable = false;	
	  Cmb_Grp.enable = false;
	  Cmb_Part.enable = false;
	  Cmb_Adv.value = "";
	  Cmb_Grp.value = "";
	  Cmb_Part.value = "";
	  Edt_custId.enable = true;
	}
}




