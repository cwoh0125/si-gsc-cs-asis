function fn_callBack(svcid,strErrCode,strErrMsg){

	if(svcid=="S1")
	{	
		ds_code_nm1.copyData(ds_code_nm);
		ds_code_nm2.copyData(ds_code_nm);
		ds_code_nm3.copyData(ds_code_nm);
		ds_code_nm4.copyData(ds_code_nm);
		ds_code_nm5.copyData(ds_code_nm);
		ds_code_nm6.copyData(ds_code_nm);
		ds_code_nm7.copyData(ds_code_nm);
		ds_code_nm8.copyData(ds_code_nm);
		ds_code_nm9.copyData(ds_code_nm);
	}
	if(svcid=="C1")
	{
		alert("저장되었습니다.");
	    Clear();
	}
	if(svcid=="S2")
	{
		if(ds_out_qa.rowcount=="0")
		{
			alert("조회된 건이 없습니다.");
			return;
		}		
		
	}
	if(svcid=="S3")
	{
			
		if(ds_out_sta.rowcount=="0")
		{
			alert("조회된 건이 없습니다.");
			return;
		}		
		
	}
	if(svcid=="S4")
	{
			
		if(ds_out_fax.rowcount=="0")
		{
			alert("조회된 건이 없습니다.");
			return;
		}		
		
	}
	if(svcid=="S5")
	{
			
		if(ds_out_ord.rowcount=="0")
		{
			alert("조회된 건이 없습니다.");
			return;
		}
		if(svcid=="S6")
		{
			
			if(ds_out_error.rowcount=="0")
			{
				alert("조회된 건이 없습니다.");
				return;
			}		
		}
	}
	
	if(svcid == 'S7')
	{
		var qa = ds_out_qa.rowcount;
		var sta = ds_out_sta.rowcount;
		var fax = ds_out_fax.rowcount;
		var ord = ds_out_ord.rowcount;
		var error = ds_out_error.rowcount ;
		
		if( qa == '0' && sta == '0' && fax == '0' && ord == '0' && error == '0' )
		{
			alert("조회된 건이 없습니다.");
			return;
		}	
	}
	
	
	if(svcid=="D1"){
	alert("삭제되었습니다");
	Clear();
	Btn_sel_onclick();
	}if(svcid=="D2"){
	alert("삭제되었습니다");
	Clear();
	Btn_sel_2_onclick();
	}if(svcid=="D3"){
	alert("삭제되었습니다");
	Clear();
	Btn_sel_3_onclick();
	}if(svcid=="D4"){
	alert("삭제되었습니다");
	Clear();
	Btn_sel_4_onclick();
	}if(svcid=="D5"){
	alert("삭제되었습니다");
	Clear();
	Btn_sel_5_onclick();
	}
	if(svcid=="U1"){
	alert("수정되었습니다");
	Clear();
	Btn_sel_onclick();
	}if(svcid=="U2"){
	alert("수정되었습니다");
