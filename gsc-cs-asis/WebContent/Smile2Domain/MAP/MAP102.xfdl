function Btn_save_onclick(obj:Button,  e:ClickEventInfo)
{
	var chkCnt = 0;
	
	if(edt_seq_qa.value=="" || edt_seq_qa.value==null){
	
	if (!cf_Confirm("COMI0014"))	return;  //저장하시겠습니까?
	//길이 체크
	
	ds_qa_insert.clearData();
	
	var in_qa_data1 = Cal_start1.text;	
			
	//fun_ds_srch(ds_qa_insert, "TIME_KEY", in_qa_data1.replace("-","").substr(0,6));		
	//fun_ds_srch(ds_qa_insert, "USR_ID",Cmb_USR1.value);
	//fun_ds_srch(ds_qa_insert, "QA_POINT", edt_jumsu1.value);
	
	
	for(var i = 0; i < ds_code_nm1.rowcount; i++)
	{
		nRow = ds_qa_insert.addRow();
	
		if(ds_code_nm1.getColumn(i,"CHK") == "1"){
			ds_qa_insert.setColumn(nRow,"TIME_KEY",in_qa_data1.replace("-","").substr(0,6));
			ds_qa_insert.setColumn(nRow,"USR_ID",ds_code_nm1.getColumn(i,"CODE"));
			ds_qa_insert.setColumn(nRow,"QA_POINT",edt_jumsu1.value);

		    chkCnt++;
		}
	}	
	
	trace(ds_qa_insert.saveXML());
						    
	//return;
	
	if(chkCnt < 1){
		cf_Alert("BNSE0032");	//저장 할 건이 없습니다.
		return;
	}
    						    
	var	strSvcID = "C1";
	var	strURL = SVC_USR_URL;
	var	strInDatasets = "INSERT_0_IDATA=ds_qa_insert:a "; 
	var	strOutDatasets = "";
	var	strArgument = "INSERT_0_ID=MAP100.Set_MAP102_QA_insert";
	var strCallbackFunc = "fn_callBack";
	var strProcessBar = true;

	cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
	}else{
	
		if (!cf_Confirm("COMI0028"))	return;  //변경하시겠습니까?
	
		ds_update.clearData();
		
		var in_qa_data = Cal_start1.text;
		
       	/*fun_ds_srch(ds_update, "TIME_KEY", in_qa_data.replace("-","").substr(0,6));		
		fun_ds_srch(ds_update, "USR_ID", Cmb_USR1.value);
		fun_ds_srch(ds_update, "QA_POINT", edt_jumsu1.value);
		fun_ds_srch(ds_update, "QA_SEQ_NO", edt_seq_qa.value);*/
		
		for( var i = 0; i < ds_out_qa.rowcount; i++)
		{
		   nRow = ds_update.addRow();
		   if(ds_out_qa.getColumn(i,"CHK") == "1"){
			   ds_update.setColumn(nRow, "TIME_KEY", ds_out_qa.getColumn(i,"TIME_KEY"));
		       ds_update.setColumn(nRow, "USR_ID", ds_out_qa.getColumn(i,"USR_ID"));
		       ds_update.setColumn(nRow, "QA_POINT", ds_out_qa.getColumn(i,"QA_POINT"));
		       ds_update.setColumn(nRow, "QA_SEQ_NO", ds_out_qa.getColumn(i,"QA_SEQ_NO"));
		       
		       chkCnt++;
		   }
		}
		
		if(chkCnt < 1)
		{
			alert("변경 할 건이 없습니다.\n 변경 할 건을 선택하시기 바랍니다.")
			return;
	    }
									
		trace(ds_update.saveXML());
												
		var	strSvcID = "U1";
		var	strURL = SVC_USR_URL;
		var	strInDatasets = "UPDATE_0_IDATA=ds_update:a "; 
		var	strOutDatasets = "";
		var	strArgument = "UPDATE_0_ID=MAP100.Set_MAP102_UPDATE_QA";
		var strCallbackFunc = "fn_callBack";
		var strProcessBar = true;

		cf_SVC(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc,strProcessBar);
	
	}
}

