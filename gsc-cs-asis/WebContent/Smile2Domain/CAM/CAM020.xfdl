	  GridExportExcel(Grd_Exel1, CmpTgtExp, "캠페인대상자");
	}
	else if (Rdo_Inkey.value == "2")
	{
	  CmpTgtExp.clear();
	  GridExportExcel(Grd_Exel2, CmpTgtExp, "캠페인대상자");
	}	
	else
	{
	  return;
	}
}


